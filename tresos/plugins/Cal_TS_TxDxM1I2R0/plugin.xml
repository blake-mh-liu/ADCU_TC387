  <extension point="dreisoft.tresos.launcher2.plugin.module"
             id="Cal_TS_TxDxM1I2R0_ModuleId"
             name="Cal_TS_TxDxM1I2R0 Module">

    <module id="Cal_TS_TxDxM1I2R0"
            label="Cal"
            mandatory="false"
            allowMultiple="false"
            description="ACG-CAL: Crypto Abstraction Library"
            copyright="(c) 2005-2013 Elektrobit Automotive GmbH"
            swVersionMajor="1"
            swVersionMinor="2"
            swVersionPatch="2"
            swVersionSuffix="B117106"
            specVersionMajor="1"
