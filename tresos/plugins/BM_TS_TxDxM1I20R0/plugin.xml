  <extension point="dreisoft.tresos.launcher2.plugin.module"
             id="BM_TS_TxDxM1I20R0_ModuleId"
             name="BM_TS_TxDxM1I20R0 Module">
    <module id="BM_TS_TxDxM1I20R0"
            label="BM"
            mandatory="false"
            allowMultiple="false"
            description="BL-Core: Bootloader Boot Manager module"
            copyright="(c) 2005-2024 Elektrobit Automotive GmbH"
            swVersionMajor="1"
            swVersionMinor="20"
            swVersionPatch="0"
            swVersionSuffix="BL3"
            specVersionMajor="4"
            specVersionMinor="0"
            specVersionPatch="3"
            specVersionSuffix="0000"
            relVersionMajor="4"
            relVersionMinor="0"
            relVersionPatch="3"
            categoryType="BM"
            categoryLayer="System Services"
            categoryCategory="Diagnostic"
            categoryComponent="ECUC">
      <cluster name="Diagnostic"/>
    </module>
  </extension>
  <extension point="dreisoft.tresos.launcher2.plugin.configuration"
             id="BM_TS_TxDxM1I20R0_ConfigId"
             name="BM_TS_TxDxM1I20R0 Configuration">
    <configuration moduleId="BM_TS_TxDxM1I20R0">
      <schema>
        <manager class="dreisoft.tresos.autosar2.resourcehandling.AutosarSchemaManager"/>
        <resource value="config/BM.xdm" type="xdm" id="res_default"/>
      </schema>

      <data>
        <manager class="dreisoft.tresos.autosar2.resourcehandling.AutosarConfigManager"/>
        <schemaNode path="ASPath:/TS_TxDxM1I20R0/BM"/>
      </data>

      <editor id="BM_TS_TxDxM1I20R0_EditorId"
              label="Default"
              tooltip="BL-Core: Bootloader Boot Manager module">
        <class class="dreisoft.tresos.launcher2.editor.GenericConfigEditor">
          <parameter name="schema" value="ASPath:/TS_TxDxM1I20R0/BM"/>
          <parameter name="title" value="BM"/>
          <parameter name="noTabs" value="false"/>
          <parameter name="noLinks" value="true"/>
          <parameter name="groupLinks" value="false"/>
          <parameter name="groupContainers" value="false"/>
          <parameter name="groupTables" value="true"/>
          <parameter name="optionalGeneralTab" value="true"/>
        </class>
      </editor>
    </configuration>
  </extension>

  <extension point="dreisoft.tresos.launcher2.plugin.generator"
             id="BM_TS_TxDxM1I20R0_GeneratorId"
             name="BM_TS_TxDxM1I20R0 Generator">
    <generator class="dreisoft.tresos.launcher2.generator.TemplateBasedCodeGenerator"
               id="BM_TS_TxDxM1I20R0_TemplateBasedCodeGeneratorId"
               moduleId="BM_TS_TxDxM1I20R0">
      <!-- common template path parameters -->
      <parameter name="templates"
                 mode="generate,verify" value="generate"/>
    </generator>
  </extension>
