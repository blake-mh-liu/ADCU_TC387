<plugin>
  <extension point="dreisoft.tresos.launcher2.plugin.module"
    id="EthSwt_Aurix2GAS440_ModuleExt"
    name="EthSwt Module">
    <module id="EthSwt_Aurix2GAS440"
      label="%ModuleLabel"
      mandatory="false"
      allowMultiple="true"
      description="%ModuleDesc"
      copyright="(c) 2020 Infineon Technologies"
      swVersionMajor="20"
      swVersionMinor="0"
      swVersionPatch="0"
      swVersionSuffix=""
      specVersionMajor="4"
      specVersionMinor="4"
      specVersionPatch="0"
      relVersionMajor="4"
      relVersionMinor="4"
      relVersionPatch="0"
      categoryType="%ModuleName"
      categoryLayer="MCAL"
      categoryCategory="ECU Firmware"
      categoryComponent="ECUC">
      <cluster name="Ethernet Stack"/>
      <ecuType target="AURIX2G" derivate="TC3E7"/>
      <ecuType target="AURIX2G" derivate="TC399"/>
      <ecuType target="AURIX2G" derivate="TC397"/>
      <ecuType target="AURIX2G" derivate="TC397_ADAS"/>
      <ecuType target="AURIX2G" derivate="TC389"/>
      <ecuType target="AURIX2G" derivate="TC387"/>
      <ecuType target="AURIX2G" derivate="TC375"/>
      <ecuType target="AURIX2G" derivate="TC377"/>
      <ecuType target="AURIX2G" derivate="TC374_ED"/>
      <ecuType target="AURIX2G" derivate="TC375_ED"/>
      <ecuType target="AURIX2G" derivate="TC377_ED"/>
      <ecuType target="AURIX2G" derivate="TC377_ED_EX"/>
      <ecuType target="AURIX2G" derivate="TC364_LQFP"/>
      <ecuType target="AURIX2G" derivate="TC364_TQFP"/>
      <ecuType target="AURIX2G" derivate="TC365"/>
      <ecuType target="AURIX2G" derivate="TC366"/>
      <ecuType target="AURIX2G" derivate="TC367"/>
      <ecuType target="AURIX2G" derivate="TC356_ADAS"/>
      <ecuType target="AURIX2G" derivate="TC357_ADAS"/>
      <ecuType target="AURIX2G" derivate="TC336_ED_ADAS"/>
      <ecuType target="AURIX2G" derivate="TC337_ED_ADAS"/>
    </module>
  </extension>
  <extension point="dreisoft.tresos.launcher2.plugin.configuration"
    id="EthSwt_Aurix2GAS440_ConfigExt"
    name="EthSwt_Aurix2G Configuration">
    <configuration moduleId="EthSwt_Aurix2GAS440">
      <schema>
        <manager class="dreisoft.tresos.autosar2.resourcehandling.AutosarSchemaManager"/>
        <resource value="config/AS440/EthSwt.xdm" type="xdm"/>
      </schema>
      <data>
        <manager class="dreisoft.tresos.autosar2.resourcehandling.AutosarConfigManager"/>
        <schemaNode path="ASPath:/AUTOSAR/EcucDefs/EthSwt"/>
      </data>
      <editor id="EthSwt_Aurix2GAS440_Edit"
        label="%ModuleName"
        tooltip="%ModuleDesc">
        <class class="dreisoft.tresos.launcher2.editor.GenericConfigEditor">
          <parameter name="schema" value="ASPath:/AUTOSAR/EcucDefs/EthSwt"/>
          <parameter name="title" value="EthSwt"/>
          <parameter name="noTabs" value="false"/>
          <parameter name="noLinks" value="true"/>
          <parameter name="groupLinks" value="false"/>
          <parameter name="groupContainers" value="false"/>
          <parameter name="groupTables" value="true"/>
          <parameter name="optionalGeneralTab" value="true"/>
        </class>
      </editor>
    </configuration>
  </extension>
  <!--
  This extension draws the connection between the module, and the com importer.
  -->
  <extension point="dreisoft.tresos.comimporter.api.plugin.comtransformer">
    <comtransformer
      id="ComTransformer_EthSwt_Aurix2GAS440"
      moduleId="EthSwt_Aurix2GAS440">
      <transformer
        class="dreisoft.tresos.comimporter.api.transformer.asr40.FrIfTransformer">
      </transformer>
    </comtransformer>
  </extension>
  <extension point="dreisoft.tresos.launcher2.plugin.generator"
    id="EthSwt_Aurix2GAS440_GenExt"
    name="EthSwt_Aurix2G Generator">
    <generator moduleId="EthSwt_Aurix2GAS440"
      class="dreisoft.tresos.launcher2.generator.TemplateBasedCodeGenerator">
      <!-- common template path parameters -->
      <parameter name="templates"
        mode="generate,verify" value="generate"/>
      <!-- swcd modes and template path parameters -->
      <parameter name="mode_type"
        mode="generate_swcd" value="generate"/>
      <parameter name="mode_type"
        mode="verify_swcd" value="verify"/>
      <parameter name="templates"
        mode="generate_swcd,verify_swcd" value="generate_swcd"/>
    </generator>
  </extension>
</plugin>
