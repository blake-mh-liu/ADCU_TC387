  <extension point="dreisoft.tresos.launcher2.plugin.module"
             id="Dccm_TS_TxDxM2I2R0_ModuleId"
             name="Dccm_TS_TxDxM2I2R0 Module">
    <module id="Dccm_TS_TxDxM2I2R0"
            label="Dccm"
            mandatory="false"
            allowMultiple="false"
            description="ACG-DCCM: Dccm Module"
            copyright="(c) 2005-2024 Elektrobit Automotive GmbH"
            swVersionMajor="2"
            swVersionMinor="2"
            swVersionPatch="3"
            swVersionSuffix="B788439"
            specVersionMajor="0"
            specVersionMinor="0"
            specVersionPatch="0"
            specVersionSuffix="0"
            relVersionMajor="4"
            relVersionMinor="0"
            relVersionPatch="3"
            categoryType="Dccm"
            categoryLayer="Application"
            categoryCategory="System Services"
            categoryComponent="ECUC">
      <cluster name="Testing"/>
    </module>
  </extension>
  <extension point="dreisoft.tresos.launcher2.plugin.configuration"
             id="Dccm_TS_TxDxM2I2R0_ConfigId"
             name="Dccm_TS_TxDxM2I2R0 Configuration">
    <configuration moduleId="Dccm_TS_TxDxM2I2R0">
      <schema>
        <manager class="dreisoft.tresos.autosar2.resourcehandling.AutosarSchemaManager"/>
        <resource value="config/Dccm.xdm" type="xdm" id="res_default"/>
      </schema>

      <data>
        <manager class="dreisoft.tresos.autosar2.resourcehandling.AutosarConfigManager"/>
        <schemaNode path="ASPath:/TS_TxDxM2I2R0/Dccm"/>
      </data>

      <editor id="Dccm_TS_TxDxM2I2R0_EditorId"
              label="Default"
              tooltip="ACG-DCCM: Dccm Module">
        <class class="dreisoft.tresos.launcher2.editor.GenericConfigEditor">
          <parameter name="schema" value="ASPath:/TS_TxDxM2I2R0/Dccm"/>
          <parameter name="title" value="Dccm"/>
          <parameter name="noTabs" value="false"/>
          <parameter name="noLinks" value="true"/>
          <parameter name="groupLinks" value="false"/>
          <parameter name="groupContainers" value="false"/>
          <parameter name="groupTables" value="true"/>
          <parameter name="optionalGeneralTab" value="true"/>
        </class>
      </editor>
    </configuration>
  </extension>

  <extension point="dreisoft.tresos.launcher2.plugin.generator"
             id="Dccm_TS_TxDxM2I2R0_GeneratorId"
             name="Dccm_TS_TxDxM2I2R0 Generator">
    <generator class="dreisoft.tresos.launcher2.generator.TemplateBasedCodeGenerator"
               id="Dccm_TS_TxDxM2I2R0_TemplateBasedCodeGeneratorId"
               moduleId="Dccm_TS_TxDxM2I2R0">
      <!-- common template path parameters -->
      <parameter name="templates"
                 mode="generate,verify" value="generate"/>

      <!-- swcd modes and template path parameters -->
      <parameter name="mode_type"
                 mode="generate_swcd" value="generate"/>
      <parameter name="mode_type"
                 mode="verify_swcd" value="verify"/>
      <parameter name="templates"
                 mode="generate_swcd,verify_swcd" value="generate_swcd"/>
    </generator>
  </extension>
  <extension point="dreisoft.tresos.autosar2.api.plugin.ecuresource">
    <ecuresource
      target="*"
      derivate="*"
      moduleId="Dccm_TS_TxDxM2I2R0"
      file="resources/AdjacentLayer.properties">
    </ecuresource>
  </extension>

  <extension point="org.eclipse.help.toc">
    <toc file="anchors.xml" primary="true" />
    <toc file="$nl$/toc.xml" primary="false"/>
  </extension>

  
  <!-- handle id calculator for DccmPhysFuncTxPduId -->
  <extension
    point="dreisoft.tresos.guidedconfig.api.plugin.pushservice">
    <pushoperation id="DccmPhysFuncTxPduId_Dccm_TS_TxDxM2I2R0"
    name="Dccm/DccmPhysFuncTxPduId"
    desc="">
        <operationclass class="dreisoft.tresos.guidedconfig.api.pushservice.operations.handleid.SimpleHandleIdPushOperation">
            <parameter name="moduleId" value="Dccm_TS_TxDxM2I2R0"/>
            <parameter name="listPath" value="/AUTOSAR/TOP-LEVEL-PACKAGES/TS_TxDxM2I2R0/ELEMENTS/Dccm/DccmPhysicalPduIds"/>
            <parameter name="idPath" value="DccmPhysicalTxPduId"/>
            <parameter name="listPath_2" value="/AUTOSAR/TOP-LEVEL-PACKAGES/TS_TxDxM2I2R0/ELEMENTS/Dccm/DccmFunctionalPduIds"/>
            <parameter name="idPath_2" value="DccmFunctionalTxPduId"/>
            <parameter name="zbcGroups" value="false"/>
            <parameter name="zbcGroups_2" value="false"/>
        </operationclass>
        <event>
            <and>
                <with variable="class">
                    <equals value="dreisoft.tresos.guidedconfig.api.pushservice.operations.handleid.HandleIdEvent"/>
                </with>
                <with variable="moduleId.Dccm_TS_TxDxM2I2R0">
                   <equals value="true"/>
                </with>
                <with variable="relVersion">
                    <equals value="4.0"/>
                </with>
            </and>
        </event>
    </pushoperation>
  </extension>

  <!-- handle id calculator for DccmPhysFuncRxPduId -->
  <extension
    point="dreisoft.tresos.guidedconfig.api.plugin.pushservice">
    <pushoperation id="DccmPhysFuncRxPduId_Dccm_TS_TxDxM2I2R0"
    name="Dccm/DccmPhysFuncRxPduId"
    desc="">
        <operationclass class="dreisoft.tresos.guidedconfig.api.pushservice.operations.handleid.SimpleHandleIdPushOperation">
            <parameter name="moduleId" value="Dccm_TS_TxDxM2I2R0"/>
            <parameter name="listPath" value="/AUTOSAR/TOP-LEVEL-PACKAGES/TS_TxDxM2I2R0/ELEMENTS/Dccm/DccmPhysicalPduIds"/>
            <parameter name="idPath" value="DccmPhysicalRxPduId"/>
            <parameter name="listPath_2" value="/AUTOSAR/TOP-LEVEL-PACKAGES/TS_TxDxM2I2R0/ELEMENTS/Dccm/DccmFunctionalPduIds"/>
            <parameter name="idPath_2" value="DccmFunctionalRxPduId"/>
            <parameter name="zbcGroups" value="false"/>
            <parameter name="zbcGroups_2" value="false"/>
        </operationclass>
        <event>
            <and>
                <with variable="class">
                    <equals value="dreisoft.tresos.guidedconfig.api.pushservice.operations.handleid.HandleIdEvent"/>
                </with>
                <with variable="moduleId.Dccm_TS_TxDxM2I2R0">
                   <equals value="true"/>
                </with>
                <with variable="relVersion">
                    <equals value="4.0"/>
                </with>
            </and>
        </event>
    </pushoperation>
  </extension>

</plugin>
