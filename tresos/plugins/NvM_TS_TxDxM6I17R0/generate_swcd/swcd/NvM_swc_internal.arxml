        [!ENDIF!][!//  
      [!ENDIF!][!//
    [!ELSE!][!//
      [!VAR "Partition" = "''"!][!//
      [!VAR "PartitionName" = "''"!][!//
    [!ENDIF!][!//
    [!IF "($GenerateArtefacts = 'TRUE')"!][!//
      [!IF "node:existsAndTrue(NvMProvideRteMirrorPort)"!][!//
                          <SYNCHRONOUS-SERVER-CALL-POINT>
                            <SHORT-NAME>sscp_[!"$InterfaceName"!]ReadRamBlockFromNvm_[!"name(.)"!]</SHORT-NAME>
                            <OPERATION-IREF>
                              <CONTEXT-R-PORT-REF DEST="R-PORT-PROTOTYPE">/AUTOSAR_NvM/[!"$swComponentTypePackageName"!]/NvM[!"$PartitionName"!]/PMirror_[!"$InterfaceName"!][!"name(.)"!]</CONTEXT-R-PORT-REF>
                              <TARGET-REQUIRED-OPERATION-REF DEST="CLIENT-SERVER-OPERATION">[!"$TopLevelPackage"!]/PortInterfaces/[!"$Mirror"!]/ReadRamBlockFromNvm</TARGET-REQUIRED-OPERATION-REF>
                            </OPERATION-IREF>
                            <TIMEOUT>0.0</TIMEOUT>
