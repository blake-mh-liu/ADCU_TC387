                              <TARGET-REQUIRED-OPERATION-REF DEST="CLIENT-SERVER-OPERATION">[!"$TopLevelPackage"!]/PortInterfaces/[!"$Mirror"!]/WriteRamBlockToNvm</TARGET-REQUIRED-OPERATION-REF>
