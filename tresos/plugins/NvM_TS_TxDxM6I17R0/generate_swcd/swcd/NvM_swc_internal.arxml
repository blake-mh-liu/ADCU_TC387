                            <OPERATION-IREF>
                              <CONTEXT-R-PORT-REF DEST="R-PORT-PROTOTYPE">/AUTOSAR_NvM/[!"$swComponentTypePackageName"!]/NvM[!"$PartitionName"!]/PNInitBlock_[!"$InterfaceName"!][!"name(.)"!]</CONTEXT-R-PORT-REF>
                              <TARGET-REQUIRED-OPERATION-REF DEST="CLIENT-SERVER-OPERATION">[!"$TopLevelPackage"!]/PortInterfaces/[!"$NotifyInitBlock"!]/InitBlock</TARGET-REQUIRED-OPERATION-REF>
