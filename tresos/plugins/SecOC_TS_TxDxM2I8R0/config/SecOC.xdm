<?xml version='1.0'?>
<datamodel version="8.0" 
           xmlns="http://www.tresos.de/_projects/DataModel2/18/root.xsd" 
           xmlns:a="http://www.tresos.de/_projects/DataModel2/18/attribute.xsd" 
           xmlns:v="http://www.tresos.de/_projects/DataModel2/06/schema.xsd" 
           xmlns:d="http://www.tresos.de/_projects/DataModel2/06/data.xsd">

  <d:ctr type="AUTOSAR" factory="autosar" 
         xmlns:ad="http://www.tresos.de/_projects/DataModel2/08/admindata.xsd" 
         xmlns:ce="http://www.tresos.de/_projects/DataModel2/18/childenable.xsd" 
         xmlns:cd="http://www.tresos.de/_projects/DataModel2/08/customdata.xsd" 
         xmlns:f="http://www.tresos.de/_projects/DataModel2/14/formulaexpr.xsd" 
         xmlns:icc="http://www.tresos.de/_projects/DataModel2/08/implconfigclass.xsd" 
         xmlns:mt="http://www.tresos.de/_projects/DataModel2/11/multitest.xsd"  
         xmlns:variant="http://www.tresos.de/_projects/DataModel2/11/variant.xsd">
    <d:lst type="TOP-LEVEL-PACKAGES">
      <d:ctr name="TS_TxDxM2I8R0" type="AR-PACKAGE">
        <d:lst type="ELEMENTS">
          <d:chc name="SecOC" type="AR-ELEMENT" value="MODULE-DEF">
            <v:ctr type="MODULE-DEF">
              <a:a name="ADMIN-DATA" type="ADMIN-DATA">
                <ad:ADMIN-DATA>
                  <ad:LANGUAGE>EN</ad:LANGUAGE>
                </ad:ADMIN-DATA>
              </a:a>
              <a:a name="DESC" 
                   value="EN: Configuration of the SecOC (Secure Onboard Communication) module."/>
              <a:a name="LOWER-MULTIPLICITY" value="1"/>
              <a:a name="POSTBUILDVARIANTSUPPORT" value="true"/>
              <a:a name="RELEASE" value="asc:4.3"/>
              <a:a name="UPPER-MULTIPLICITY" value="1"/>
              <a:a name="UUID" value="505e4f69-8af3-4e02-8b8b-4f78165ec8dd"/>
              <a:da name="INVALID" type="XPath" 
                    expr="((count(SecOCRxPduProcessing/*) + count(SecOCTxPduProcessing/*)) &gt; 0)" 
                    false="At least one Rx or Tx Processing PDU must be configured."/>
              <v:ctr name="PublishedInformation" type="IDENTIFIABLE">
                <a:a name="DESC">
                  <a:v>EN:
                    &lt;html&gt;
                      Additional published parameters not covered by CommonPublishedInformation container.
                  &lt;/html&gt;</a:v>
                </a:a>
                <a:a name="LABEL" value="EB Published Information"/>
                <a:a name="TAB" value="EB Published Information"/>
                <a:a name="UUID" 
                     value="ECUC:cb4128ed-26c1-4ddd-906e-854bed917e07"/>
                <v:var name="PbcfgMSupport" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>EN:
                      &lt;html&gt;
                        Specifies whether or not the SecOC can use the PbcfgM module for post-build support.
                    &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       value="PublishedInformation"/>
                  <a:a name="LABEL" value="PbcfgM support"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" 
                       value="ECUC:eb61002e-69c0-4f16-a910-c27adb75f6ee"/>
                  <a:da name="DEFAULT" value="true"/>
                </v:var>
              </v:ctr>
              <v:ctr name="CommonPublishedInformation" type="IDENTIFIABLE">
                <a:a name="DESC">
                  <a:v>EN:
                    &lt;html&gt;
                      Common container, aggregated by all modules. It contains published information about vendor and versions.
                  &lt;/html&gt;</a:v>
                </a:a>
                <a:a name="LABEL" value="Common Published Information"/>
                <a:a name="UUID" 
                     value="ECUC:a00be3e0-8783-9123-2d52-1eb616737ca6"/>
                <v:var name="ArMajorVersion" type="INTEGER_LABEL">
                  <a:a name="DESC">
                    <a:v>EN:
                      &lt;html&gt;
                        Major version number of AUTOSAR specification on which the appropriate implementation is based on.
                    &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       value="PublishedInformation"/>
                  <a:a name="LABEL" value="AUTOSAR Major Version"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" 
                       value="ECUC:948f3f2e-f129-4bc5-b4e1-7a7bdb8599e1"/>
                  <a:da name="DEFAULT" value="4"/>
                </v:var>
                <v:var name="ArMinorVersion" type="INTEGER_LABEL">
                  <a:a name="DESC">
                    <a:v>EN:
                      &lt;html&gt;
                        Minor version number of AUTOSAR specification on which the appropriate implementation is based on.
                    &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       value="PublishedInformation"/>
                  <a:a name="LABEL" value="AUTOSAR Minor Version"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" 
                       value="ECUC:2893b920-59d5-4ac2-b2c1-e23742e66d70"/>
                  <a:da name="DEFAULT" value="3"/>
                </v:var>
                <v:var name="ArPatchVersion" type="INTEGER_LABEL">
                  <a:a name="DESC">
                    <a:v>EN:
                      &lt;html&gt;
                        Patch level version number of AUTOSAR specification on which the appropriate implementation is based on.
                    &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       value="PublishedInformation"/>
                  <a:a name="LABEL" value="AUTOSAR Patch Version"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" 
                       value="ECUC:6428eb9b-8790-488a-b9a3-0fba52d0f59e"/>
                  <a:da name="DEFAULT" value="0"/>
                </v:var>
                <v:var name="SwMajorVersion" type="INTEGER_LABEL">
                  <a:a name="DESC">
                    <a:v>EN:
                      &lt;html&gt;
                        Major version number of the vendor specific implementation of the module.
                    &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       value="PublishedInformation"/>
                  <a:a name="LABEL" value="Software Major Version"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" 
                       value="ECUC:605b18ae-3f9a-41d4-9225-67c9c5f6fc34"/>
                  <a:da name="DEFAULT" value="2"/>
                </v:var>
                <v:var name="SwMinorVersion" type="INTEGER_LABEL">
                  <a:a name="DESC">
                    <a:v>EN:
                      &lt;html&gt;
                        Minor version number of the vendor specific implementation of the module. The numbering is vendor specific.
                    &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       value="PublishedInformation"/>
                  <a:a name="LABEL" value="Software Minor Version"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="a7fe44dc-ea25-4b8d-8fad-9fbcae86d56f"/>
                  <a:da name="DEFAULT" value="8"/>
                </v:var>
                <v:var name="SwPatchVersion" type="INTEGER_LABEL">
                  <a:a name="DESC">
                    <a:v>EN:
                      &lt;html&gt;
                        Patch level version number of the vendor specific implementation of the module. The numbering is vendor specific.
                    &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       value="PublishedInformation"/>
                  <a:a name="LABEL" value="Software Patch Version"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="a318d2d9-0e75-49da-ac43-e7e4e682e2f9"/>
                  <a:da name="DEFAULT" value="14"/>
                </v:var>
                <v:var name="ModuleId" type="INTEGER_LABEL">
                  <a:a name="DESC">
                    <a:v>EN:
                      &lt;html&gt;
                        Module ID of this module from Module List
                    &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       value="PublishedInformation"/>
                  <a:a name="LABEL" value="Numeric Module ID"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" 
                       value="ECUC:78bc8362-080f-4253-b3da-804ab69a7154"/>
                  <a:da name="DEFAULT" value="607"/>
                </v:var>
                <v:var name="VendorId" type="INTEGER_LABEL">
                  <a:a name="DESC">
                    <a:v>EN:
                      &lt;html&gt;
                        Vendor ID of the dedicated implementation of this module according to the AUTOSAR vendor list
                    &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       value="PublishedInformation"/>
                  <a:a name="LABEL" value="Vendor ID"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" 
                       value="ECUC:01b0f467-6943-4558-b4f2-3fa1fad28449"/>
                  <a:da name="DEFAULT" value="1"/>
                </v:var>
                <v:var name="Release" type="STRING_LABEL">
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       value="PublishedInformation"/>
                  <a:a name="LABEL" value="Release Information"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" 
                       value="ECUC:1c68a547-f24e-4a4e-9540-69fbd533ec89"/>
                  <a:da name="DEFAULT" value=""/>
                </v:var>
              </v:ctr>
              <v:var name="IMPLEMENTATION_CONFIG_VARIANT" type="ENUMERATION">
                <a:a name="DESC">
                  <a:v>&lt;html&gt;
                        &lt;p&gt;
                          Select the configuration variant.
                        &lt;/p&gt;
                      &lt;/html&gt;</a:v>
                </a:a>
                <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                     type="IMPLEMENTATIONCONFIGCLASS">
                  <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                </a:a>
                <a:a name="LABEL" value="Config Variant"/>
                <a:a name="UUID" value="a11623fa-c3fe-4548-809f-007dd3b09dc5"/>
                <a:da name="DEFAULT" value="VariantPostBuild"/>
                <a:da name="EDITABLE" value="true"/>
                <a:da name="ENABLE" value="true"/>
                <a:da name="RANGE" value="VariantPostBuild"/>
              </v:var>
              <v:ctr name="SecOCGeneral" type="IDENTIFIABLE">
                <v:var name="SecOCMainFunctionPeriodRx" type="FLOAT">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          &lt;p&gt;
                            The Rx main function period in seconds.
                          &lt;/p&gt;
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="Rx Main Function Period"/>
                  <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                  <a:da name="DEFAULT" value="0.01"/>
                  <a:da name="INVALID" type="Range">
                    <a:tst expr="&gt;0"/>
                    <a:tst expr="&lt;=4294967295"/>
                  </a:da>
                </v:var>
                <v:var name="SecOCMainFunctionPeriodTx" type="FLOAT">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          &lt;p&gt;
                            The Tx main function period in seconds.
                          &lt;/p&gt;
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="Tx Main Function Period"/>
                  <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                  <a:da name="DEFAULT" value="0.01"/>
                  <a:da name="INVALID" type="Range">
                    <a:tst expr="&gt;0"/>
                    <a:tst expr="&lt;=4294967295"/>
                  </a:da>
                </v:var>
                <v:var name="SecOCQueryFreshnessValue" type="ENUMERATION">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          &lt;p&gt;
                            This parameter specifies how the current freshness value shall be
                            determined.
                          &lt;/p&gt;

                          &lt;p&gt;Range:&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;&lt;code&gt;NONE&lt;/code&gt;: SecOC will not use freshness mechanism&lt;/li&gt;
                            &lt;li&gt;&lt;code&gt;CFUNC&lt;/code&gt;: SecOC queries the CDD for freshness value for every PDU,
                                by using the C function defined by the configuration parameter
                                &lt;code&gt;SecOCFreshnessValueFuncName&lt;/code&gt;&lt;/li&gt;
                            &lt;li&gt;&lt;code&gt;RTE&lt;/code&gt;: SecOC queries the SWC for freshness value for every PDU,
                                by using the Rte service port
                                &lt;code&gt;RxFreshnessManagement_&lt;SecOCFreshnessValueId&gt;&lt;/code&gt;
                                or &lt;code&gt;TxFreshnessManagement_&lt;SecOCFreshnessValueId&gt;&lt;/code&gt;&lt;/li&gt;
                          &lt;/ul&gt;
                          &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;Enable Rte Usage (&lt;code&gt;&lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/SecOC/ELEMENTS/SecOC/SecOCEbGeneral/SecOCRteUsage&quot;&gt;SecOCRteUsage&lt;/a&gt;&lt;/code&gt;): when RTE option is selected then the Rte usage must be enabled&lt;/li&gt;
                          &lt;/ul&gt;
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="Query Freshness Value"/>
                  <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:da name="DEFAULT" value="CFUNC"/>
                  <a:da name="INVALID" type="XPath" 
                        expr="(node:value(.)=&apos;RTE&apos; and node:value(../../SecOCEbGeneral/SecOCRteUsage) = &apos;false&apos;)" 
                        true="Enable Rte usage for SecOC to use the Rte interface for querying the freshness value."/>
                  <a:da name="RANGE">
                    <a:v>NONE</a:v>
                    <a:v>CFUNC</a:v>
                    <a:v>RTE</a:v>
                  </a:da>
                </v:var>
                <v:var name="SecOCVersionInfoApi" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          &lt;p&gt;
                            Enables version information API feature (&lt;code&gt;SecOC_GetVersionInfo()&lt;/code&gt;).
                          &lt;/p&gt;
                          &lt;p&gt;Range:&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;&lt;code&gt;TRUE&lt;/code&gt;: the API is available to be used&lt;/li&gt;
                            &lt;li&gt;&lt;code&gt;FALSE&lt;/code&gt;: the API is not available&lt;/li&gt;
                          &lt;/ul&gt;
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="Enable Version Info API"/>
                  <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                  <a:da name="DEFAULT" value="false"/>
                </v:var>
                <v:var name="SecOcIgnoreVerificationResult" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          &lt;p&gt;
                            The result of the authentication  process (e.g. MAC Verify) is ignored
                            after the first try (in case of failure) the SecOC proceeds like the result was a success.
                            The calculation of the authenticator is still done, only its result will
                            be ignored.
                          &lt;/p&gt;

                          &lt;p&gt;Range:&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;&lt;code&gt;TRUE&lt;/code&gt;: enabled (the verification result is ignored)&lt;/li&gt;
                            &lt;li&gt;&lt;code&gt;FALSE&lt;/code&gt;: disabled (the verification result is NOT ignored)&lt;/li&gt;
                          &lt;/ul&gt;
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="Enable Ignore Verification Result"/>
                  <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                  <a:da name="DEFAULT" value="false"/>
                </v:var>
                <v:var name="SecOCPropagateOnlyFinalVerificationStatus" 
                       type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          &lt;p&gt;
                            This parameter is used to specify when the verification status shall be reported.
                          &lt;/p&gt;

                          &lt;p&gt;Range:&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;&lt;code&gt;TRUE&lt;/code&gt;: the verification status shall be reported only after the final determination/calculation of the verification status&lt;/li&gt;
                            &lt;li&gt;&lt;code&gt;FALSE&lt;/code&gt;: the verification status shall be reported on every verification attempt&lt;/li&gt;
                          &lt;/ul&gt;
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" 
                       value="Enable Propagate Only Final Verification Status"/>
                  <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                  <a:da name="DEFAULT" value="false"/>
                </v:var>
                <v:var name="SecOCDevErrorDetect" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          &lt;p&gt;
                            &lt;strong&gt;Currently not supported.&lt;/strong&gt;
                          &lt;/p&gt;

                          &lt;p&gt;
                            Enables the Development Error Detection and Notification functionality.
                          &lt;/p&gt;
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="Enable Development Error Detection"/>
                  <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="73c17f28-66b3-412b-ad88-5a7109bca9e7"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="EDITABLE" value="false"/>
                  <a:da name="ENABLE" value="false"/>
                </v:var>
                <v:var name="SecOCEnableForcedPassOverride" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          &lt;p&gt;
                            When this configuration option is set to TRUE then the functionality inside of
                            the function &lt;code&gt;SecOC_VerifyStatusOverride()&lt;/code&gt; to send PDUs to upper layer independent
                            of the verification result is enabled.
                          &lt;/p&gt;
                          &lt;p&gt;Range:&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;&lt;code&gt;TRUE&lt;/code&gt;: the API functionality is extended&lt;/li&gt;
                            &lt;li&gt;&lt;code&gt;FALSE&lt;/code&gt;: the API functionality is not available&lt;/li&gt;
                          &lt;/ul&gt;
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="Enable Forced Pass Override"/>
                  <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="EDITABLE" value="true"/>
                </v:var>
                <v:var name="SecOCEnableSecurityEventReporting" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          &lt;p&gt;
                            Switches the reporting of security events to the IdsM
                          &lt;/p&gt;

                          &lt;p&gt;Range:&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;&lt;code&gt;TRUE&lt;/code&gt;: reporting is enabled&lt;/li&gt;
                            &lt;li&gt;&lt;code&gt;FALSE&lt;/code&gt;: reporting is disabled&lt;/li&gt;
                          &lt;/ul&gt;
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" 
                       value="Enables Security Event Reporting to IdsM"/>
                  <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                  <a:da name="DEFAULT" value="false"/>
                </v:var>
                <v:ctr name="SecOCSecurityEventRefs" type="IDENTIFIABLE">
                  <a:a name="DESC" 
                       value="EN: Container for the references to IdsMEvent elements representing the security events that the SecOC module shall report to the IdsM in case the coresponding security related event occurs. The standardized security events in this container can be extended by vendor-specific security events"/>
                  <a:da name="ENABLE" type="XPath" 
                        expr="(../SecOCEnableSecurityEventReporting = &apos;true&apos;)"/>
                  <a:da name="INVALID" type="XPath" 
                        expr="(num:i(count(*)) = 0) and (../SecOCEnableSecurityEventReporting = &apos;true&apos;)" 
                        true="If SecOCEnableSecurityEventReporting is enabled, at least oneref reference to security event IDs has to be added."/>
                  <v:ref name="SECOC_SEV_MAC_VERIFICATION_FAILED" 
                         type="SYMBOLIC-NAME-REFERENCE">
                    <a:a name="DESC" 
                         value="EN: MAC verification of a received PDU failed."/>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="OPTIONAL" value="true"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="POSTBUILDVARIANTMULTIPLICITY" value="false"/>
                    <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                    <a:a name="SCOPE" value="LOCAL"/>
                    <a:da name="ENABLE" value="false"/>
                    <a:da name="INVALID" type="Multi">
                      <mt:xpath 
                                false="Reference shall point to a valid node or must be disabled." 
                                expr="node:refvalid(.)"/>
                    </a:da>
                    <a:da name="REF" 
                          value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/IdsM/IdsMConfiguration/IdsMEvent"/>
                    <a:da name="WARNING" type="XPath" 
                          expr="node:value(as:ref(.)/IdsMExternalEventId) != 44" 
                          true="The given external event ID is not equal to the ID defined in AUTOSAR specification, which is 44."/>
                  </v:ref>
                  <v:ref name="SECOC_SEV_FRESHNESS_NOT_AVAILABLE" 
                         type="SYMBOLIC-NAME-REFERENCE">
                    <a:a name="DESC" 
                         value="EN: Faild to get freshness value from FvM."/>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="OPTIONAL" value="true"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="POSTBUILDVARIANTMULTIPLICITY" value="false"/>
                    <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                    <a:a name="SCOPE" value="LOCAL"/>
                    <a:da name="ENABLE" value="false"/>
                    <a:da name="INVALID" type="Multi">
                      <mt:xpath 
                                false="Reference shall point to a valid node or must be disabled." 
                                expr="node:refvalid(.)"/>
                    </a:da>
                    <a:da name="REF" 
                          value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/IdsM/IdsMConfiguration/IdsMEvent"/>
                    <a:da name="WARNING" type="XPath" 
                          expr="node:value(as:ref(.)/IdsMExternalEventId) != 45" 
                          true="The given external event ID is not equal to the ID defined in AUTOSAR specification, which is 45."/>
                  </v:ref>
                </v:ctr>
                <v:var name="SecOCMaxAlignScalarType" type="STRING">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          &lt;p&gt;
                            &lt;strong&gt;Currently not supported.&lt;/strong&gt;
                          &lt;/p&gt;

                          &lt;p&gt;
                            The type with maximal alignment restrictions on the platform.
                          &lt;/p&gt;
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" 
                       value="Type with maximal alignment restrictions"/>
                  <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:da name="EDITABLE" value="false"/>
                  <a:da name="ENABLE" value="false"/>
                  <a:da name="INVALID" type="XPath" 
                        expr="(. != &apos;uint8&apos;) and (. != &apos;uint16&apos;) and (. != &apos;uint32&apos;)" 
                        true="Only uint8, uint16 and uint32 values are allowed."/>
                </v:var>
                <v:lst name="SecOCVerificationStatusCallout">
                  <a:a name="COLUMNS" value="SecOCVerificationStatusCallout"/>
                  <a:a name="LABEL" value="VerificationStatus Callout"/>
                  <a:da name="MAX" value="65535"/>
                  <a:da name="MIN" value="0"/>
                  <v:var name="SecOCVerificationStatusCallout" 
                         type="FUNCTION-NAME">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              Name of a Callout function, which may be invoked on every
                              authentication verification attempt.
                            &lt;/p&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v class="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Callout function name"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:da name="INVALID" type="XPath">
                      <a:tst expr="text:uniq(../*,.)" 
                             false="The names of the callout functions are not unique."/>
                      <a:tst 
                             expr="(. != &apos;&apos;) and text:match(., &apos;^[a-zA-Z][a-zA-Z0-9_]*$&apos;)" 
                             false="Missing or invalid C function name."/>
                    </a:da>
                  </v:var>
                </v:lst>
                <v:lst name="SecOCMacGenerateStatusCallout">
                  <a:a name="COLUMNS" value="SecOCMacGenerateStatusCallout"/>
                  <a:a name="LABEL" value="MacGenerateStatus Callout"/>
                  <a:da name="MAX" value="65535"/>
                  <a:da name="MIN" value="0"/>
                  <v:var name="SecOCMacGenerateStatusCallout" 
                         type="FUNCTION-NAME">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              Name of a Callout function, which may be invoked after
                              the MAC Generate failed.
                            &lt;/p&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v class="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Callout function name"/>
                    <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                    <a:da name="INVALID" type="XPath">
                      <a:tst expr="text:uniq(../*,.)" 
                             false="The names of the callout functions are not unique."/>
                      <a:tst 
                             expr="(. != &apos;&apos;) and text:match(., &apos;^[a-zA-Z][a-zA-Z0-9_]*$&apos;)" 
                             false="Missing or invalid C function name."/>
                    </a:da>
                  </v:var>
                </v:lst>
                <v:var name="SecOCOverrideStatusWithDataId" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          &lt;p&gt;
                            This option defines if the parameter &quot;ValueId&quot; of the function
                            SecOC_VerifyStatusOverride() accepts the freshness value
                            (as a collection of one or more secured PDUs to freshness)
                            or the dataID for individual secured PDUs.
                          &lt;/p&gt;
                          &lt;p&gt;Range:&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;&lt;code&gt;TRUE&lt;/code&gt;: Function SecOC_VerifyStatusOverride accepts
                            SecOCDataId as parameter.&lt;code&gt;&lt;/code&gt;&lt;/li&gt;
                            &lt;li&gt;&lt;code&gt;FALSE&lt;/code&gt;: Function SecOC_VerifyStatusOverride accepts
                            SecOCFreshnessValueId as parameter.&lt;code&gt;&lt;/code&gt;&lt;/li&gt;
                          &lt;/ul&gt;
                          &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;Verify Status Override API Version (&lt;code&gt;&lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/SecOC/ELEMENTS/SecOC/SecOCEbGeneral/SecOCEbVerifyStatusOverrideApiVersion&quot;&gt;SecOCEbVerifyStatusOverrideApiVersion&lt;/a&gt;&lt;/code&gt;): must be set to SECOC_API_VERSION_20_11 in order to be able to use this feature&lt;/li&gt;
                          &lt;/ul&gt;
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="Enable Override Status With Data ID"/>
                  <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="ENABLE" type="XPath" 
                        expr="node:value(../../SecOCEbGeneral/SecOCEbVerifyStatusOverrideApiVersion)=&apos;SECOC_API_VERSION_20_11&apos;"/>
                </v:var>
                <v:var name="SecOCDefaultAuthenticationInformationPattern" 
                       type="INTEGER">
                  <a:a name="DEFAULT_RADIX" value="HEX"/>
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          &lt;p&gt;
                          If this parameter is enabled and configured, SecOC will use this value as a pattern for each byte of Freshness Value and Authenticator
                          when building the Authentication Information, and will not cancel the transmission request upon failure.
                          &lt;/p&gt;
                          &lt;p&gt;
                          The SecOC proceeds with the transmission in following cases:
                          &lt;ul&gt;
                          &lt;li&gt;
                          When authentication build counter has reached the configuration value  &lt;code&gt;SecOCAuthenticationBuildAttempts&lt;/code&gt;
                          &lt;/li&gt;
                          &lt;li&gt;
                          The query of the freshness function returns E_NOT_OK
                          &lt;/li&gt;
                          &lt;li&gt;
                          The calculation of the authenticator has returned a non-recoverable error such as returning E_NOT_OK or KEY_FAILURE.
                          &lt;/li&gt;
                          &lt;/ul&gt;
                          &lt;/p&gt;
                          &lt;br/&gt;
                          &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;Secured Tx PDUs (&lt;code&gt;&lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/SecOC/ELEMENTS/SecOC/SecOCGeneral/SecOCTxPduProcessing&quot;&gt;SecOCTxPduProcessing&lt;/a&gt;&lt;/code&gt;): at least one secured Tx PDU must be configured&lt;/li&gt;
                          &lt;/ul&gt;
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" 
                       value="Default Authentication Information Pattern"/>
                  <a:a name="OPTIONAL" value="true"/>
                  <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:da name="DEFAULT" value="0"/>
                  <a:da name="EDITABLE" value="true"/>
                  <a:da name="ENABLE" value="false"/>
                  <a:da name="INVALID" type="Multi">
                    <mt:range>
                      <mt:tst expr="&lt;=255"/>
                      <mt:tst expr="&gt;=0"/>
                    </mt:range>
                    <mt:xpath 
                              true="There must be at least one Tx PDU configured." 
                              expr="num:i(count(../../SecOCTxPduProcessing/*)) = 0"/>
                  </a:da>
                </v:var>
              </v:ctr>
              <v:ctr name="SecOCEbGeneral" type="IDENTIFIABLE">
                <v:var name="SecOCASR403" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          &lt;p&gt;
                            Specifies whether the AUTOSAR 4.0.3 APIs or the AUTOSAR 4.2.1 APIs shall
                            be used for &lt;code&gt;PduR&lt;/code&gt; interfaces (e.g.
                            &lt;code&gt;SecOC_StartOfReception()&lt;/code&gt;, &lt;code&gt;SecOC_Transmit()&lt;/code&gt;,
                            &lt;code&gt;SecOC_RxIndication()&lt;/code&gt;).
                          &lt;/p&gt;

                          &lt;p&gt;Range:&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;&lt;code&gt;TRUE&lt;/code&gt;: AUTOSAR 4.0.3 APIs shall be used in the interaction with the PduR&lt;/li&gt;
                            &lt;li&gt;&lt;code&gt;FALSE&lt;/code&gt;: AUTOSAR 4.2.1 APIs shall be used in the interaction with the PduR&lt;/li&gt;
                          &lt;/ul&gt;
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="Enable AUTOSAR 4.0.3 PduR"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="TAB" value="EB General"/>
                  <a:da name="DEFAULT" value="false"/>
                </v:var>
                <v:var name="SecOCEbPropagateVerificationStatusApiVersion" 
                       type="ENUMERATION">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          &lt;p&gt;
                            Specifies whether the option to propagate the verification status,
                            through sender /receiver RTE service or C functions, is used or not.
                          &lt;/p&gt;
                          &lt;p&gt;
                            The difference between &lt;code&gt;AUTOSAR&lt;/code&gt; and &lt;code&gt;EB_CUSTOM&lt;/code&gt; is in the type
                            &lt;code&gt;SecOC_VerificationStatusType&lt;/code&gt; which has an additional member &lt;code&gt;verificationStatus&lt;/code&gt;
                            where the return value of the &quot;mac verification&quot; or &quot;get freshness&quot; operations is being stored.
                          &lt;/p&gt;
                          &lt;p&gt;Range:&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;&lt;code&gt;NONE&lt;/code&gt;: SecOC will not propagate the verification status&lt;/li&gt;
                            &lt;li&gt;&lt;code&gt;SECOC_API_VERSION_430&lt;/code&gt;: SecOC will propagate the verification status according to AUTOSAR 4.3.0 specifications&lt;/li&gt;
                            &lt;li&gt;&lt;code&gt;SECOC_API_VERSION_20_11&lt;/code&gt;: SecOC will propagate the verification status according to AUTOSAR 20-11 specifications&lt;/li&gt;
                            &lt;li&gt;&lt;code&gt;EB_CUSTOM&lt;/code&gt;: SecOC will propagate the verification status via the custom API(s)&lt;/li&gt;
                          &lt;/ul&gt;
                          &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;Enable Rte Usage (&lt;code&gt;&lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/SecOC/ELEMENTS/SecOC/SecOCEbGeneral/SecOCRteUsage&quot;&gt;SecOCRteUsage&lt;/a&gt;&lt;/code&gt;): in order to propagate the verification status via RTE&lt;/li&gt;
                            &lt;li&gt;Verification Status Callout Functions (&lt;code&gt;&lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/SecOC/ELEMENTS/SecOC/SecOCGeneral/SecOCVerificationStatusCallout&quot;&gt;SecOCVerificationStatusCallout&lt;/a&gt;&lt;/code&gt;): in order to propagate the verification status via C functions&lt;/li&gt;
                            &lt;li&gt;Verify Status Override API Version (&lt;code&gt;&lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/SecOC/ELEMENTS/SecOC/SecOCEbGeneral/SecOCEbVerifyStatusOverrideApiVersion&quot;&gt;SecOCEbVerifyStatusOverrideApiVersion&lt;/a&gt;&lt;/code&gt;): limits the range in which &quot;Propagate Verification Status API Version&quot; can be configured&lt;/li&gt;
                          &lt;/ul&gt;
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" 
                       value="Propagate Verification Status API Version"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="TAB" value="EB General"/>
                  <a:da name="DEFAULT" value="NONE"/>
                  <a:da name="EDITABLE" value="true"/>
                  <a:da name="INVALID" type="XPath">
                    <a:tst 
                           expr="node:value(.) != &apos;NONE&apos; and num:i(count(../../SecOCGeneral/SecOCVerificationStatusCallout/*)) = 0 and node:value(../SecOCRteUsage) = &apos;false&apos;" 
                           true="To propagate the verification status the RTE usage must be enabled or at least one callout function must be configured in the VerificationStatus Callout container."/>
                    <a:tst 
                           expr="node:value(.) = &apos;SECOC_API_VERSION_430&apos; and node:value(../SecOCEbVerifyStatusOverrideApiVersion) != &apos;SECOC_API_VERSION_430&apos;" 
                           true="To propagate the verification status according to AUTOSAR 4.3.0 specifications then SecOCEbVerifyStatusOverrideApiVersion must be set to SECOC_API_VERSION_430."/>
                    <a:tst 
                           expr="node:value(.) = &apos;EB_CUSTOM&apos; and node:value(../SecOCEbVerifyStatusOverrideApiVersion) != &apos;SECOC_API_VERSION_430&apos;" 
                           true="To propagate the verification status according to EB custom solution then SecOCEbVerifyStatusOverrideApiVersion must be set to SECOC_API_VERSION_430."/>
                    <a:tst 
                           expr="node:value(.) = &apos;SECOC_API_VERSION_20_11&apos; and node:value(../SecOCEbVerifyStatusOverrideApiVersion) != &apos;SECOC_API_VERSION_20_11&apos;" 
                           true="To propagate the verification status according to AUTOSAR 20-11 specifications then SecOCEbVerifyStatusOverrideApiVersion must be set to SECOC_API_VERSION_20_11."/>
                  </a:da>
                  <a:da name="RANGE">
                    <a:v>NONE</a:v>
                    <a:v>SECOC_API_VERSION_430</a:v>
                    <a:v>SECOC_API_VERSION_20_11</a:v>
                    <a:v>EB_CUSTOM</a:v>
                  </a:da>
                </v:var>
                <v:var 
                       name="SecOCEbClientServerPropagateVerificationStatusApiVersion" 
                       type="ENUMERATION">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          &lt;p&gt;
                            Specifies whether the option to propagate the verification status,
                            through RTE client server services, is used or not.
                          &lt;/p&gt;
                          &lt;p&gt;Range:&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;&lt;code&gt;NONE&lt;/code&gt;: SecOC will not propagate the verification status&lt;/li&gt;
                            &lt;li&gt;&lt;code&gt;SECOC_API_VERSION_20_11&lt;/code&gt;: SecOC will propagate the verification status according to AUTOSAR 20-11 specifications&lt;/li&gt;
                          &lt;/ul&gt;
                          &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;Enable Rte Usage (&lt;code&gt;&lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/SecOC/ELEMENTS/SecOC/SecOCEbGeneral/SecOCRteUsage&quot;&gt;SecOCRteUsage&lt;/a&gt;&lt;/code&gt;): in order to propagate the verification status via RTE&lt;/li&gt;
                          &lt;/ul&gt;
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" 
                       value="C/S Propagate Verification Status API Version"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="TAB" value="EB General"/>
                  <a:da name="DEFAULT" value="NONE"/>
                  <a:da name="EDITABLE" value="true"/>
                  <a:da name="INVALID" type="XPath" 
                        expr="node:value(.) != &apos;NONE&apos; and node:value(../SecOCRteUsage) = &apos;false&apos;" 
                        true="To propagate the verification status the RTE usage must be enabled."/>
                  <a:da name="RANGE">
                    <a:v>NONE</a:v>
                    <a:v>SECOC_API_VERSION_20_11</a:v>
                  </a:da>
                </v:var>
                <v:var name="SecOCEbVerifyStatusOverrideApiVersion" 
                       type="ENUMERATION">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                        &lt;p&gt;
                          Specifies the compatibility of the SecOC module API
                          description as specified by the configured AUTOSAR version.
                          Specifies whether the AUTOSAR 4.3.0 implementation or the AUTOSAR 20-11
                          implementation shall be used for &lt;code&gt;VerifyStatusOverride&lt;/code&gt;
                          interface.
                        &lt;/p&gt;
                        &lt;p&gt;Range:&lt;/p&gt;
                        &lt;ul&gt;
                          &lt;li&gt;&lt;code&gt;SECOC_API_VERSION_430&lt;/code&gt;: Provide and expect an API
                          description as specified by AUTOSAR 4.3.0.&lt;code&gt;&lt;/code&gt;&lt;/li&gt;
                          &lt;li&gt;&lt;code&gt;SECOC_API_VERSION_20_11&lt;/code&gt;: Provide and expect an API
                          description as specified by AUTOSAR 20-11.&lt;code&gt;&lt;/code&gt;&lt;/li&gt;
                        &lt;/ul&gt;
                        &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                        &lt;ul&gt;
                          &lt;li&gt;Data ID Length (&lt;code&gt;&lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/SecOC/ELEMENTS/SecOC/SecOCEbGeneral/SecOCDataIdLength&quot;&gt;SecOCDataIdLength&lt;/a&gt;&lt;/code&gt;): when SECOC_API_VERSION_20_11 was selected then the data ID length must be set to UINT16&lt;/li&gt;
                        &lt;/ul&gt;
                      &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="Verify Status Override API Version"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="TAB" value="EB General"/>
                  <a:da name="DEFAULT" value="SECOC_API_VERSION_430"/>
                  <a:da name="INVALID" type="XPath" 
                        expr="(node:value(.) = &apos;SECOC_API_VERSION_20_11&apos; and node:value(../SecOCDataIdLength) != &apos;UINT16&apos;)" 
                        true="In order to use SECOC_API_VERSION_20_11 the value of SecOCDataIdLength needs to be configured to UINT16."/>
                  <a:da name="RANGE">
                    <a:v>SECOC_API_VERSION_430</a:v>
                    <a:v>SECOC_API_VERSION_20_11</a:v>
                  </a:da>
                </v:var>
                <v:var name="SecOCEnableMetaDataUse" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          &lt;p&gt;
                            Enables the use of Meta Data transfer from the secured PDU to the authentic PDU.
                          &lt;/p&gt;
                          &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;Enable meta data usage (&lt;code&gt;&lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/EcuC/ELEMENTS/EcuC/EcucGeneral/EcucMetaDataHandlingEnabled&quot;&gt;EcucMetaDataHandlingEnabled&lt;/a&gt;&lt;/code&gt;): must be set to enabled in order to be able to use this feature&lt;/li&gt;
                          &lt;/ul&gt;
                          &lt;p&gt;&lt;strong&gt;For SecOCPduCollection, MetaData is not yet supported.&lt;/strong&gt;&lt;/p&gt;
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="Enable Meta Data Usage"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="TAB" value="EB General"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="INVALID" type="XPath">
                    <a:tst 
                           expr="node:value(.) = &apos;true&apos; and node:value(as:modconf(&apos;EcuC&apos;)[1]/EcucGeneral/EcucMetaDataHandlingEnabled) = &apos;false&apos;" 
                           true="To be able to use the meta-data in SecOC, the EcuC meta-data handling must also be enabled."/>
                    <a:tst 
                           expr="node:value(.) = &apos;true&apos; and num:i(count(../../SecOCRxPduProcessing/*[node:exists(SecOCEnableMetaData)and node:value(SecOCEnableMetaData) = &apos;true&apos;])) =0" 
                           true="At least one RxPDU needs to have the meta-data usage enabled."/>
                  </a:da>
                </v:var>
                <v:var name="SecOCRteUsage" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          &lt;p&gt;
                            Enables the use of RTE.
                          &lt;/p&gt;
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="Enable Rte Usage"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="TAB" value="EB General"/>
                  <a:da name="DEFAULT" value="false"/>
                </v:var>
                <v:var name="SecOCUseSecuredArea" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          &lt;p&gt;
                            Specifies whether the option to configure an area in the authentic data
                            that will be the input to the authenticator verification algorithm
                            is enabled or not.
                          &lt;/p&gt;
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="Enable Secured Area Usage"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="TAB" value="EB General"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="EDITABLE" value="true"/>
                </v:var>
                <v:var name="SecOCCryptoBitLength" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                         &lt;p&gt;
                           Specifies, whether the length of the authenticator can be passed to the
                           cryptographic routines in bits or in bytes.
                         &lt;/p&gt;

                         &lt;p&gt;Range:&lt;/p&gt;
                         &lt;ul&gt;
                          &lt;li&gt;&lt;code&gt;TRUE&lt;/code&gt;:  the length of the authenticator is passed to the
                              cryptographic routines in bits&lt;/li&gt;
                          &lt;li&gt;&lt;code&gt;FALSE&lt;/code&gt;: the length of the authenticator is passed to the
                              cryptographic routines in bytes&lt;/li&gt;
                         &lt;/ul&gt;
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="Enable Crypto Bit Length Usage"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="TAB" value="EB General"/>
                  <a:da name="DEFAULT" value="true"/>
                </v:var>
                <v:var name="SecOCRelocatablePbcfgEnable" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          &lt;p&gt;
                            Enables/disable support for relocatable postbuild configuration.
                          &lt;/p&gt;

                          &lt;p&gt;Range:&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;True: Postbuild configuration relocatable in memory.&lt;/li&gt;
                            &lt;li&gt;False: Postbuild configuration not relocatable in memory.&lt;/li&gt;
                          &lt;/ul&gt;
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" 
                       value="Enable Post-build Relocatable Configuration"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="TAB" value="EB General"/>
                  <a:da name="DEFAULT" value="true"/>
                  <a:da name="EDITABLE" type="XPath" 
                        expr="(node:contains(node:refs(as:modconf(&apos;PbcfgM&apos;)/PbcfgMBswModules/*/PbcfgMBswModuleRef), as:modconf(&apos;SecOC&apos;)) = &apos;true&apos;)" 
                        true="false" false="true"/>
                </v:var>
                <v:var name="SecOCIgnoreFvMFailures" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          &lt;p&gt;
                            The result of the freshness process (e.g. calling FvM function) for the RX PDU is ignored and the SecOC proceeds
                            with like the result was a success. The authentication process for the received PDU will be skipped
                            and the PDU will be forwarded to the upper layer.
                          &lt;/p&gt;
                          &lt;p&gt;Range:&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;&lt;code&gt;TRUE&lt;/code&gt;:  enabled  (FvM result is ignored).&lt;/li&gt;
                            &lt;li&gt;&lt;code&gt;FALSE&lt;/code&gt;: disabled (FvM result is NOT ignored).&lt;/li&gt;
                          &lt;/ul&gt;
                          &lt;br/&gt;
                          &lt;p&gt;
                          When SecOCIgnoreFvMFailures is set to TRUE:
                          &lt;ul&gt;
                          &lt;li&gt;
                          If &lt;code&gt;SecOCAuthenticationBuildAttempts&lt;/code&gt; is reached or equal to ZERO, the SecOC proceeds with like the result was a success after a failure.
                          The authentication process will be skipped and the PDU will be forwarded to the upper layer.
                          &lt;/li&gt;
                          &lt;li&gt;
                          If &lt;code&gt;AuthenticationVerifyAttempts&lt;/code&gt; is reached or equal to ZERO, the SecOC proceeds with like the result was a success after a failure
                          including MAC verification failures. The PDU will be forwarded to the upper layer.
                          &lt;/li&gt;
                          &lt;/ul&gt;
                          &lt;/p&gt;
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="Ignore FvM Failures"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="TAB" value="EB General"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="INVALID" type="XPath" 
                        expr="count(../../SecOCRxPduProcessing/*) = 0 and node:value(.) = &apos;true&apos;" 
                        true="At least one Rx Processing PDU must be configured to enable this feature."/>
                </v:var>
                <v:var name="SecOCRxShapeFuncName" type="FUNCTION-NAME">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          &lt;p&gt;
                            This parameter specifies the name of the C API function which shall be
                            called by the SecOC to update a project specific layout of the secured PDU, which deviates from the AUTOSAR standard,
                            to the layout defined by the AUTOSAR standard
                            before the verification procedure is started.
                          &lt;/p&gt;
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="Shaping Rx Function Name"/>
                  <a:a name="OPTIONAL" value="true"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="TAB" value="EB General"/>
                  <a:da name="ENABLE" value="false"/>
                  <a:da name="INVALID" type="XPath" 
                        expr="(. != &apos;&apos;) and text:match(., &apos;^[a-zA-Z][a-zA-Z0-9_]*$&apos;)" 
                        false="Missing or invalid C function name."/>
                </v:var>
                <v:var name="SecOCTxShapeFuncName" type="FUNCTION-NAME">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          &lt;p&gt;
                            This parameter specifies the name of the C API function which shall be
                            called to modify the layout of the secured PDU before it is send to the lower layer.
                            So the layout of a secured PDU on the bus can be adapted project specific deviating from the AUTOSAR standard.
                          &lt;/p&gt;
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="Shaping Tx Function Name"/>
                  <a:a name="OPTIONAL" value="true"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="TAB" value="EB General"/>
                  <a:da name="ENABLE" value="false"/>
                  <a:da name="INVALID" type="XPath" 
                        expr="(. != &apos;&apos;) and text:match(., &apos;^[a-zA-Z][a-zA-Z0-9_]*$&apos;)" 
                        false="Missing or invalid C function name."/>
                </v:var>
                <v:var name="SecOCDefaultAuthenticatorValue" type="INTEGER">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          &lt;p&gt;
                            This parameter defines the default value for the authenticator.
                            The configured value will be set for every byte within the authenticator.
                          &lt;/p&gt;
                          &lt;p&gt;
                            Parameter ENABLE: SecOC shall send secured messages with the default MAC,
                            if the MAC could not be generated, i.e. Csm_MacGenerate returns something
                            different than E_OK.
                          &lt;/p&gt;
                          &lt;p&gt;
                            Parameter DISABLE: SecOC shall not send secured messages,
                            if the MAC could not be generated.
                          &lt;/p&gt;
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="Default Authenticator Value"/>
                  <a:a name="OPTIONAL" value="true"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="TAB" value="EB General"/>
                  <a:da name="EDITABLE" value="true"/>
                  <a:da name="ENABLE" value="false"/>
                  <a:da name="INVALID" type="Range">
                    <a:tst expr="&gt;=0"/>
                    <a:tst expr="&lt;=255"/>
                  </a:da>
                </v:var>
                <v:var name="SecOCDataIdLength" type="ENUMERATION">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          &lt;p&gt;
                            This parameter defines the length in bits of the PDU Data ID.
                          &lt;/p&gt;

                          &lt;p&gt;Range:&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;&lt;code&gt;UINT8&lt;/code&gt;: PDU Data ID will have 8 bits length&lt;/li&gt;
                            &lt;li&gt;&lt;code&gt;UINT16&lt;/code&gt;: PDU Data ID will have 16 bits length&lt;/li&gt;
                            &lt;li&gt;&lt;code&gt;UINT32&lt;/code&gt;: PDU Data ID will have 32 bits length&lt;/li&gt;
                          &lt;/ul&gt;
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="Data ID Length"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="TAB" value="EB General"/>
                  <a:da name="DEFAULT" value="UINT16"/>
                  <a:da name="RANGE">
                    <a:v>UINT8</a:v>
                    <a:v>UINT16</a:v>
                    <a:v>UINT32</a:v>
                  </a:da>
                </v:var>
                <v:ctr name="SecOCBypassAuthenticationRoutine" 
                       type="IDENTIFIABLE">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          &lt;p&gt;
                            This feature provides the ability to bypass the authentication
                            routine, when enabled, the secured PDU or cryptographic PDU shall be
                            send to the lower layer with a default value for the authentication
                            information (authenticator/MAC + truncated freshness value).
                          &lt;/p&gt;
                          &lt;p&gt;
                            Furthermore, when this feature is enabled during the runtime by calling
                            the provided API, the FvM and Csm interface shall not be called.
                          &lt;/p&gt;
                          &lt;p&gt;
                            Enable support for SecOC_BypassAuthRoutine() API which can be used to
                            enabled/disable the bypass mechanism during the runtime.
                          &lt;/p&gt;
                          &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;Secured Tx PDUs (&lt;code&gt;&lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/SecOC/ELEMENTS/SecOC/SecOCGeneral/SecOCTxPduProcessing&quot;&gt;SecOCTxPduProcessing&lt;/a&gt;&lt;/code&gt;): at least one secured Tx PDU must be configured&lt;/li&gt;
                          &lt;/ul&gt;
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="LABEL" value="Enable Bypass Authentication Routine"/>
                  <a:a name="OPTIONAL" value="true"/>
                  <a:a name="TAB" value="EB General"/>
                  <a:da name="ENABLE" value="false"/>
                  <a:da name="INVALID" type="XPath" 
                        expr="num:i(count(../../SecOCTxPduProcessing/*)) = 0" 
                        true="There must be at least one Tx PDU configured."/>
                  <v:var name="SecOCDefaultAuthenticationInfoValue" 
                         type="INTEGER">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              This parameter defines the default value for the authentication information.
                              The configured value will be set for every byte within the authentication information.
                            &lt;/p&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" 
                         value="Default Authentication Information Value"/>
                    <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                    <a:da name="EDITABLE" value="true"/>
                    <a:da name="INVALID" type="Range">
                      <a:tst expr="&gt;=0"/>
                      <a:tst expr="&lt;=255"/>
                    </a:da>
                  </v:var>
                </v:ctr>
                <v:var name="SecOCCsmJobRefCallout" type="FUNCTION-NAME">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          &lt;p&gt;
                            This parameter defines the name of the callout function that
                            shall be called by the SecOC for every item
                            in &lt;code&gt;&lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/SecOC/ELEMENTS/SecOC/SecOCRxPduProcessing&quot;&gt;Secured Rx PDUs&lt;/a&gt;&lt;/code&gt; and
                            &lt;code&gt;&lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/SecOC/ELEMENTS/SecOC/SecOCTxPduProcessing&quot;&gt;Secured Tx PDUs&lt;/a&gt;&lt;/code&gt; to obtain
                            the Csm job ID.
                          &lt;/p&gt;
                          &lt;p&gt;
                            This function shall be called in the context of &lt;code&gt;SecOC_Init()&lt;/code&gt; with
                            Csm job ID extracted from the referenced &lt;code&gt;SecOCRxAuthServiceConfigRef&lt;/code&gt; or
                            &lt;code&gt;SecOCTxAuthServiceConfigRef&lt;/code&gt; as the input parameter.
                          &lt;/p&gt;
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" 
                       value="Callout Function To Obtain The Csm Job ID"/>
                  <a:a name="OPTIONAL" value="true"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="TAB" value="EB General"/>
                  <a:da name="ENABLE" value="false"/>
                  <a:da name="INVALID" type="XPath" 
                        expr="(. != &apos;&apos;) and text:match(., &apos;^[a-zA-Z][a-zA-Z0-9_]*$&apos;)" 
                        false="Missing or invalid C function name."/>
                </v:var>
              </v:ctr>
              <v:lst name="SecOCSameBufferPduCollection" type="MAP">
                <a:a name="COLUMNS" value="SecOCBufferLength"/>
                <a:a name="LABEL" value="Same Buffer PDU Collection"/>
                <v:ctr name="SecOCSameBufferPduCollection" type="IDENTIFIABLE">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          &lt;p&gt;
                            The buffer configuration that may be used by a collection of PDUs.
                          &lt;/p&gt;
                          &lt;p&gt;
                            The buffer can be used either by Rx PDUs or Tx PDUs, it cannot be used/
                            configured that both Rx and Tx PDUs can use it.
                          &lt;/p&gt;
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <v:var name="SecOCBufferLength" type="INTEGER">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              This parameter defines the length in bytes of the buffer, which is
                              used by the SecOC module.
                            &lt;/p&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Buffer Length"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:da name="EDITABLE" value="true"/>
                    <a:da name="ENABLE" value="true"/>
                    <a:da name="RANGE" type="Range">
                      <a:tst expr="&gt;=0"/>
                      <a:tst expr="&lt;=4294967295"/>
                    </a:da>
                  </v:var>
                </v:ctr>
              </v:lst>
              <v:lst name="SecOCRxPduProcessing" type="MAP">
                <a:a name="LABEL" value="Secured Rx PDUs"/>
                <a:da name="MAX" value="65535"/>
                <a:da name="MIN" value="0"/>
                <v:ctr name="SecOCRxPduProcessing" type="IDENTIFIABLE">
                  <a:da name="INVALID" type="XPath" 
                        expr="count(../*[SecOCFreshnessValueId = node:value(node:current()/SecOCFreshnessValueId)]) = count(../*[(SecOCFreshnessValueId = node:value(node:current()/SecOCFreshnessValueId))                                  and (SecOCFreshnessValueLength = node:value(node:current()/SecOCFreshnessValueLength))])" 
                        false="The secured RX PDUs with the same freshness value ID must have                                   the same freshness value length."/>
                  <v:ref name="SecOCRxPduMainFunctionRef" 
                         type="SYMBOLIC-NAME-REFERENCE">
                    <a:a name="COLUMN_TITLE" value="Main Function Reference"/>
                    <a:a name="COLUMN_TOOLTIP" value="SecOCRxPduMainFunctionRef"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              Reference to the main function (&lt;code&gt;&lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/SecOC/ELEMENTS/SecOC/SecOCMainFunctionRx&quot;&gt;SecOCMainFunctionRx&lt;/a&gt;&lt;/code&gt;)
                              which shall process this secured PDU.
                              Mandatory, if multiple main functions are defined.
                            &lt;/p&gt;
                            &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                            &lt;ul&gt;
                              &lt;li&gt;SecOC Main Function (&lt;code&gt;&lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/SecOC/ELEMENTS/SecOC/SecOCMainFunctionRx&quot;&gt;SecOCMainFunctionRx&lt;/a&gt;&lt;/code&gt;): at least one main function must be configured in the list in order to be able to use this feature&lt;/li&gt;
                              &lt;li&gt;Csm Main Function (&lt;code&gt;&lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/Csm/ELEMENTS/Csm/CsmMainFunction&quot;&gt;CsmMainFunction&lt;/a&gt;&lt;/code&gt;): must reference the same EcuC partition as the SecOC main function&lt;/li&gt;
                            &lt;/ul&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Main Function Reference"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:da name="ENABLE" type="XPath" 
                          expr="num:i(count(../../../SecOCMainFunctionRx/*)) &gt; 0"/>
                    <a:da name="INVALID" type="XPath">
                      <a:tst expr="(node:refvalid(.))" 
                             false="The expression argument reference is not valid."/>
                      <a:tst 
                             expr="node:ref(.)/SecOCMainFunctionRxPartitionRef = node:fallback(&apos;-&gt;node:ref(node:ref(node:ref(../SecOCRxAuthServiceConfigRef)/CsmJobQueueRef)/CsmQueueMainFunctionRef)/CsmMainFunctionPartitionRef&apos;, node:ref(.)/SecOCMainFunctionRxPartitionRef)" 
                             false="The RX MainFunction has to reference the same EcuC partition as the associated Csm queue."/>
                    </a:da>
                    <a:da name="REF" 
                          value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/SecOC/SecOCMainFunctionRx"/>
                  </v:ref>
                  <v:ref name="SecOCRxAuthServiceConfigRef" 
                         type="CHOICE-REFERENCE">
                    <a:a name="COLUMN_TITLE" value="Verification Job Reference"/>
                    <a:a name="COLUMN_TOOLTIP" 
                         value="SecOCRxAuthServiceConfigRef"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              This parameter defines the authentication algorithm used for
                              authentication verification.
                            &lt;/p&gt;
                            &lt;p&gt;
                              The value of this parameter must be a valid configuration of a
                              MacVerify or SignatureVerify configuration in a Csm module.
                            &lt;/p&gt;

                            &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                            &lt;ul&gt;
                              &lt;li&gt;Enable PDU Verification (&lt;code&gt;&lt;a href=&quot;path:../SecOCRxSecuredPduLayer/SecOCSecuredRxPduVerification&quot;&gt;SecOCSecuredRxPduVerification&lt;/a&gt;&lt;/code&gt;): in order to able to configure the verification job, the verification must be enabled&lt;/li&gt;
                            &lt;/ul&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Verification Job Reference"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:da name="EDITABLE" value="true"/>
                    <a:da name="ENABLE" value="true"/>
                    <a:da name="INVALID" type="XPath">
                      <a:tst 
                             expr="node:value(../SecOCRxSecuredPduLayer/SecOCSecuredRxPduVerification) = &apos;true&apos; and not(node:refvalid(.))" 
                             true="The expression argument reference is not valid."/>
                      <a:tst 
                             expr="node:value(../SecOCRxSecuredPduLayer/SecOCSecuredRxPduVerification) = &apos;true&apos; and not(node:exists(as:ref(as:ref(.)/CsmJobPrimitiveRef)/CsmMacVerify) or node:exists(as:ref(as:ref(.)/CsmJobPrimitiveRef)/CsmSignatureVerify))" 
                             true="The referenced Csm job needs to be configured to use the MacVerify as primitive."/>
                    </a:da>
                    <a:da name="REF" 
                          value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/Csm/CsmJobs/CsmJob"/>
                  </v:ref>
                  <v:var name="SecOCAuthInfoTxLength" type="INTEGER">
                    <a:a name="COLUMN_TITLE" 
                         value="Authenticator Truncated Length"/>
                    <a:a name="COLUMN_TOOLTIP" value="SecOCAuthInfoTxLength"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              This parameter defines the length in bits of the authenticator (MAC),
                              which is included in the payload of the secured PDU or cryptographic PDU.
                            &lt;/p&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Authenticator Truncated Length"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                    <a:da name="EDITABLE" value="true"/>
                    <a:da name="INVALID" type="Range">
                      <a:tst expr="&gt;=1"/>
                      <a:tst expr="&lt;=65535"/>
                    </a:da>
                  </v:var>
                  <v:var name="SecOCDataId" type="INTEGER">
                    <a:a name="COLUMN_TITLE" value="Data ID"/>
                    <a:a name="COLUMN_TOOLTIP" value="SecOCDataId"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              This parameter defines a numerical identifier for the secured PDU.
                            &lt;/p&gt;
                            &lt;p&gt;Range:&lt;/p&gt;
                            &lt;ul&gt;
                              &lt;li&gt;0 .. 255: when Data ID Length (&lt;code&gt;&lt;a href=&quot;path:../../../SecOCEbGeneral/SecOCDataIdLength&quot;&gt;SecOCDataIdLength&lt;/a&gt;&lt;/code&gt;) is set to &lt;code&gt;UINT8&lt;/code&gt;&lt;/li&gt;
                              &lt;li&gt;0 .. 65535: when Data ID Length (&lt;code&gt;&lt;a href=&quot;path:../../../SecOCEbGeneral/SecOCDataIdLength&quot;&gt;SecOCDataIdLength&lt;/a&gt;&lt;/code&gt;) is set to &lt;code&gt;UINT16&lt;/code&gt;&lt;/li&gt;
                              &lt;li&gt;0 .. 4294967295: when Data ID Length (&lt;code&gt;&lt;a href=&quot;path:../../../SecOCEbGeneral/SecOCDataIdLength&quot;&gt;SecOCDataIdLength&lt;/a&gt;&lt;/code&gt;) is set to &lt;code&gt;UINT32&lt;/code&gt;&lt;/li&gt;
                            &lt;/ul&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Data ID"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                    <a:da name="EDITABLE" value="true"/>
                    <a:da name="INVALID" type="Multi">
                      <mt:range>
                        <mt:tst expr="&lt;=4294967295"/>
                        <mt:tst expr="&gt;=0"/>
                      </mt:range>
                      <mt:xpath>
                        <mt:tst 
                                true="SecOCDataId must not be lower then 0 or greater than 255" 
                                expr="((node:value(.) &lt; 0 or node:value(.) &gt; 255) and node:value(../../../SecOCEbGeneral/SecOCDataIdLength) = &apos;UINT8&apos;)"/>
                        <mt:tst 
                                true="SecOCDataId must not be lower then 0 or greater than 65535" 
                                expr="((node:value(.) &lt; 0 or node:value(.) &gt; 65535) and node:value(../../../SecOCEbGeneral/SecOCDataIdLength) = &apos;UINT16&apos;)"/>
                        <mt:tst 
                                true="SecOCDataId must not be lower then 0 or greater than 4294967295" 
                                expr="((node:value(.) &lt; 0 or node:value(.) &gt; 4294967295) and node:value(../../../SecOCEbGeneral/SecOCDataIdLength) = &apos;UINT32&apos;)"/>
                      </mt:xpath>
                    </a:da>
                    <a:da name="WARNING" type="XPath" 
                          expr="text:uniq(../../../*/*/SecOCDataId,.)" 
                          false="The DataId for the secured PDU is not unique                                 in the present configuration."/>
                  </v:var>
                  <v:var name="SecOCFreshnessValueId" type="INTEGER">
                    <a:a name="COLUMN_TITLE" value="Freshness Value ID"/>
                    <a:a name="COLUMN_TOOLTIP" value="SecOCFreshnessValueId"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              This parameter defines the ID of the Freshness value.
                              The Freshness value might be a normal counter or a time value.
                              If Freshness counters are used, the FreshnessValueId with the same
                              value must have the same FreshnessValueLength value.
                            &lt;/p&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Freshness Value ID"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                    <a:da name="EDITABLE" value="true"/>
                    <a:da name="INVALID" type="Range">
                      <a:tst expr="&gt;=0"/>
                      <a:tst expr="&lt;=65535"/>
                    </a:da>
                  </v:var>
                  <v:var name="SecOCFreshnessValueLength" type="INTEGER">
                    <a:a name="COLUMN_TITLE" value="Freshness Value Length"/>
                    <a:a name="COLUMN_TOOLTIP" value="SecOCFreshnessValueLength"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              This parameter defines the complete length in bits of the Freshness Value.
                            &lt;/p&gt;
                            &lt;p&gt;
                              As long as the key doesn&apos;t change the counter shall not overflow.
                              The length of the counter shall be determined based on the expected life
                              time of the corresponding key and frequency of usage of the Freshness
                              Value.
                            &lt;/p&gt;
                            &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                            &lt;ul&gt;
                              &lt;li&gt;Query Freshness Value (&lt;code&gt;&lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/SecOC/ELEMENTS/SecOC/SecOCGeneral/SecOCQueryFreshnessValue&quot;&gt;SecOCQueryFreshnessValue&lt;/a&gt;&lt;/code&gt;): set to something different that NONE in order to be able to set the freshness value length bigger than 0&lt;/li&gt;
                            &lt;/ul&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Freshness Value Length"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                    <a:da name="EDITABLE" value="true"/>
                    <a:da name="INVALID" type="Multi">
                      <mt:range>
                        <mt:tst expr="&lt;=64"/>
                        <mt:tst expr="&gt;=0"/>
                      </mt:range>
                      <mt:xpath 
                                true="When SecOCQueryFreshnessValue is set to &apos;NONE&apos; the freshness value length must be set to 0." 
                                expr="(../../../SecOCGeneral/SecOCQueryFreshnessValue = &apos;NONE&apos;) and (node:value(.) &gt; 0)"/>
                    </a:da>
                  </v:var>
                  <v:var name="SecOCFreshnessValueTxLength" type="INTEGER">
                    <a:a name="COLUMN_TITLE" 
                         value="Freshness Value Truncated Length"/>
                    <a:a name="COLUMN_TOOLTIP" 
                         value="SecOCFreshnessValueTxLength"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                             This parameter defines the length in bits of the freshness value to be
                             extracted/verified from the payload of the secured PDU or cryptographic PDU.
                            &lt;/p&gt;
                            &lt;p&gt;
                             This length is specificing the amount of least significant bits that
                             shall be used from the complete freshness value.
                            &lt;/p&gt;
                            &lt;p&gt;
                             If the parameter is 0 no freshness value shall be included in the secured PDU or cryptographic PDU.
                            &lt;/p&gt;
                            &lt;p&gt;Range:&lt;/p&gt;
                            &lt;ul&gt;
                              &lt;li&gt;0 .. &lt;code&gt;&lt;a href=&quot;path:../SecOCFreshnessValueLength&quot;&gt;Freshness Value Length&lt;/a&gt;&lt;/code&gt;&lt;/li&gt;
                            &lt;/ul&gt;
                            &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                            &lt;ul&gt;
                              &lt;li&gt;Freshness Value Length (&lt;code&gt;&lt;a href=&quot;path:../SecOCFreshnessValueLength&quot;&gt;SecOCFreshnessValueLength&lt;/a&gt;&lt;/code&gt;): limits the range selection, the truncated freshness value cannot be bigger than the complete freshness value&lt;/li&gt;
                              &lt;li&gt;Query Freshness Value (&lt;code&gt;&lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/SecOC/ELEMENTS/SecOC/SecOCGeneral/SecOCQueryFreshnessValue&quot;&gt;SecOCQueryFreshnessValue&lt;/a&gt;&lt;/code&gt;): set to something different that NONE in order to be able to set the freshness value length bigger than 0&lt;/li&gt;
                            &lt;/ul&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Freshness Value Truncated Length"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                    <a:da name="EDITABLE" value="true"/>
                    <a:da name="INVALID" type="Multi">
                      <mt:range>
                        <mt:tst expr="&lt;=64"/>
                        <mt:tst expr="&gt;=0"/>
                      </mt:range>
                      <mt:xpath>
                        <mt:tst 
                                true="The truncated freshness value length must not be greater than freshness value length." 
                                expr="(. &gt; ../SecOCFreshnessValueLength )"/>
                        <mt:tst 
                                true="When SecOCQueryFreshnessValue is set to &apos;NONE&apos; the truncated freshness value length must be set to 0." 
                                expr="(../../../SecOCGeneral/SecOCQueryFreshnessValue = &apos;NONE&apos;) and (node:value(.) &gt; 0)"/>
                      </mt:xpath>
                    </a:da>
                  </v:var>
                  <v:var name="SecOCFreshnessValueFuncName" 
                         type="FUNCTION-NAME">
                    <a:a name="COLUMN_TITLE" 
                         value="Freshness Value Function Name"/>
                    <a:a name="COLUMN_TOOLTIP" 
                         value="SecOCFreshnessValueFuncName"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              This parameter specifies the name of the C API function which shall be
                              called to query the freshness for the current PDU.
                            &lt;/p&gt;
                            &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                            &lt;ul&gt;
                              &lt;li&gt;Query Freshness Value (&lt;code&gt;&lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/SecOC/ELEMENTS/SecOC/SecOCGeneral/SecOCQueryFreshnessValue&quot;&gt;SecOCQueryFreshnessValue&lt;/a&gt;&lt;/code&gt;): to be able to configure the name of the C API function, query must be set on &lt;code&gt;CFUNC&lt;/code&gt;&lt;/li&gt;
                            &lt;/ul&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v class="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Freshness Value Function Name"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:da name="ENABLE" type="XPath" 
                          expr="(../../../SecOCGeneral/SecOCQueryFreshnessValue = &apos;CFUNC&apos;)"/>
                    <a:da name="INVALID" type="XPath" 
                          expr="(. != &apos;&apos;) and text:match(., &apos;^[a-zA-Z][a-zA-Z0-9_]*$&apos;)" 
                          false="Missing or invalid C function name."/>
                  </v:var>
                  <v:var name="SecOCAuthenticationBuildAttempts" type="INTEGER">
                    <a:a name="COLUMN_TITLE" 
                         value="Authentication Build Attempts"/>
                    <a:a name="COLUMN_TOOLTIP" 
                         value="SecOCAuthenticationBuildAttempts"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              This parameter defines the number of authentication build attempts
                              when a verification failed because the freshness value could not
                              be obtained or the verification of the authenticator could not be
                              performed.
                           &lt;/p&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Authentication Build Attempts"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:da name="DEFAULT" value="0"/>
                    <a:da name="EDITABLE" value="true"/>
                    <a:da name="INVALID" type="Range">
                      <a:tst expr="&gt;=0"/>
                      <a:tst expr="&lt;=65535"/>
                    </a:da>
                  </v:var>
                  <v:var name="SecOCAuthenticationVerifyAttempts" 
                         type="INTEGER">
                    <a:a name="COLUMN_TITLE" 
                         value="Authentication Verify Attempts"/>
                    <a:a name="COLUMN_TOOLTIP" 
                         value="SecOCAuthenticationVerifyAttempts"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              This parameter specifies the number of authentication verify attempts
                              that are to be carried out when the verification of the authentication
                              information failed for a given secured PDU. If zero is set,
                              then only one authentication verification attempt is done.
                            &lt;/p&gt;
                            &lt;p&gt;
                              If the freshness value length is 0 and the MAC verification was executed,
                              but the result was invalid MAC, no additional verification attempt will be executed.
                            &lt;/p&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Authentication Verify Attempts"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:da name="DEFAULT" value="0"/>
                    <a:da name="EDITABLE" value="true"/>
                    <a:da name="INVALID" type="Range">
                      <a:tst expr="&gt;=0"/>
                      <a:tst expr="&lt;=65535"/>
                    </a:da>
                  </v:var>
                  <v:var name="SecOCVerificationStatusPropagationMode" 
                         type="ENUMERATION">
                    <a:a name="COLUMN_TITLE" 
                         value="Sender Receiver Verification Status Propagation Mode"/>
                    <a:a name="COLUMN_TOOLTIP" 
                         value="SecOCVerificationStatusPropagationMode"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              This parameter is used to describe the propagation of the status of
                              each verification attempt from the SecOC module to the application
                              via callouts defined in &lt;code&gt;&lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/SecOC/ELEMENTS/SecOC/SecOCGeneral/SecOCVerificationStatusCallout&quot;&gt;SecOCVerificationStatusCallout&lt;/a&gt;&lt;/code&gt; or
                              via RTE sender receiver interface.
                            &lt;/p&gt;
                            &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                            &lt;ul&gt;
                              &lt;li&gt;Enable Rte Usage (&lt;code&gt;&lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/SecOC/ELEMENTS/SecOC/SecOCEbGeneral/SecOCRteUsage&quot;&gt;SecOCRteUsage&lt;/a&gt;&lt;/code&gt;): in order to propagate the verification status via RTE&lt;/li&gt;
                              &lt;li&gt;Verification Status Callout Functions (&lt;code&gt;&lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/SecOC/ELEMENTS/SecOC/SecOCGeneral/SecOCVerificationStatusCallout&quot;&gt;SecOCVerificationStatusCallout&lt;/a&gt;&lt;/code&gt;): in order to propagate the verification status via C functions&lt;/li&gt;
                              &lt;li&gt;Verify Status Override API Version (&lt;code&gt;&lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/SecOC/ELEMENTS/SecOC/SecOCEbGeneral/SecOCEbPropagateVerificationStatusApiVersion&quot;&gt;SecOCEbPropagateVerificationStatusApiVersion&lt;/a&gt;&lt;/code&gt;): must not be set to NONE&lt;/li&gt;
                            &lt;/ul&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" 
                         value="Sender Receiver Verification Status Propagation Mode"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                    <a:da name="DEFAULT" value="NONE"/>
                    <a:da name="EDITABLE" value="true"/>
                    <a:da name="INVALID" type="XPath" 
                          expr="node:value(.) != &apos;NONE&apos; and node:value(../../../SecOCEbGeneral/SecOCEbPropagateVerificationStatusApiVersion) = &apos;NONE&apos; and                                    num:i(count(../../../SecOCGeneral/SecOCVerificationStatusCallout/*)) = 0 and node:value(../../../SecOCEbGeneral/SecOCRteUsage) = &apos;false&apos;" 
                          true="To propagate the verification status SecOCEbPropagateVerificationStatusApiVersion must be different from NONE and                                    the RTE usage must be enabled or at least one callout function must be configured in the VerificationStatus Callout container."/>
                    <a:da name="RANGE">
                      <a:v>NONE</a:v>
                      <a:v>FAILURE_ONLY</a:v>
                      <a:v>BOTH</a:v>
                    </a:da>
                  </v:var>
                  <v:var 
                         name="SecOCClientServerVerificationStatusPropagationMode" 
                         type="ENUMERATION">
                    <a:a name="COLUMN_TITLE" 
                         value="Client Server Verification Status Propagation Mode"/>
                    <a:a name="COLUMN_TOOLTIP" 
                         value="SecOCClientServerVerificationStatusPropagationMode"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              This parameter is used to describe the propagation of the status of
                              each verification attempt from the SecOC module to the application
                              via RTE client server interface.
                            &lt;/p&gt;
                            &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                            &lt;ul&gt;
                              &lt;li&gt;Enable Rte Usage (&lt;code&gt;&lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/SecOC/ELEMENTS/SecOC/SecOCEbGeneral/SecOCRteUsage&quot;&gt;SecOCRteUsage&lt;/a&gt;&lt;/code&gt;): in order to propagate the verification status via RTE&lt;/li&gt;
                              &lt;li&gt;Verify Status Override API Version (&lt;code&gt;&lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/SecOC/ELEMENTS/SecOC/SecOCEbGeneral/SecOCEbClientServerPropagateVerificationStatusApiVersion&quot;&gt;SecOCEbClientServerPropagateVerificationStatusApiVersion&lt;/a&gt;&lt;/code&gt;): must not be set to NONE&lt;/li&gt;
                            &lt;/ul&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" 
                         value="Client Server Verification Status Propagation Mode"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                    <a:da name="DEFAULT" value="NONE"/>
                    <a:da name="EDITABLE" value="true"/>
                    <a:da name="INVALID" type="XPath" 
                          expr="node:value(.) != &apos;NONE&apos; and node:value(../../../SecOCEbGeneral/SecOCEbClientServerPropagateVerificationStatusApiVersion) = &apos;NONE&apos;" 
                          true="To propagate the verification status SecOCEbClientServerPropagateVerificationStatusApiVersion must be different from NONE."/>
                    <a:da name="RANGE">
                      <a:v>NONE</a:v>
                      <a:v>FAILURE_ONLY</a:v>
                      <a:v>BOTH</a:v>
                    </a:da>
                  </v:var>
                  <v:ref name="SecOCSameBufferPduRef" type="REFERENCE">
                    <a:a name="COLUMN_TITLE" value="Same PDU Buffer Reference"/>
                    <a:a name="COLUMN_TOOLTIP" value="SecOCSameBufferPduRef"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              This reference is used to collect PDUs that are using the same
                              SecOC buffer.
                            &lt;/p&gt;
                            &lt;p&gt;
                             The referenced buffer must be used only by Rx PDU(s).
                            &lt;/p&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v class="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Same PDU Buffer Reference"/>
                    <a:a name="OPTIONAL" value="true"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:da name="EDITABLE" value="true"/>
                    <a:da name="ENABLE" value="true"/>
                    <a:da name="INVALID" type="XPath">
                      <a:tst expr="(node:refvalid(.))" 
                             false="The expression argument reference is not valid."/>
                      <a:tst 
                             expr="(../SecOCRxSecuredPduLayer = &apos;SecOCRxSecuredPduCollection&apos;) and                                    ((node:value(as:ref(../SecOCRxSecuredPduLayer/SecOCRxAuthenticPdu/SecOCRxAuthenticPduRef)/PduLength) + node:value(as:ref(../SecOCRxSecuredPduLayer/SecOCRxCryptographicPdu/SecOCRxCryptographicPduRef)/PduLength))                                     &gt; node:value(as:ref(.)/SecOCBufferLength))" 
                             true=" The size of the references SecOCSameBufferPduCollection must be                                      at least equal with the combined length of the lower layer authentic PDU                                      and cryptographic PDU."/>
                      <a:tst 
                             expr="(../SecOCRxSecuredPduLayer = &apos;SecOCRxSecuredPdu&apos;) and                                    (node:value(as:ref(../SecOCRxSecuredPduLayer/SecOCRxSecuredLayerPduRef)/PduLength) &gt; node:value(as:ref(.)/SecOCBufferLength))" 
                             true=" The size of the references SecOCSameBufferPduCollection must be                                      at least equal with the length of the secured PDU."/>
                      <a:tst 
                             expr="node:containsValue(../../../SecOCTxPduProcessing/*[node:exists(./SecOCSameBufferPduRef) = true()]/SecOCSameBufferPduRef,.)" 
                             true=" The selected buffer is already being used by the Tx PDUs."/>
                      <a:tst 
                             expr="node:value(../SecOCReceptionOverflowStrategy) = &apos;REPLACE&apos;" 
                             true="Same buffer cannot be used together with SecOCReceptionOverflowStrategy set to REPLACE."/>
                      <a:tst 
                             expr="node:value(../SecOCRxAuthenticPduLayer/SecOCPduType) = &apos;SECOC_TPPDU&apos;" 
                             true="Same buffer cannot be used together with SecOCPduType set to SECOC_TPPDU."/>
                    </a:da>
                    <a:da name="REF" 
                          value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/SecOC/SecOCSameBufferPduCollection"/>
                  </v:ref>
                  <v:var name="SecOCUseAuthDataFreshness" type="BOOLEAN">
                    <a:a name="COLUMN_TITLE" 
                         value="Enable Authentic Data To Freshness SWC"/>
                    <a:a name="COLUMN_TOOLTIP" value="SecOCUseAuthDataFreshness"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              This parameter indicates if a part of the authentic data from
                              the secured PDU shall be passed on to the SWC that verifies and
                              generates the freshness value.
                              If it is set to &lt;code&gt;TRUE&lt;/code&gt;, the values
                              &lt;code&gt;SecOCAuthDataFreshnessStartPosition&lt;/code&gt; and
                              &lt;code&gt;SecOCAuthDataFreshnessLen&lt;/code&gt; must be set to specify the
                              bit start position and length within the secured PDU.
                            &lt;/p&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" 
                         value="Enable Authentic Data To Freshness SWC"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:da name="DEFAULT" value="false"/>
                    <a:da name="ENABLE" value="true"/>
                  </v:var>
                  <v:var name="SecOCAuthDataFreshnessLen" type="INTEGER">
                    <a:a name="COLUMN_TITLE" 
                         value="Authentic Data Freshness Length"/>
                    <a:a name="COLUMN_TOOLTIP" value="SecOCAuthDataFreshnessLen"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              This parameter defines the length in bits the authentic data part
                              from the secured PDU that will be passed on to the Freshness SWC.
                            &lt;/p&gt;
                            &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                            &lt;ul&gt;
                              &lt;li&gt;Enable Authentic Data To Freshness SWC (&lt;code&gt;&lt;a href=&quot;path:../SecOCAuthDataFreshnessLen&quot;&gt;SecOCAuthDataFreshnessLen&lt;/a&gt;&lt;/code&gt;): must be enabled in order to use this feature&lt;/li&gt;
                            &lt;/ul&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v class="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Authentic Data Freshness Length"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:da name="ENABLE" type="XPath" 
                          expr="../SecOCUseAuthDataFreshness = &apos;true&apos;"/>
                    <a:da name="INVALID" type="Range">
                      <a:tst expr="&gt;=0"/>
                      <a:tst expr="&lt;=64"/>
                    </a:da>
                  </v:var>
                  <v:var name="SecOCAuthDataFreshnessStartPosition" 
                         type="INTEGER">
                    <a:a name="COLUMN_TITLE" 
                         value="Authentic Data Freshness Start Position"/>
                    <a:a name="COLUMN_TOOLTIP" 
                         value="SecOCAuthDataFreshnessStartPosition"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              This parameter defines the start position in bits (uint16) of the
                              authentic data part from the secured PDU that shall be passed
                              on to the Freshness SWC.
                              The bit position starts counting from the MSB of the first byte of
                              the PDU.
                            &lt;/p&gt;
                            &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                            &lt;ul&gt;
                              &lt;li&gt;Enable Authentic Data To Freshness SWC (&lt;code&gt;&lt;a href=&quot;path:../SecOCAuthDataFreshnessLen&quot;&gt;SecOCAuthDataFreshnessLen&lt;/a&gt;&lt;/code&gt;): must be enabled in order to use this feature&lt;/li&gt;
                            &lt;/ul&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v class="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" 
                         value="Authentic Data Freshness Start Position"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:da name="ENABLE" type="XPath" 
                          expr="../SecOCUseAuthDataFreshness = &apos;true&apos;"/>
                    <a:da name="INVALID" type="Range">
                      <a:tst expr="&gt;=0"/>
                      <a:tst expr="&lt;=65535"/>
                    </a:da>
                  </v:var>
                  <v:var name="SecOCReceptionOverflowStrategy" 
                         type="ENUMERATION">
                    <a:a name="COLUMN_TITLE" value="Reception Overflow Strategy"/>
                    <a:a name="COLUMN_TOOLTIP" 
                         value="SecOCReceptionOverflowStrategy"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              This parameter specifies the overflow strategy for receiving PDUs.
                            &lt;/p&gt;

                            &lt;p&gt;Range:&lt;/p&gt;
                            &lt;ul&gt;
                              &lt;li&gt;&lt;code&gt;QUEUE&lt;/code&gt;: Subsequent received message will be queued.&lt;/li&gt;
                              &lt;li&gt;&lt;code&gt;REJECT&lt;/code&gt;: Subsequent received message will be discarded&lt;/li&gt;
                              &lt;li&gt;&lt;code&gt;REPLACE&lt;/code&gt;: Subsequent received message will replace the
                                  currently processed message&lt;/li&gt;
                            &lt;/ul&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Reception Overflow Strategy"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:da name="DEFAULT" value="REJECT"/>
                    <a:da name="EDITABLE" value="true"/>
                    <a:da name="ENABLE" value="true"/>
                    <a:da name="INVALID" type="XPath">
                      <a:tst 
                             expr="node:value(.) = &apos;QUEUE&apos; and (../SecOCRxSecuredPduLayer = &apos;SecOCRxSecuredPduCollection&apos;)" 
                             true="When SecOCReceptionOverflowStrategy is set to &apos;QUEUE&apos;, PDU collection cannot be used."/>
                      <a:tst 
                             expr="node:value(.) = &apos;QUEUE&apos; and node:exists(../SecOCSameBufferPduRef) = true()" 
                             true="When SecOCReceptionOverflowStrategy is set to &apos;QUEUE&apos;, PDU cannot share a buffer"/>
                    </a:da>
                    <a:da name="RANGE">
                      <a:v>REJECT</a:v>
                      <a:v>REPLACE</a:v>
                      <a:v>QUEUE</a:v>
                    </a:da>
                  </v:var>
                  <v:var name="SecOCReceptionQueueSize" type="INTEGER">
                    <a:a name="COLUMN_TITLE" value="Reception Queue Size"/>
                    <a:a name="COLUMN_TOOLTIP" value="SecOCReceptionQueueSize"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              This parameter defines the queue size in case the overflow strategy
                              for receiving PDUs is set to QUEUE.
                            &lt;/p&gt;
                            &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                            &lt;ul&gt;
                              &lt;li&gt;Reception Overflow Strategy (&lt;code&gt;&lt;a href=&quot;path:../SecOCReceptionOverflowStrategy&quot;&gt;SecOCReceptionOverflowStrategy&lt;/a&gt;&lt;/code&gt;): the &lt;code&gt;QUEUE&lt;/code&gt; option must be selected in order to be able to set the queue size&lt;/li&gt;
                            &lt;/ul&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v class="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Reception Queue Size"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:da name="DEFAULT" value="1"/>
                    <a:da name="ENABLE" type="XPath" 
                          expr="node:value(../SecOCReceptionOverflowStrategy) = &apos;QUEUE&apos;"/>
                    <a:da name="INVALID" type="Range">
                      <a:tst expr="&gt;=1"/>
                      <a:tst expr="&lt;=65535"/>
                    </a:da>
                  </v:var>
                  <v:var name="SecOCEnableMetaData" type="BOOLEAN">
                    <a:a name="COLUMN_TITLE" value="Enable Meta Data Usage"/>
                    <a:a name="COLUMN_TOOLTIP" value="SecOCEnableMetaData"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              Enables the use of Meta Data transfer from the secured PDU to the authentic PDU.
                            &lt;/p&gt;
                            &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                            &lt;ul&gt;
                              &lt;li&gt;Enable Meta Data Usage (&lt;code&gt;&lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/SecOC/ELEMENTS/SecOC/SecOCEbGeneral/SecOCEnableMetaDataUse&quot;&gt;SecOCEnableMetaDataUse&lt;/a&gt;&lt;/code&gt;): must be enabled in order to use this feature&lt;/li&gt;
                            &lt;/ul&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v class="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Enable Meta Data Usage"/>
                    <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                    <a:da name="DEFAULT" value="false"/>
                    <a:da name="ENABLE" type="XPath" 
                          expr="node:value(../../../SecOCEbGeneral/SecOCEnableMetaDataUse) = &apos;true&apos;"/>
                    <a:da name="INVALID" type="XPath">
                      <a:tst 
                             expr="node:value(.) = &apos;true&apos; and node:value(../SecOCRxSecuredPduLayer) = &apos;SecOCRxSecuredPduCollection&apos;" 
                             true="MetaData can not be used for PDU Collection."/>
                      <a:tst 
                             expr="node:value(.) = &apos;true&apos; and node:value(../SecOCReceptionOverflowStrategy) != &apos;QUEUE&apos;" 
                             true="In order to use the MetaData for a given PDU the parameter SecOCReceptionOverflowStrategy must be set to QUEUE."/>
                      <a:tst 
                             expr="node:value(.) = &apos;true&apos; and node:exists(node:refs(../SecOCRxSecuredPduLayer/SecOCRxSecuredLayerPduRef)/MetaDataTypeRef) = false()" 
                             true="In order to acquire the MetaData from the EcuC the MetaDataTypeRef for that PDU must be enabled."/>
                      <a:tst 
                             expr="node:value(.) = &apos;true&apos; and node:exists(node:refs(../SecOCRxSecuredPduLayer/SecOCRxSecuredLayerPduRef)/PduId) = false()" 
                             true="In order to acquire the MetaData from the EcuC the PduId of an EcuC PduId must be enabled."/>
                      <a:tst 
                             expr="node:value(.) = &apos;true&apos; and node:exists(node:refs(../SecOCRxAuthenticPduLayer/SecOCRxAuthenticLayerPduRef)/MetaDataTypeRef) = false()" 
                             true="In order to transmit the MetaData from the EcuC the MetaDataTypeRef for that PDU must be enabled."/>
                      <a:tst 
                             expr="node:value(.) = &apos;true&apos; and node:exists(node:refs(../SecOCRxAuthenticPduLayer/SecOCRxAuthenticLayerPduRef)/PduId) = false()" 
                             true="In order to transmit the MetaData from the EcuC the PduId of an EcuC PduId must be enabled."/>
                      <a:tst 
                             expr="node:value(.) = &apos;true&apos; and (node:value(node:refs(../SecOCRxSecuredPduLayer/SecOCRxSecuredLayerPduRef)/MetaDataTypeRef) = node:value(node:refs(../SecOCRxAuthenticPduLayer/SecOCRxAuthenticLayerPduRef)/MetaDataTypeRef))" 
                             true="The secured and authentic PDU cant reference the same MetaDataTypeRef ."/>
                    </a:da>
                  </v:var>
                  <v:var name="SecOCRxUseShapeFunc" type="BOOLEAN">
                    <a:a name="COLUMN_TITLE" 
                         value="Enable Rx Shape Function Usage"/>
                    <a:a name="COLUMN_TOOLTIP" value="SecOCRxUseShapeFunc"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              This parameter indicates, whether the layout shaping functionality
                              its enabled or not for this PDU.
                            &lt;/p&gt;
                            &lt;p&gt;
                              By enabling this parameter, the layout of the secured PDU can be updated by the SecOC callout function
                              which name is configured in the
                              &lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/SecOC/ELEMENTS/SecOC/SecOCEbGeneral/SecOCRxShapeFuncName&quot;&gt;SecOCRxShapeFuncName&lt;/a&gt; parameter.
                            &lt;/p&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Enable Rx Shape Function Usage"/>
                    <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                    <a:da name="DEFAULT" value="false"/>
                    <a:da name="INVALID" type="XPath">
                      <a:tst 
                             expr="node:value(.) = &apos;true&apos; and node:exists(../../../SecOCEbGeneral/SecOCRxShapeFuncName) = false()" 
                             true="The callout function to shape the secured PDU layout is enabled but the name of the callout function is not configured."/>
                      <a:tst 
                             expr="node:value(.) = &apos;true&apos; and node:value(../SecOCRxAuthenticPduLayer/SecOCPduType) = &apos;SECOC_TPPDU&apos;" 
                             true="The fix layout of the Rx secured PDU can be used only with direct transmission (IF)."/>
                    </a:da>
                  </v:var>
                  <v:var name="SecOCRxSyncPduProcessing" type="BOOLEAN">
                    <a:a name="COLUMN_TITLE" 
                         value="Enable Synchronous PDU Processing"/>
                    <a:a name="COLUMN_TOOLTIP" value="SecOCRxSyncPduProcessing"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              This parameter indicates whether the PDU is processed synchronously,
                              i.e. the PDU is processed directly (within the PduR call) without waiting the main function.
                            &lt;/p&gt;
                            &lt;p&gt;
                              Note: Manually calling the main function has no effect since the PDU
                              processing is done within the PduR calls of SecOC interface (i.e. SecOC_RxIndication).
                            &lt;/p&gt;
                            &lt;p&gt;
                              Synchronous PDU processing cannot be combined with asynchronous Csm Mode.
                            &lt;/p&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Enable Synchronous PDU Processing"/>
                    <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                    <a:da name="DEFAULT" value="false"/>
                    <a:da name="ENABLE" value="true"/>
                    <a:da name="INVALID" type="XPath" 
                          expr="node:value(../SecOCRxSecuredPduLayer/SecOCSecuredRxPduVerification) = &apos;true&apos; and node:value(.) = &apos;true&apos; and                                    ((node:exists(as:ref(as:ref(../SecOCRxAuthServiceConfigRef)/CsmJobPrimitiveRef)/CsmMacVerify) = &apos;true&apos; and node:value(as:ref(as:ref(../SecOCRxAuthServiceConfigRef)/CsmJobPrimitiveRef)/CsmMacVerify/CsmMacVerifyConfig/CsmMacVerifyProcessing) = &apos;CSM_ASYNCHRONOUS&apos;) or                                    (node:exists(as:ref(as:ref(../SecOCRxAuthServiceConfigRef)/CsmJobPrimitiveRef)/CsmSignatureVerify) = &apos;true&apos; and node:value(as:ref(as:ref(../SecOCRxAuthServiceConfigRef)/CsmJobPrimitiveRef)/CsmSignatureVerify/CsmSignatureVerifyConfig/CsmSignatureVerifyProcessing) = &apos;CSM_ASYNCHRONOUS&apos;))" 
                          true="Synchronous PDU processing cannot be combined with asynchronous Csm mode"/>
                    <a:da name="WARNING" type="XPath">
                      <a:tst 
                             expr="node:value(.) = &apos;true&apos; and node:value(../SecOCAuthenticationBuildAttempts) &gt; 0" 
                             true="It&apos;s highly recommended to set SecOCAuthenticationBuildAttempts to zero when using synchronous PDU processing,                          since otherwise calls are blocking for the duration of retries."/>
                      <a:tst 
                             expr="node:value(.) = &apos;true&apos; and node:value(../SecOCAuthenticationVerifyAttempts) &gt; 0" 
                             true="It&apos;s highly recommended to set SecOCAuthenticationVerifyAttempts to zero when using synchronous PDU processing,                          since otherwise calls are blocking for the duration of retries."/>
                    </a:da>
                  </v:var>
                  <v:chc name="SecOCRxSecuredPduLayer" type="IDENTIFIABLE">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              This container specifies the PDU that is received by the SecOC
                              module from the PduR.
                            &lt;/p&gt;
                            &lt;p&gt;
                              There are two available possibilities to receive the data from the lower layer:
                            &lt;/p&gt;
                            &lt;ul&gt;
                              &lt;li&gt;
                                SecOCRxSecuredPdu - the whole content will be received within a PDU (secured PDU)
                              &lt;/li&gt;
                              &lt;li&gt;
                                SecOCRxSecuredPduCollection - the whole content will be received with two
                                PDUs, the authentic PDU (which contains the authentic data) and
                                the cryptographic PDU (which contains the cryptographic information
                                like MAC etc)
                              &lt;/li&gt;
                            &lt;/ul&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="FRAME" value="TITLE"/>
                    <a:a name="LABEL" value="Secured PDU Layer"/>
                    <v:ctr name="SecOCRxSecuredPdu" type="IDENTIFIABLE">
                      <a:a name="DESC">
                        <a:v>&lt;html&gt;
                            &lt;p&gt;
                              This container specifies the secured PDU that is received by the SecOC
                              module from the PduR.
                            &lt;/p&gt;
                            &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                            &lt;ul&gt;
                              &lt;li&gt;The secured PDU (&lt;code&gt;&lt;a href=&quot;path:./SecOCRxSecuredLayerPduRef&quot;&gt;SecOCRxSecuredLayerPduRef&lt;/a&gt;&lt;/code&gt;) length must be equal with the combined length
                              of the secure PDU header (&lt;code&gt;&lt;a href=&quot;path:./SecOCAuthPduHeaderLength&quot;&gt;SecOCAuthPduHeaderLength&lt;/a&gt;&lt;/code&gt;),
                              the authentic PDU (&lt;code&gt;&lt;a href=&quot;path:../SecOCRxAuthenticPduLayer/SecOCRxAuthenticLayerPduRef&quot;&gt;SecOCRxAuthenticLayerPduRef&lt;/a&gt;&lt;/code&gt;),
                              the truncated freshness value (&lt;code&gt;&lt;a href=&quot;path:../SecOCFreshnessValueTxLength&quot;&gt;SecOCFreshnessValueTxLength&lt;/a&gt;&lt;/code&gt;) and
                              the truncated authenticator (&lt;code&gt;&lt;a href=&quot;path:../SecOCAuthInfoTxLength&quot;&gt;SecOCAuthInfoTxLength&lt;/a&gt;&lt;/code&gt;), applicable when the secured PDU header is being used&lt;/li&gt;
                              &lt;li&gt;OR&lt;/li&gt;
                              &lt;li&gt;The secured PDU (&lt;code&gt;&lt;a href=&quot;path:./SecOCRxSecuredLayerPduRef&quot;&gt;SecOCRxSecuredLayerPduRef&lt;/a&gt;&lt;/code&gt;) length must be equal with the combined length
                              of the authentic PDU (&lt;code&gt;&lt;a href=&quot;path:../SecOCRxAuthenticPduLayer/SecOCRxAuthenticLayerPduRef&quot;&gt;SecOCRxAuthenticLayerPduRef&lt;/a&gt;&lt;/code&gt;),
                              the truncated freshness value (&lt;code&gt;&lt;a href=&quot;path:../SecOCFreshnessValueTxLength&quot;&gt;SecOCFreshnessValueTxLength&lt;/a&gt;&lt;/code&gt;) and
                              the truncated authenticator (&lt;code&gt;&lt;a href=&quot;path:../SecOCAuthInfoTxLength&quot;&gt;SecOCAuthInfoTxLength&lt;/a&gt;&lt;/code&gt;), applicable when the secured PDU header is not being used&lt;/li&gt;
                            &lt;/ul&gt;
                            &lt;p&gt;Note: Take into account that when calculating the combined length some configuration parameters hold the length in bytes and some in bits.&lt;/p&gt;
                          &lt;/html&gt;</a:v>
                      </a:a>
                      <a:a name="LABEL" value="Secured PDU"/>
                      <a:da name="INVALID" type="XPath">
                        <a:tst 
                               expr="node:exists(./SecOCAuthPduHeaderLength) and                                      ((node:value(./SecOCAuthPduHeaderLength) + node:value(node:ref(../SecOCRxAuthenticPduLayer/SecOCRxAuthenticLayerPduRef)/PduLength) + ceiling((node:value(../SecOCFreshnessValueTxLength) + node:value(../SecOCAuthInfoTxLength)) div 8)) !=                                       node:value(node:ref(./SecOCRxSecuredLayerPduRef)/PduLength))" 
                               true="Rx secured PDU length must be equal with the combined length of the secure PDU Header, authentic PDU, truncated freshness value and MAC"/>
                        <a:tst 
                               expr="not(node:exists(./SecOCAuthPduHeaderLength)) and                                      ((node:value(node:ref(../SecOCRxAuthenticPduLayer/SecOCRxAuthenticLayerPduRef)/PduLength) + ceiling((node:value(../SecOCFreshnessValueTxLength) + node:value(../SecOCAuthInfoTxLength)) div 8)) !=                                      node:value(node:ref(./SecOCRxSecuredLayerPduRef)/PduLength))" 
                               true="Rx secured PDU length must be equal with the combined length of the authentic PDU, truncated freshness value and MAC"/>
                      </a:da>
                      <v:var name="SecOCAuthPduHeaderLength" type="INTEGER">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                                &lt;p&gt;
                                  This parameter indicates the length (in bytes) of the secured PDU
                                  header in the secured PDU. The length of zero means there&apos;s no header
                                  in the PDU.
                                &lt;/p&gt;
                              &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="PreCompile">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="LABEL" value="Secured PDU Header Length"/>
                        <a:a name="OPTIONAL" value="true"/>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:da name="DEFAULT" value="0"/>
                        <a:da name="ENABLE" value="false"/>
                        <a:da name="INVALID" type="Multi">
                          <mt:range>
                            <mt:tst expr="&lt;=4"/>
                            <mt:tst expr="&gt;=0"/>
                          </mt:range>
                          <mt:xpath 
                                    false="The length of secured PDU header must be smaller than Rx authentic PDU." 
                                    expr="node:value(node:ref(../../SecOCRxAuthenticPduLayer/SecOCRxAuthenticLayerPduRef)/PduLength) &gt; node:value(.)"/>
                        </a:da>
                      </v:var>
                      <v:var name="SecOCRxSecuredLayerPduId" type="INTEGER">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                                &lt;p&gt;
                                  PDU identifier assigned by SecureOnboardCommunication module.
                                  Used by PduR for &lt;code&gt;SecOC_PduRRxIndication&lt;/code&gt;.
                                &lt;/p&gt;

                                &lt;p&gt;Note: The Handle-Id Wizard can be used to set this value automatically.&lt;/p&gt;
                              &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="LABEL" value="Secured PDU ID"/>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="true"/>
                        <a:da name="INVALID" type="Multi">
                          <mt:range>
                            <mt:tst expr="&gt;=0"/>
                            <mt:tst expr="&lt;=65535"/>
                          </mt:range>
                        </a:da>
                      </v:var>
                      <v:var name="SecOCSecuredRxPduVerification" 
                             type="BOOLEAN">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                                &lt;p&gt;
                                  This parameter defines whether the MAC verification shall be
                                  performed on this secured PDU. If set to false, the SecOC module
                                  extracts the authentic PDU from the secured PDU without
                                  verification.
                                &lt;/p&gt;
                              &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="LABEL" value="Enable PDU Verification"/>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:da name="DEFAULT" value="true"/>
                        <a:da name="EDITABLE" value="true"/>
                      </v:var>
                      <v:ref name="SecOCRxSecuredLayerPduRef" type="REFERENCE">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                                &lt;p&gt;
                                  Reference to the global PDU holding a secured PDU, which shall be
                                  verified by the SecOC module.
                                &lt;/p&gt;
                              &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="LABEL" value="Secured PDU Reference"/>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                        <a:da name="INVALID" type="XPath" 
                              expr="(node:refvalid(.))" 
                              false="The expression argument reference is not valid."/>
                        <a:da name="REF" 
                              value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/EcuC/EcucPduCollection/Pdu"/>
                      </v:ref>
                    </v:ctr>
                    <v:ctr name="SecOCRxSecuredPduCollection" 
                           type="IDENTIFIABLE">
                      <a:a name="DESC">
                        <a:v>&lt;html&gt;
                              &lt;p&gt;
                                This container specifies two PDUs that are received by the SecOC module
                                from the PduR and a message linking between them.
                              &lt;/p&gt;
                              &lt;p&gt;
                                &lt;a href=&quot;path:./SecOCRxAuthenticPdu&quot;&gt;SecOCRxAuthenticPdu&lt;/a&gt; contains the original authentic PDU, i.e. the
                                secured data, and the &lt;a href=&quot;path:./SecOCRxCryptographicPdu&quot;&gt;SecOCRxCryptographicPdu&lt;/a&gt; contains the
                                freshness value, authenticator and message link, i.e. the actual authentication information.
                              &lt;/p&gt;
                            &lt;/html&gt;</a:v>
                      </a:a>
                      <a:a name="LABEL" value="Secured Pdu Collection"/>
                      <v:var name="SecOCSecuredRxPduVerification" 
                             type="BOOLEAN">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                                &lt;p&gt;
                                  This parameter defines whether the MAC verification shall be
                                  performed on this secured PDU. If set to false, the SecOC module
                                  extracts the authentic PDU from the secured PDU without
                                  verification.
                                &lt;/p&gt;
                              &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="LABEL" value="Enable PDU Verification"/>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:da name="DEFAULT" value="true"/>
                        <a:da name="EDITABLE" value="true"/>
                      </v:var>
                      <v:ctr name="SecOCRxAuthenticPdu" type="IDENTIFIABLE">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                                &lt;p&gt;
                                  This container specifies the PDU that is received by the SecOC
                                  module from the lower layer, which contains the authentic data
                                  that will form with the corresponding cryptographic PDU
                                  the secured PDU.
                                &lt;/p&gt;
                                &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                                &lt;ul&gt;
                                  &lt;li&gt;The length of the authentic PDU (&lt;code&gt;&lt;a href=&quot;path:./SecOCRxAuthenticPduRef&quot;&gt;SecOCRxAuthenticPduRef&lt;/a&gt;&lt;/code&gt;)
                                  received from the lower layer must have the same length with the authentic PDU (&lt;code&gt;&lt;a href=&quot;path:../../SecOCRxAuthenticPduLayer/SecOCRxAuthenticLayerPduRef&quot;&gt;SecOCRxAuthenticLayerPduRef&lt;/a&gt;&lt;/code&gt;)
                                  that needs to be send to the upper layer plus
                                  the secured header length (&lt;code&gt;&lt;a href=&quot;path:./SecOCAuthPduHeaderLength&quot;&gt;SecOCAuthPduHeaderLength&lt;/a&gt;&lt;/code&gt;),
                                  applicable when the secured PDU header is being used&lt;/li&gt;
                                  &lt;li&gt;OR&lt;/li&gt;
                                  &lt;li&gt;The length of the authentic PDU (&lt;code&gt;&lt;a href=&quot;path:./SecOCRxAuthenticPduRef&quot;&gt;SecOCRxAuthenticPduRef&lt;/a&gt;&lt;/code&gt;)
                                  received from the lower layer must have the same length with the authentic PDU (&lt;code&gt;&lt;a href=&quot;path:../../SecOCRxAuthenticPduLayer/SecOCRxAuthenticLayerPduRef&quot;&gt;SecOCRxAuthenticLayerPduRef&lt;/a&gt;&lt;/code&gt;)
                                  that needs to be send to the upper layer,
                                  applicable when the secured PDU header is not being used&lt;/li&gt;
                                &lt;/ul&gt;
                              &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="LABEL" value="Authentic PDU"/>
                        <a:da name="INVALID" type="XPath">
                          <a:tst 
                                 expr="node:exists(./SecOCAuthPduHeaderLength) and                                        (node:value(./SecOCAuthPduHeaderLength) + node:value(node:ref(../../SecOCRxAuthenticPduLayer/SecOCRxAuthenticLayerPduRef)/PduLength) !=                                         node:value(node:ref(./SecOCRxAuthenticPduRef)/PduLength))" 
                                 true="The length of the authentic PDU received from the lower layer must have the same length with the authentic PDU that needs to be send to the upper layer plus the secured header length."/>
                          <a:tst 
                                 expr="not(node:exists(./SecOCAuthPduHeaderLength)) and                                        (node:value(node:ref(../../SecOCRxAuthenticPduLayer/SecOCRxAuthenticLayerPduRef)/PduLength) != node:value(node:ref(./SecOCRxAuthenticPduRef)/PduLength))" 
                                 true="The length of the authentic PDU received from the lower layer must have the same length with the authentic PDU that needs to be send to the upper layer"/>
                        </a:da>
                        <v:var name="SecOCAuthPduHeaderLength" type="INTEGER">
                          <a:a name="DESC">
                            <a:v>&lt;html&gt;
                                  &lt;p&gt;
                                    This parameter indicates the length (in bytes) of the secured PDU
                                    header in the authentic PDU. The length of zero means there&apos;s no header
                                    in the PDU.
                                  &lt;/p&gt;
                                &lt;/html&gt;</a:v>
                          </a:a>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v class="PreCompile">VariantPostBuild</icc:v>
                          </a:a>
                          <a:a name="LABEL" value="Secured PDU Header"/>
                          <a:a name="OPTIONAL" value="true"/>
                          <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                          <a:da name="DEFAULT" value="0"/>
                          <a:da name="ENABLE" value="false"/>
                          <a:da name="INVALID" type="Multi">
                            <mt:range>
                              <mt:tst expr="&lt;=4"/>
                              <mt:tst expr="&gt;=0"/>
                            </mt:range>
                            <mt:xpath 
                                      false="The length of secured PDU header must be smaller than Rx authentic PDU." 
                                      expr="node:value(node:ref(../SecOCRxAuthenticPduRef)/PduLength) &gt; node:value(.)"/>
                          </a:da>
                        </v:var>
                        <v:var name="SecOCRxAuthenticPduId" type="INTEGER">
                          <a:a name="DESC">
                            <a:v>&lt;html&gt;
                                  &lt;p&gt;
                                    This parameter defines the PDU identifier of the authentic PDU
                                    assigned by SecOC module.
                                    Used by PduR for SecOC_PduRRxIndication.
                                  &lt;/p&gt;
                                  &lt;p&gt;Note: The Handle-Id Wizard can be used to set this value automatically.&lt;/p&gt;
                                &lt;/html&gt;</a:v>
                          </a:a>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                          </a:a>
                          <a:a name="LABEL" value="Authentic PDU ID"/>
                          <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                          <a:da name="EDITABLE" value="true"/>
                          <a:da name="INVALID" type="Range">
                            <a:tst expr="&gt;=0"/>
                            <a:tst expr="&lt;=65535"/>
                          </a:da>
                        </v:var>
                        <v:ref name="SecOCRxAuthenticPduRef" type="REFERENCE">
                          <a:a name="DESC">
                            <a:v>&lt;html&gt;
                                  &lt;p&gt;
                                    Reference to the global PDU.
                                  &lt;/p&gt;
                                &lt;/html&gt;</a:v>
                          </a:a>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                          </a:a>
                          <a:a name="LABEL" value="Authentic PDU Reference"/>
                          <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                          <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                          <a:da name="EDITABLE" value="true"/>
                          <a:da name="REF" 
                                value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/EcuC/EcucPduCollection/Pdu"/>
                        </v:ref>
                      </v:ctr>
                      <v:ctr name="SecOCRxCryptographicPdu" type="IDENTIFIABLE">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                                &lt;p&gt;
                                  This container specifies the cryptographic PDU that is received by the
                                  SecOC module from the PduR.
                                &lt;/p&gt;
                                &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                                &lt;ul&gt;
                                  &lt;li&gt;The length of the cryptographic PDU (&lt;code&gt;&lt;a href=&quot;path:./SecOCRxCryptographicPduRef&quot;&gt;SecOCRxCryptographicPduRef&lt;/a&gt;&lt;/code&gt;)
                                  must be equal with the combined length of the truncated freshness value
                                  (&lt;code&gt;&lt;a href=&quot;path:../../SecOCFreshnessValueTxLength&quot;&gt;SecOCFreshnessValueTxLength&lt;/a&gt;&lt;/code&gt;),
                                  the truncated authenticator (&lt;code&gt;&lt;a href=&quot;path:../../SecOCAuthInfoTxLength&quot;&gt;SecOCAuthInfoTxLength&lt;/a&gt;&lt;/code&gt;),
                                  and the message link (&lt;code&gt;&lt;a href=&quot;path:../SecOCUseMessageLink/SecOCMessageLinkLen&quot;&gt;SecOCMessageLinkLen&lt;/a&gt;&lt;/code&gt;),
                                  applicable when message link is being used&lt;/li&gt;
                                  &lt;li&gt;OR&lt;/li&gt;
                                  &lt;li&gt;The length of the cryptographic PDU (&lt;code&gt;&lt;a href=&quot;path:./SecOCRxCryptographicPduRef&quot;&gt;SecOCRxCryptographicPduRef&lt;/a&gt;&lt;/code&gt;)
                                  must be equal with the combined length of the truncated freshness value
                                  (&lt;code&gt;&lt;a href=&quot;path:../../SecOCFreshnessValueTxLength&quot;&gt;SecOCFreshnessValueTxLength&lt;/a&gt;&lt;/code&gt;)
                                  and the truncated authenticator (&lt;code&gt;&lt;a href=&quot;path:../../SecOCAuthInfoTxLength&quot;&gt;SecOCAuthInfoTxLength&lt;/a&gt;&lt;/code&gt;),
                                  applicable when message link is not being used&lt;/li&gt;
                                &lt;/ul&gt;
                                &lt;p&gt;Note: Take into account that when calculating the combined length some configuration parameters hold the length in bytes and some in bits.&lt;/p&gt;
                              &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="LABEL" value="Cryptographic PDU"/>
                        <a:da name="INVALID" type="XPath">
                          <a:tst 
                                 expr="node:exists(../SecOCUseMessageLink) and                                       (as:ref(./SecOCRxCryptographicPduRef)/PduLength != num:i(ceiling((../../SecOCAuthInfoTxLength + ../../SecOCFreshnessValueTxLength + ../SecOCUseMessageLink/SecOCMessageLinkLen) div 8)))" 
                                 true="The length of the cryptographic PDU must be equal                                        with the combined length of the truncated                                        freshness value, the truncated authenticator and                                        the message link."/>
                          <a:tst 
                                 expr="not(node:exists(../SecOCUseMessageLink)) and                                       (as:ref(./SecOCRxCryptographicPduRef)/PduLength != num:i(ceiling((../../SecOCAuthInfoTxLength + ../../SecOCFreshnessValueTxLength) div 8)))" 
                                 true="The length of the cryptographic PDU must be equal                                        with the combined length of the truncated                                        freshness value and the truncated authenticator."/>
                        </a:da>
                        <v:var name="SecOCRxCryptographicPduId" type="INTEGER">
                          <a:a name="DESC">
                            <a:v>&lt;html&gt;
                                  &lt;p&gt;
                                    This parameter defines the PDU identifier of the cryptographic
                                    PDU assigned by SecOC module.
                                    Used by PduR for &lt;code&gt;SecOC_PduRRxIndication&lt;/code&gt;.
                                  &lt;/p&gt;
                                  &lt;p&gt;Note: The Handle-Id Wizard can be used to set this value automatically.&lt;/p&gt;
                                &lt;/html&gt;</a:v>
                          </a:a>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                          </a:a>
                          <a:a name="LABEL" value="Cryptographic PDU ID"/>
                          <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                          <a:da name="EDITABLE" value="true"/>
                          <a:da name="INVALID" type="Range">
                            <a:tst expr="&gt;=0"/>
                            <a:tst expr="&lt;=65535"/>
                          </a:da>
                        </v:var>
                        <v:ref name="SecOCRxCryptographicPduRef" 
                               type="REFERENCE">
                          <a:a name="DESC">
                            <a:v>&lt;html&gt;
                                  &lt;p&gt;
                                    Reference to the global PDU.
                                  &lt;/p&gt;
                                &lt;/html&gt;</a:v>
                          </a:a>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                          </a:a>
                          <a:a name="LABEL" value="Cryptographic PDU Reference"/>
                          <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                          <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                          <a:da name="EDITABLE" value="true"/>
                          <a:da name="REF" 
                                value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/EcuC/EcucPduCollection/Pdu"/>
                        </v:ref>
                      </v:ctr>
                      <v:ctr name="SecOCUseMessageLink" type="IDENTIFIABLE">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                                &lt;p&gt;
                                  SecOC links an authentic PDU and cryptographic PDU together by
                                  repeating a specific part (message link) of the authentic PDU
                                  in the cryptographic PDU.
                                &lt;/p&gt;
                              &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="LABEL" value="Message Link"/>
                        <a:a name="OPTIONAL" value="true"/>
                        <a:da name="ENABLE" value="false"/>
                        <a:da name="INVALID" type="XPath" 
                              expr="(num:i(ceiling((node:value(./SecOCMessageLinkLen) + node:value(./SecOCMessageLinkPos)) div 8)) &gt; node:value(node:ref(../../SecOCRxAuthenticPduLayer/SecOCRxAuthenticLayerPduRef)/PduLength))" 
                              true="The sum of configured value of message link length and message link offset (converted in bytes) must be smaller than or equal to the authentic PDU length"/>
                        <v:var name="SecOCMessageLinkLen" type="INTEGER">
                          <a:a name="DESC">
                            <a:v>&lt;html&gt;
                                  &lt;p&gt;
                                    This parameter defines the length of the message link inside
                                    the authentic PDU in bits.
                                  &lt;/p&gt;
                                &lt;/html&gt;</a:v>
                          </a:a>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                          </a:a>
                          <a:a name="LABEL" value="Message Link Length"/>
                          <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                          <a:da name="EDITABLE" value="true"/>
                          <a:da name="INVALID" type="Range">
                            <a:tst expr="&gt;=1"/>
                            <a:tst expr="&lt;=65535"/>
                          </a:da>
                        </v:var>
                        <v:var name="SecOCMessageLinkPos" type="INTEGER">
                          <a:a name="DESC">
                            <a:v>&lt;html&gt;
                                  &lt;p&gt;
                                    This parameter defines the position of the message link
                                    inside the authentic PDU in bits.
                                  &lt;/p&gt;
                                &lt;/html&gt;</a:v>
                          </a:a>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                          </a:a>
                          <a:a name="LABEL" value="Message Link Position"/>
                          <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                          <a:da name="EDITABLE" value="true"/>
                          <a:da name="INVALID" type="Range">
                            <a:tst expr="&gt;=0"/>
                            <a:tst expr="&lt;=65535"/>
                          </a:da>
                        </v:var>
                      </v:ctr>
                    </v:ctr>
                  </v:chc>
                  <v:ctr name="SecOCRxPduSecuredArea" type="IDENTIFIABLE">
                    <a:a name="COLUMN_TITLE" value="Secured Area"/>
                    <a:a name="COLUMN_TOOLTIP" value="SecOCRxPduSecuredArea"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                              &lt;p&gt;
                                This container specifies an area in the authentic PDU that will
                                be the input to the authenticator verification algorithm. If this
                                container does not exist in the configuration the complete
                                authentic PDU will be the input to the authenticator
                                verification algorithm.
                              &lt;/p&gt;
                              &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                              &lt;ul&gt;
                                &lt;li&gt;Enable Secured Area Usage (&lt;code&gt;&lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/SecOC/ELEMENTS/SecOC/SecOCEbGeneral/SecOCUseSecuredArea&quot;&gt;SecOCUseSecuredArea&lt;/a&gt;&lt;/code&gt;): must be enabled in order to be able to use this feature&lt;/li&gt;
                                &lt;li&gt;Secured Area Length (&lt;code&gt;&lt;a href=&quot;path:./SecOCSecuredRxPduLength&quot;&gt;SecOCSecuredRxPduLength&lt;/a&gt;&lt;/code&gt;) and Secured Area Offset (&lt;code&gt;&lt;a href=&quot;path:./SecOCSecuredRxPduOffset&quot;&gt;SecOCSecuredRxPduOffset&lt;/a&gt;&lt;/code&gt;): the sum of the secured area length and the secured area offset must be smaller than or equal to the authentic PDU length&lt;/li&gt;
                              &lt;/ul&gt;
                            &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="LABEL" value="Secured Area"/>
                    <a:a name="OPTIONAL" value="true"/>
                    <a:a name="POSTBUILDVARIANTMULTIPLICITY" value="true"/>
                    <a:da name="ENABLE" value="false"/>
                    <a:da name="INVALID" type="XPath">
                      <a:tst 
                             expr="(node:value(./SecOCSecuredRxPduLength) + node:value(./SecOCSecuredRxPduOffset) &gt; node:value(node:ref(../SecOCRxAuthenticPduLayer/SecOCRxAuthenticLayerPduRef)/PduLength))" 
                             true="The sum of configured value of secured area length and secured area offset must be smaller than or equal to the authentic PDU length"/>
                      <a:tst 
                             expr="node:value(../../../SecOCEbGeneral/SecOCUseSecuredArea) = &apos;false&apos;" 
                             true="In the General tab the SecOCUseSecuredArea configuration parameter must be enabled"/>
                    </a:da>
                    <v:var name="SecOCSecuredRxPduLength" type="INTEGER">
                      <a:a name="COLUMN_TITLE" value="Secured Area Length"/>
                      <a:a name="COLUMN_TOOLTIP" value="SecOCSecuredRxPduLength"/>
                      <a:a name="DESC">
                        <a:v>&lt;html&gt;
                                &lt;p&gt;
                                  This parameter defines the length (in bytes) of the area within
                                  the PDU which shall be secured.
                                &lt;/p&gt;
                              &lt;/html&gt;</a:v>
                      </a:a>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                           type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                      </a:a>
                      <a:a name="LABEL" value="Secured Area Length"/>
                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                      <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                      <a:da name="INVALID" type="Range">
                        <a:tst expr="&gt;=0"/>
                        <a:tst expr="&lt;=65535"/>
                      </a:da>
                    </v:var>
                    <v:var name="SecOCSecuredRxPduOffset" type="INTEGER">
                      <a:a name="COLUMN_TITLE" value="Secured Area Offset"/>
                      <a:a name="COLUMN_TOOLTIP" value="SecOCSecuredRxPduOffset"/>
                      <a:a name="DESC">
                        <a:v>&lt;html&gt;
                                &lt;p&gt;
                                  This parameter defines the start position (offset in bytes) of the
                                  area within the PDU which shall be secured.
                                &lt;/p&gt;
                              &lt;/html&gt;</a:v>
                      </a:a>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                           type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                      </a:a>
                      <a:a name="LABEL" value="Secured Area Offset"/>
                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                      <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                      <a:da name="INVALID" type="Range">
                        <a:tst expr="&gt;=0"/>
                        <a:tst expr="&lt;=65535"/>
                      </a:da>
                    </v:var>
                  </v:ctr>
                  <v:ctr name="SecOCRxAuthenticPduLayer" type="IDENTIFIABLE">
                    <a:a name="COLUMN_TITLE" value="Authentic PDU Layer"/>
                    <a:a name="COLUMN_TOOLTIP" value="SecOCRxAuthenticPduLayer"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              This container specifies the PDU that is transmitted by the SecOC module
                              to the PduR after the Mac was verified.
                            &lt;/p&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="LABEL" value="Authentic PDU Layer"/>
                    <v:var name="SecOCPduType" type="ENUMERATION">
                      <a:a name="COLUMN_TITLE" value="PduR API Type"/>
                      <a:a name="COLUMN_TOOLTIP" value="SecOCPduType"/>
                      <a:a name="DESC">
                        <a:v>&lt;html&gt;
                              &lt;p&gt;
                                This parameter defines API Type to use for communication with PduR.
                              &lt;/p&gt;

                              &lt;p&gt;Range:&lt;/p&gt;
                              &lt;ul&gt;
                                &lt;li&gt;&lt;code&gt;SECOC_IFPDU&lt;/code&gt;: Interface communication API&lt;/li&gt;
                                &lt;li&gt;&lt;code&gt;SECOC_TPPDU&lt;/code&gt;: Transport Protocol communication API&lt;/li&gt;
                              &lt;/ul&gt;
                            &lt;/html&gt;</a:v>
                      </a:a>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                           type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                      </a:a>
                      <a:a name="LABEL" value="PduR API Type"/>
                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                      <a:da name="DEFAULT" value="SECOC_IFPDU"/>
                      <a:da name="RANGE">
                        <a:v>SECOC_IFPDU</a:v>
                        <a:v>SECOC_TPPDU</a:v>
                      </a:da>
                    </v:var>
                    <v:ref name="SecOCRxAuthenticLayerPduRef" type="REFERENCE">
                      <a:a name="COLUMN_TITLE" value="Authentic PDU Reference"/>
                      <a:a name="COLUMN_TOOLTIP" 
                           value="SecOCRxAuthenticLayerPduRef"/>
                      <a:a name="DESC">
                        <a:v>&lt;html&gt;
                              &lt;p&gt;Reference to the global PDU holding an authenticated PDU.&lt;/p&gt;
                            &lt;/html&gt;</a:v>
                      </a:a>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                           type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                      </a:a>
                      <a:a name="LABEL" value="Authentic PDU Reference"/>
                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                      <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                      <a:da name="INVALID" type="XPath" 
                            expr="(node:refvalid(.))" 
                            false="The expression argument reference is not valid."/>
                      <a:da name="REF" 
                            value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/EcuC/EcucPduCollection/Pdu"/>
                    </v:ref>
                    <v:var name="SecOCUnsupportedPduId" type="INTEGER">
                      <a:a name="DESC">
                        <a:v>&lt;html&gt;
                              &lt;p&gt;
                                This parameter defines the ID of unsupported PDU
                              &lt;/p&gt;
                            &lt;/html&gt;</a:v>
                      </a:a>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                           type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                      </a:a>
                      <a:a name="LABEL" value="Dummy Id"/>
                      <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                      <a:a name="VISIBLE" value="false"/>
                      <a:da name="DEFAULT" value="255"/>
                      <a:da name="EDITABLE" value="false"/>
                    </v:var>
                  </v:ctr>
                </v:ctr>
              </v:lst>
              <v:lst name="SecOCTxPduProcessing" type="MAP">
                <a:a name="LABEL" value="Secured Tx PDUs"/>
                <a:da name="MAX" value="65535"/>
                <a:da name="MIN" value="0"/>
                <v:ctr name="SecOCTxPduProcessing" type="IDENTIFIABLE">
                  <a:da name="INVALID" type="XPath" 
                        expr="count(../*[SecOCFreshnessValueId = node:value(node:current()/SecOCFreshnessValueId)]) = count(../*[(SecOCFreshnessValueId = node:value(node:current()/SecOCFreshnessValueId))                                  and (SecOCFreshnessValueLength = node:value(node:current()/SecOCFreshnessValueLength))])" 
                        false="The secured TX PDUs with the same freshness value ID must have                                   the same freshness value length."/>
                  <v:ref name="SecOCTxPduMainFunctionRef" 
                         type="SYMBOLIC-NAME-REFERENCE">
                    <a:a name="COLUMN_TITLE" value="Main Function Reference"/>
                    <a:a name="COLUMN_TOOLTIP" value="SecOCTxPduMainFunctionRef"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              Reference to the main function (&lt;code&gt;&lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/SecOC/ELEMENTS/SecOC/SecOCMainFunctionRx&quot;&gt;SecOCMainFunctionRx&lt;/a&gt;&lt;/code&gt;)
                              which shall process this secured PDU.
                              Mandatory, if multiple main functions are defined.
                            &lt;/p&gt;
                            &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                            &lt;ul&gt;
                              &lt;li&gt;SecOC Main Function (&lt;code&gt;&lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/SecOC/ELEMENTS/SecOC/SecOCMainFunctionRx&quot;&gt;SecOCMainFunctionRx&lt;/a&gt;&lt;/code&gt;): at least one main function must be configured in the list in order to be able to use this feature&lt;/li&gt;
                              &lt;li&gt;Csm Main Function (&lt;code&gt;&lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/Csm/ELEMENTS/Csm/CsmMainFunction&quot;&gt;CsmMainFunction&lt;/a&gt;&lt;/code&gt;): must reference the same EcuC partition as the SecOC main function&lt;/li&gt;
                            &lt;/ul&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Main Function Reference"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:da name="ENABLE" type="XPath" 
                          expr="num:i(count(../../../SecOCMainFunctionTx/*)) &gt; 0"/>
                    <a:da name="INVALID" type="XPath">
                      <a:tst expr="(node:refvalid(.))" 
                             false="The expression argument reference is not valid."/>
                      <a:tst 
                             expr="node:ref(.)/SecOCMainFunctionTxPartitionRef = node:fallback(&apos;-&gt;node:ref(node:ref(node:ref(../SecOCTxAuthServiceConfigRef)/CsmJobQueueRef)/CsmQueueMainFunctionRef)/CsmMainFunctionPartitionRef&apos;, node:ref(.)/SecOCMainFunctionTxPartitionRef)" 
                             false="The TX MainFunction has to reference the same EcuC partition as the associated Csm queue."/>
                    </a:da>
                    <a:da name="REF" 
                          value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/SecOC/SecOCMainFunctionTx"/>
                  </v:ref>
                  <v:ref name="SecOCTxAuthServiceConfigRef" 
                         type="CHOICE-REFERENCE">
                    <a:a name="COLUMN_TITLE" 
                         value="Authentication Job Reference"/>
                    <a:a name="COLUMN_TOOLTIP" 
                         value="SecOCTxAuthServiceConfigRef"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              This parameter defines the authentication algorithm used for
                              authentication generation.
                            &lt;/p&gt;
                            &lt;p&gt;
                              The value of this parameter must be a valid configuration of a
                              MacGenerate or SignatureGenerate configuration in a Csm module.
                            &lt;/p&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Authentication Job Reference"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:da name="EDITABLE" value="true"/>
                    <a:da name="INVALID" type="XPath">
                      <a:tst expr="(node:refvalid(.))" 
                             false="The expression argument reference is not valid."/>
                      <a:tst 
                             expr="node:exists(as:ref(as:ref(.)/CsmJobPrimitiveRef)/CsmMacGenerate) or (node:exists(as:ref(as:ref(.)/CsmJobPrimitiveRef)/CsmSignatureGenerate))" 
                             false="The referenced Csm job needs to be configured to use the MacGenerate or SignatureGenerate as primitive."/>
                    </a:da>
                    <a:da name="REF" 
                          value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/Csm/CsmJobs/CsmJob"/>
                  </v:ref>
                  <v:var name="SecOCAuthInfoTxLength" type="INTEGER">
                    <a:a name="COLUMN_TITLE" 
                         value="Authenticator Truncated Length"/>
                    <a:a name="COLUMN_TOOLTIP" value="SecOCAuthInfoTxLength"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              This parameter defines the length in bits of the authenticator (MAC),
                              which is included in the payload of the secured PDU or cryptographic PDU.
                            &lt;/p&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Authenticator Truncated Length"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                    <a:da name="EDITABLE" value="true"/>
                    <a:da name="INVALID" type="Range">
                      <a:tst expr="&gt;=1"/>
                      <a:tst expr="&lt;=65535"/>
                    </a:da>
                  </v:var>
                  <v:var name="SecOCDataId" type="INTEGER">
                    <a:a name="COLUMN_TITLE" value="Data ID"/>
                    <a:a name="COLUMN_TOOLTIP" value="SecOCDataId"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              This parameter defines a numerical identifier for the secured PDU.
                            &lt;/p&gt;
                            &lt;p&gt;Range:&lt;/p&gt;
                            &lt;ul&gt;
                              &lt;li&gt;0 .. 255: when Data ID Length (&lt;code&gt;&lt;a href=&quot;path:../../../SecOCEbGeneral/SecOCDataIdLength&quot;&gt;SecOCDataIdLength&lt;/a&gt;&lt;/code&gt;) is set to UINT8&lt;/li&gt;
                              &lt;li&gt;0 .. 65535: when Data ID Length (&lt;code&gt;&lt;a href=&quot;path:../../../SecOCEbGeneral/SecOCDataIdLength&quot;&gt;SecOCDataIdLength&lt;/a&gt;&lt;/code&gt;) is set to UINT16&lt;/li&gt;
                              &lt;li&gt;0 .. 4294967295: when Data ID Length (&lt;code&gt;&lt;a href=&quot;path:../../../SecOCEbGeneral/SecOCDataIdLength&quot;&gt;SecOCDataIdLength&lt;/a&gt;&lt;/code&gt;) is set to UINT32&lt;/li&gt;
                            &lt;/ul&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Data ID"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                    <a:da name="EDITABLE" value="true"/>
                    <a:da name="INVALID" type="Multi">
                      <mt:range>
                        <mt:tst expr="&lt;=4294967295"/>
                        <mt:tst expr="&gt;=0"/>
                      </mt:range>
                      <mt:xpath>
                        <mt:tst 
                                true="SecOCDataId must not be lower then 0 or greater than 255" 
                                expr="((node:value(.) &lt; 0 or node:value(.) &gt; 255) and node:value(../../../SecOCEbGeneral/SecOCDataIdLength) = &apos;UINT8&apos;)"/>
                        <mt:tst 
                                true="SecOCDataId must not be lower then 0 or greater than 65535" 
                                expr="((node:value(.) &lt; 0 or node:value(.) &gt; 65535) and node:value(../../../SecOCEbGeneral/SecOCDataIdLength) = &apos;UINT16&apos;)"/>
                        <mt:tst 
                                true="SecOCDataId must not be lower then 0 or greater than 4294967295" 
                                expr="((node:value(.) &lt; 0 or node:value(.) &gt; 4294967295) and node:value(../../../SecOCEbGeneral/SecOCDataIdLength) = &apos;UINT32&apos;)"/>
                      </mt:xpath>
                    </a:da>
                    <a:da name="WARNING" type="XPath" 
                          expr="text:uniq(../../../*/*/SecOCDataId,.)" 
                          false="The DataId for the secured PDU is not unique                                 in the present configuration."/>
                  </v:var>
                  <v:var name="SecOCFreshnessValueId" type="INTEGER">
                    <a:a name="COLUMN_TITLE" value="Freshness Value ID"/>
                    <a:a name="COLUMN_TOOLTIP" value="SecOCFreshnessValueId"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              This parameter defines the ID of the Freshness value.
                              The Freshness value might be a normal counter or a time value.
                              If Freshness counters are used, the FreshnessValueId with the same
                              value must have the same FreshnessValueLength.
                            &lt;/p&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Freshness Value ID"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                    <a:da name="EDITABLE" value="true"/>
                    <a:da name="INVALID" type="Range">
                      <a:tst expr="&gt;=0"/>
                      <a:tst expr="&lt;=65535"/>
                    </a:da>
                  </v:var>
                  <v:var name="SecOCFreshnessValueLength" type="INTEGER">
                    <a:a name="COLUMN_TITLE" value="Freshness Value Length"/>
                    <a:a name="COLUMN_TOOLTIP" value="SecOCFreshnessValueLength"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              This parameter defines the complete length in bits of the Freshness Value.
                            &lt;/p&gt;
                            &lt;p&gt;
                              As long as the key doesn&apos;t change the counter shall not overflow.
                              The length of the counter shall be determined based on the expected life
                              time of the corresponding key and frequency of usage of the Freshness
                              Value.
                            &lt;/p&gt;

                            &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                            &lt;ul&gt;
                              &lt;li&gt;Query Freshness Value (&lt;code&gt;&lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/SecOC/ELEMENTS/SecOC/SecOCGeneral/SecOCQueryFreshnessValue&quot;&gt;SecOCQueryFreshnessValue&lt;/a&gt;&lt;/code&gt;): set to something different that NONE in order to be able to set the freshness value length bigger than 0&lt;/li&gt;
                            &lt;/ul&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Freshness Value Length"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                    <a:da name="EDITABLE" value="true"/>
                    <a:da name="INVALID" type="Multi">
                      <mt:range>
                        <mt:tst expr="&lt;=64"/>
                        <mt:tst expr="&gt;=0"/>
                      </mt:range>
                      <mt:xpath 
                                true="When SecOCQueryFreshnessValue is set to &apos;NONE&apos; the freshness value length must be set to 0." 
                                expr="(../../../SecOCGeneral/SecOCQueryFreshnessValue = &apos;NONE&apos;) and (node:value(.) &gt; 0)"/>
                    </a:da>
                  </v:var>
                  <v:var name="SecOCFreshnessValueTxLength" type="INTEGER">
                    <a:a name="COLUMN_TITLE" 
                         value="Freshness Value Truncated Length"/>
                    <a:a name="COLUMN_TOOLTIP" 
                         value="SecOCFreshnessValueTxLength"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                             This parameter defines the length in bits of the freshness value to be
                             included in the payload of the secured PDU or cryptographic PDU.
                            &lt;/p&gt;
                            &lt;p&gt;
                             This length is specificing the amount of least significant bits that
                             shall be used from the complete freshness value.
                            &lt;/p&gt;
                            &lt;p&gt;
                             If the parameter is 0 no freshness value is included in the secured PDU or cryptographic PDU.
                            &lt;/p&gt;
                            &lt;p&gt;Range:&lt;/p&gt;
                            &lt;ul&gt;
                              &lt;li&gt;0 .. &lt;code&gt;&lt;a href=&quot;path:../SecOCFreshnessValueLength&quot;&gt;Freshness Value Length&lt;/a&gt;&lt;/code&gt;&lt;/li&gt;
                            &lt;/ul&gt;
                            &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                            &lt;ul&gt;
                              &lt;li&gt;Freshness Value Length (&lt;code&gt;&lt;a href=&quot;path:../SecOCFreshnessValueLength&quot;&gt;SecOCFreshnessValueLength&lt;/a&gt;&lt;/code&gt;): limits the range selection, the truncated freshness value cannot be bigger than the complete freshness value&lt;/li&gt;
                              &lt;li&gt;Query Freshness Value (&lt;code&gt;&lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/SecOC/ELEMENTS/SecOC/SecOCGeneral/SecOCQueryFreshnessValue&quot;&gt;SecOCQueryFreshnessValue&lt;/a&gt;&lt;/code&gt;): set to something different than NONE in order to be able to set the freshness value length bigger than 0&lt;/li&gt;
                            &lt;/ul&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Freshness Value Truncated Length"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                    <a:da name="EDITABLE" value="true"/>
                    <a:da name="INVALID" type="Multi">
                      <mt:range>
                        <mt:tst expr="&lt;=64"/>
                        <mt:tst expr="&gt;=0"/>
                      </mt:range>
                      <mt:xpath>
                        <mt:tst 
                                true="The truncated freshness value length must not be greater than freshness value length." 
                                expr="(. &gt; ../SecOCFreshnessValueLength)"/>
                        <mt:tst 
                                true="When SecOCQueryFreshnessValue is set to &apos;NONE&apos; the truncated freshness value length must be set to 0." 
                                expr="(../../../SecOCGeneral/SecOCQueryFreshnessValue = &apos;NONE&apos;) and (node:value(.) &gt; 0)"/>
                      </mt:xpath>
                    </a:da>
                  </v:var>
                  <v:var name="SecOCFreshnessValueFuncName" 
                         type="FUNCTION-NAME">
                    <a:a name="COLUMN_TITLE" 
                         value="Freshness Value Function Name"/>
                    <a:a name="COLUMN_TOOLTIP" 
                         value="SecOCFreshnessValueFuncName"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              This parameter specifies the name of the C API function which shall be
                              called to query the freshness for the current PDU.
                            &lt;/p&gt;
                            &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                            &lt;ul&gt;
                              &lt;li&gt;Query Freshness Value (&lt;code&gt;&lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/SecOC/ELEMENTS/SecOC/SecOCGeneral/SecOCQueryFreshnessValue&quot;&gt;SecOCQueryFreshnessValue&lt;/a&gt;&lt;/code&gt;): to be able to configure the name of the C API function, query must be set on &lt;code&gt;CFUNC&lt;/code&gt;&lt;/li&gt;
                            &lt;/ul&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v class="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Freshness Value Function Name"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:da name="ENABLE" type="XPath" 
                          expr="(../../../SecOCGeneral/SecOCQueryFreshnessValue = &apos;CFUNC&apos;)"/>
                    <a:da name="INVALID" type="XPath" 
                          expr="(. != &apos;&apos;) and text:match(., &apos;^[a-zA-Z][a-zA-Z0-9_]*$&apos;)" 
                          false="Missing or invalid C function name."/>
                  </v:var>
                  <v:var name="SecOCSecuredPDUTransmittedFuncName" 
                         type="FUNCTION-NAME">
                    <a:a name="COLUMN_TITLE" 
                         value="SPduTxConfirmation Function Name"/>
                    <a:a name="COLUMN_TOOLTIP" 
                         value="SecOCSecuredPDUTransmittedFuncName"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                             This parameter specifies the name of the C API function which shall be
                             called after a secured PDU has been started for transmission.
                            &lt;/p&gt;
                            &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                            &lt;ul&gt;
                              &lt;li&gt;Query Freshness Value (&lt;code&gt;&lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/SecOC/ELEMENTS/SecOC/SecOCGeneral/SecOCQueryFreshnessValue&quot;&gt;SecOCQueryFreshnessValue&lt;/a&gt;&lt;/code&gt;): to be able to configure the name of the C API function, query must be set on &lt;code&gt;CFUNC&lt;/code&gt;&lt;/li&gt;
                            &lt;/ul&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v class="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="SPduTxConfirmation Function Name"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:da name="ENABLE" type="XPath" 
                          expr="(../../../SecOCGeneral/SecOCQueryFreshnessValue = &apos;CFUNC&apos;)"/>
                    <a:da name="INVALID" type="XPath" 
                          expr="(. != &apos;&apos;) and text:match(., &apos;^[a-zA-Z][a-zA-Z0-9_]*$&apos;)" 
                          false="Missing or invalid C function name."/>
                  </v:var>
                  <v:var name="SecOCAuthenticationBuildAttempts" type="INTEGER">
                    <a:a name="COLUMN_TITLE" 
                         value="Authentication Build Attempts"/>
                    <a:a name="COLUMN_TOOLTIP" 
                         value="SecOCAuthenticationBuildAttempts"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              This parameter defines the number of authentication build attempts
                              when an authentication failed because the freshness value could not be
                              obtained or the generation of the authenticator could not be performed.
                            &lt;/p&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Authentication Build Attempts"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:da name="DEFAULT" value="0"/>
                    <a:da name="EDITABLE" value="true"/>
                    <a:da name="INVALID" type="Range">
                      <a:tst expr="&gt;=0"/>
                      <a:tst expr="&lt;=65535"/>
                    </a:da>
                  </v:var>
                  <v:var name="SecOCMacGenerateStatusPropagationMode" 
                         type="ENUMERATION">
                    <a:a name="COLUMN_TITLE" 
                         value="Generate Status Propagation Mode"/>
                    <a:a name="COLUMN_TOOLTIP" 
                         value="SecOCMacGenerateStatusPropagationMode"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              This parameter is used to describe the propagation of the status of
                              each authentication attempt from the SecOC module to the application
                              via callouts defined in &lt;code&gt;&lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/SecOC/ELEMENTS/SecOC/SecOCGeneral/SecOCMacGenerateStatusCallout&quot;&gt;SecOCMacGenerateStatusCallout&lt;/a&gt;&lt;/code&gt; or
                              via RTE sender receiver interface.
                            &lt;/p&gt;
                            &lt;p&gt;Range:&lt;/p&gt;
                            &lt;ul&gt;
                              &lt;li&gt;&lt;code&gt;NONE&lt;/code&gt;: SecOC will not propagate the status of the authenticator generation&lt;/li&gt;
                              &lt;li&gt;&lt;code&gt;FAILURE_ONLY&lt;/code&gt;: SecOC will propagate the status only when the authenticator generation failed&lt;/li&gt;
                              &lt;li&gt;&lt;code&gt;BOTH&lt;/code&gt;:   SecOC will propagate both negative and posivite status of the authenticator generation&lt;/li&gt;
                            &lt;/ul&gt;
                            &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                            &lt;ul&gt;
                              &lt;li&gt;Enable Rte Usage (&lt;code&gt;&lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/SecOC/ELEMENTS/SecOC/SecOCEbGeneral/SecOCRteUsage&quot;&gt;SecOCRteUsage&lt;/a&gt;&lt;/code&gt;): in order to propagate the authenticator generation status via RTE&lt;/li&gt;
                              &lt;li&gt;MAC Generate Status Callout Functions (&lt;code&gt;&lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/SecOC/ELEMENTS/SecOC/SecOCGeneral/SecOCMacGenerateStatusCallout&quot;&gt;SecOCMacGenerateStatusCallout&lt;/a&gt;&lt;/code&gt;): in order to propagate the authenticator generation status via C functions&lt;/li&gt;
                            &lt;/ul&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Generate Status Propagation Mode"/>
                    <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                    <a:da name="DEFAULT" value="NONE"/>
                    <a:da name="EDITABLE" value="true"/>
                    <a:da name="INVALID" type="XPath" 
                          expr="node:value(.) != &apos;NONE&apos; and num:i(count(../../../SecOCGeneral/SecOCMacGenerateStatusCallout/*)) = 0 and node:value(../../../SecOCEbGeneral/SecOCRteUsage) = &apos;false&apos;" 
                          true="To propagate the authenticator generation status the RTE usage must be enabled or at least one callout function must be configured in the MacGenerateStatus Callout container."/>
                    <a:da name="RANGE">
                      <a:v>BOTH</a:v>
                      <a:v>FAILURE_ONLY</a:v>
                      <a:v>NONE</a:v>
                    </a:da>
                  </v:var>
                  <v:ref name="SecOCSameBufferPduRef" type="REFERENCE">
                    <a:a name="COLUMN_TITLE" value="Same Buffer PDU Reference"/>
                    <a:a name="COLUMN_TOOLTIP" value="SecOCSameBufferPduRef"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                           &lt;p&gt;
                             This reference is used to collect PDUs that are using the same
                             SecOC buffer.
                           &lt;/p&gt;
                           &lt;p&gt;
                             The referenced buffer must be used only by Tx PDU(s).
                           &lt;/p&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v class="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Same Buffer PDU Reference"/>
                    <a:a name="OPTIONAL" value="true"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:da name="EDITABLE" value="true"/>
                    <a:da name="ENABLE" value="true"/>
                    <a:da name="INVALID" type="XPath">
                      <a:tst expr="(node:refvalid(.))" 
                             false="The expression argument reference is not valid."/>
                      <a:tst 
                             expr="(../SecOCTxSecuredPduLayer = &apos;SecOCTxSecuredPduCollection&apos;) and                                    ((node:value(as:ref(../SecOCTxSecuredPduLayer/SecOCTxAuthenticPdu/SecOCTxAuthenticPduRef)/PduLength) + node:value(as:ref(../SecOCTxSecuredPduLayer/SecOCTxCryptographicPdu/SecOCTxCryptographicPduRef)/PduLength))                                     &gt; node:value(as:ref(.)/SecOCBufferLength))" 
                             true=" The size of the references SecOCSameBufferPduCollection must be                                      at least equal with the combined length of the lower layer authentic PDU                                      and cryptographic PDU."/>
                      <a:tst 
                             expr="(../SecOCTxSecuredPduLayer = &apos;SecOCTxSecuredPdu&apos;) and                                    (node:value(as:ref(../SecOCTxSecuredPduLayer/SecOCTxSecuredLayerPduRef)/PduLength) &gt; node:value(as:ref(.)/SecOCBufferLength))" 
                             true=" The size of the references SecOCSameBufferPduCollection must be                                      at least equal with the length of the secured PDU."/>
                      <a:tst 
                             expr="node:containsValue(../../../SecOCRxPduProcessing/*[node:exists(./SecOCSameBufferPduRef) = true()]/SecOCSameBufferPduRef,.)" 
                             true=" The selected buffer is already being used by the Rx PDUs."/>
                    </a:da>
                    <a:da name="REF" 
                          value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/SecOC/SecOCSameBufferPduCollection"/>
                  </v:ref>
                  <v:var name="SecOCProvideTxTruncatedFreshnessValue" 
                         type="BOOLEAN">
                    <a:a name="COLUMN_TITLE" 
                         value="Enable Provide Truncated Freshness Value"/>
                    <a:a name="COLUMN_TOOLTIP" 
                         value="SecOCProvideTxTruncatedFreshnessValue"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              This parameter specifies if the Tx query freshness function provides
                              the truncated freshness value besides the complet freshness value.
                              In this case, SecOC shall add this data to the secured PDU instead
                              of truncating from the complete freshness value.
                            &lt;/p&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" 
                         value="Enable Provide Truncated Freshness Value"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:da name="DEFAULT" value="false"/>
                    <a:da name="ENABLE" value="true"/>
                  </v:var>
                  <v:var name="SecOCReAuthenticateAfterTriggerTransmit" 
                         type="BOOLEAN">
                    <a:a name="COLUMN_TITLE" 
                         value="ReAuthenticate After Trigger Transmit"/>
                    <a:a name="COLUMN_TOOLTIP" 
                         value="SecOCReAuthenticateAfterTriggerTransmit"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                           &lt;p&gt;
                             This parameter specifies if the authentication information of the Secured PDU
                             is updated after the successful transmission of a triggered transmission was confirmed.
                           &lt;/p&gt;
                           &lt;p&gt;Range:&lt;/p&gt;
                           &lt;ul&gt;
                             &lt;li&gt;&lt;code&gt;TRUE&lt;/code&gt;: the authentication information shall be updated after triggered transmission&lt;/li&gt;
                             &lt;li&gt;&lt;code&gt;FALSE&lt;/code&gt;: the authentication information shall not be updated after triggered transmission&lt;/li&gt;
                           &lt;/ul&gt;
                           &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                           &lt;ul&gt;
                             &lt;li&gt;Immediate PDU processing (&lt;code&gt;&lt;a href=&quot;path:../SecOCTxSyncPduProcessing&quot;&gt;SecOCTxSyncPduProcessing&lt;/a&gt;&lt;/code&gt;): when immediate PDU processing feature is set to TRUE, reauthentication after trigger transmit is not possible.&lt;/li&gt;
                           &lt;/ul&gt;
                           &lt;p&gt;Note: This parameter should only be set to FALSE if the upper layer SecOC_IfTransmit has the same or a higher frequency than the SecOC_TriggerTransmit calls.&lt;/p&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" 
                         value="ReAuthenticate After Trigger Transmit"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:da name="DEFAULT" value="false"/>
                    <a:da name="INVALID" type="XPath" 
                          expr="node:value(.) = &apos;true&apos; and node:value(../SecOCTxSyncPduProcessing) = &apos;true&apos;" 
                          true="Immediate processing is not possible when reauthentication after trigger transmit is enabled"/>
                  </v:var>
                  <v:var name="SecOCUseTxConfirmation" type="BOOLEAN">
                    <a:a name="COLUMN_TITLE" 
                         value="Enable SPduTxConfirmation Usage"/>
                    <a:a name="COLUMN_TOOLTIP" value="SecOCUseTxConfirmation"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              This parameter indicates if the function
                              &lt;code&gt;SecOC_SPduTxConfirmation&lt;/code&gt; shall be called for this PDU.
                            &lt;/p&gt;
                            &lt;p&gt;Range:&lt;/p&gt;
                            &lt;ul&gt;
                              &lt;li&gt;&lt;code&gt;TRUE&lt;/code&gt;: the function shall be called&lt;/li&gt;
                              &lt;li&gt;&lt;code&gt;FALSE&lt;/code&gt;: the function shall not be called&lt;/li&gt;
                            &lt;/ul&gt;
                            &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                             &lt;ul&gt;
                               &lt;li&gt;SecOCQueryFreshnessValue(&lt;code&gt;&lt;a href=&quot;path:../../../SecOCGeneral/SecOCQueryFreshnessValue&quot;&gt;SecOCQueryFreshnessValue&lt;/a&gt;&lt;/code&gt;): The &lt;code&gt;SecOC_SPduTxConfirmation&lt;/code&gt; cannot be enabled if SecOCQueryFreshnes is set to NONE.&lt;/li&gt;
                             &lt;/ul&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v class="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Enable SPduTxConfirmation Usage"/>
                    <a:a name="OPTIONAL" value="true"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:da name="DEFAULT" value="false"/>
                    <a:da name="ENABLE" value="true"/>
                    <a:da name="INVALID" type="XPath" 
                          expr="node:exists(.) = &apos;true&apos; and node:value(../../../SecOCGeneral/SecOCQueryFreshnessValue) = &apos;NONE&apos;" 
                          true="When SecOCQueryFreshnessValue is set to none this option cannot be enabled."/>
                  </v:var>
                  <v:var name="SecOCTxConfirmationTimeout" type="FLOAT">
                    <a:a name="COLUMN_TITLE" 
                         value="Timeout period for SPduTxConfirmation"/>
                    <a:a name="COLUMN_TOOLTIP" 
                         value="SecOCTxConfirmationTimeout"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              Period in seconds for SPduTxConfirmation timeout.
                            &lt;/p&gt;
                            &lt;p&gt;
                              If the value is 0, the timeout feature will be disabled.
                            &lt;/p&gt;
                            &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                            &lt;ul&gt;
                              &lt;li&gt;Main function period (&lt;code&gt;&lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/SecOC/ELEMENTS/SecOC/SecOCGeneral/SecOCMainFunctionPeriodTx&quot;&gt;SecOCMainFunctionPeriodTx&lt;/a&gt;&lt;/code&gt;): when the time out value is different than 0, then it must be equal to or greater than Tx main function period.&lt;/li&gt;
                            &lt;/ul&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" 
                         value="Timeout period for SPduTxConfirmation"/>
                    <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                    <a:da name="DEFAULT" value="0"/>
                    <a:da name="INVALID" type="Multi">
                      <mt:range>
                        <mt:tst expr="&gt;=0"/>
                        <mt:tst expr="&lt;=4294967295"/>
                      </mt:range>
                      <mt:xpath 
                                true="The SecOCTxConfirmationTimeout value must be equal or greater than the Tx main function period value." 
                                expr="(. != 0) and (. &lt; node:value(../../../SecOCGeneral/SecOCMainFunctionPeriodTx))"/>
                    </a:da>
                  </v:var>
                  <v:var name="SecOCTxUseShapeFunc" type="BOOLEAN">
                    <a:a name="COLUMN_TITLE" 
                         value="Enable Tx Shape Function Usage"/>
                    <a:a name="COLUMN_TOOLTIP" value="SecOCTxUseShapeFunc"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              This parameter indicates, whether the layout shaping functionality
                              its enabled or not for this PDU.
                            &lt;/p&gt;
                            &lt;p&gt;
                              By enabling this parameter, the secured PDU layout can be updated
                              by the SecOC callout function which name is configured in the
                              &lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/SecOC/ELEMENTS/SecOC/SecOCEbGeneral/SecOCTxShapeFuncName&quot;&gt;SecOCTxShapeFuncName&lt;/a&gt; parameter.
                            &lt;/p&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Enable Tx Shape Function Usage"/>
                    <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                    <a:da name="DEFAULT" value="false"/>
                    <a:da name="INVALID" type="XPath" 
                          expr="node:value(.) = &apos;true&apos; and node:exists(../../../SecOCEbGeneral/SecOCTxShapeFuncName) = false()" 
                          true="The callout function to shape the secured PDU layout is enabled but the name of the callout function is not configured."/>
                  </v:var>
                  <v:var name="SecOCTxSyncPduProcessing" type="BOOLEAN">
                    <a:a name="COLUMN_TITLE" 
                         value="Enable Synchronous PDU Processing"/>
                    <a:a name="COLUMN_TOOLTIP" value="SecOCTxSyncPduProcessing"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              This parameter indicates whether the PDU is processed synchronously,
                              i.e. the PDU is processed directly (within the PduR call) without waiting the main function.
                            &lt;/p&gt;
                            &lt;p&gt;
                              Note that manually calling the main function has no effect since the
                              PDU processing is done within the PduR calls of SecOC interface
                              (i.e. SecOC_Transmit).
                            &lt;/p&gt;
                            &lt;p&gt;
                              Synchronous PDU processing cannot be combined with asynchronous Csm Mode.
                            &lt;/p&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Enable Synchronous PDU Processing"/>
                    <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                    <a:da name="DEFAULT" value="false"/>
                    <a:da name="ENABLE" value="true"/>
                    <a:da name="INVALID" type="XPath" 
                          expr="node:value(.) = &apos;true&apos; and                                    ((node:exists(as:ref(as:ref(../SecOCTxAuthServiceConfigRef)/CsmJobPrimitiveRef)/CsmMacGenerate) and node:value(as:ref(as:ref(../SecOCTxAuthServiceConfigRef)/CsmJobPrimitiveRef)/CsmMacGenerate/CsmMacGenerateConfig/CsmMacGenerateProcessing) = &apos;CSM_ASYNCHRONOUS&apos;) or                                     (node:exists(as:ref(as:ref(../SecOCTxAuthServiceConfigRef)/CsmJobPrimitiveRef)/CsmSignatureGenerate) and node:value(as:ref(as:ref(../SecOCTxAuthServiceConfigRef)/CsmJobPrimitiveRef)/CsmSignatureGenerate/CsmSignatureGenerateConfig/CsmSignatureGenerateProcessing) = &apos;CSM_ASYNCHRONOUS&apos;))" 
                          true="Synchronous PDU processing cannot be combined with asynchronous Csm mode"/>
                    <a:da name="WARNING" type="XPath" 
                          expr="node:value(.) = &apos;true&apos; and node:value(../SecOCAuthenticationBuildAttempts) &gt; 0" 
                          true="It&apos;s highly recommended to set SecOCAuthenticationBuildAttempts to zero when using synchronous PDU processing,                          since otherwise calls are blocking for the duration of retries."/>
                  </v:var>
                  <v:chc name="SecOCTxSecuredPduLayer" type="IDENTIFIABLE">
                    <a:a name="COLUMN_TITLE" value="Secured PDU Layer"/>
                    <a:a name="COLUMN_TOOLTIP" value="SecOCTxSecuredPduLayer"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              This container specifies the PDU that is transmitted by the SecOC
                              module to the PduR after the Mac was generated.
                            &lt;/p&gt;
                            &lt;p&gt;
                              There are two available possibilities to send the data to the lower layer:
                            &lt;/p&gt;
                            &lt;ul&gt;
                              &lt;li&gt;
                                SecOCTxSecuredPdu - the whole content will be send within a PDU
                                (Secured PDU)
                              &lt;/li&gt;
                              &lt;li&gt;
                                SecOCTxSecuredPduCollection - the whole content will be send with two
                                PDUs, the authentic PDU (which contains the authentic data) and
                                the cryptographic PDU (which contains the cryptographic information
                                like MAC etc)
                              &lt;/li&gt;
                            &lt;/ul&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="FRAME" value="TITLE"/>
                    <a:a name="LABEL" value="Secured PDU Layer"/>
                    <v:ctr name="SecOCTxSecuredPdu" type="IDENTIFIABLE">
                      <a:a name="DESC">
                        <a:v>&lt;html&gt;
                            &lt;p&gt;
                              This container specifies the secured PDU that is transmitted by the SecOC
                              module to the PduR after the Mac was generated.
                            &lt;/p&gt;
                            &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                            &lt;ul&gt;
                              &lt;li&gt;The secured PDU (&lt;code&gt;&lt;a href=&quot;path:./SecOCTxSecuredLayerPduRef&quot;&gt;SecOCTxSecuredLayerPduRef&lt;/a&gt;&lt;/code&gt;) length must be equal with the combined length of
                              the secure PDU header (&lt;code&gt;&lt;a href=&quot;path:./SecOCAuthPduHeaderLength&quot;&gt;SecOCAuthPduHeaderLength&lt;/a&gt;&lt;/code&gt;),
                              the authentic PDU (&lt;code&gt;&lt;a href=&quot;path:../SecOCTxAuthenticPduLayer/SecOCTxAuthenticLayerPduRef&quot;&gt;SecOCTxAuthenticLayerPduRef&lt;/a&gt;&lt;/code&gt;),
                              the truncated freshness value (&lt;code&gt;&lt;a href=&quot;path:../SecOCFreshnessValueTxLength&quot;&gt;SecOCFreshnessValueTxLength&lt;/a&gt;&lt;/code&gt;) and
                              the truncated authenticator (&lt;code&gt;&lt;a href=&quot;path:../SecOCAuthInfoTxLength&quot;&gt;SecOCAuthInfoTxLength&lt;/a&gt;&lt;/code&gt;), applicable when the secured PDU header is being used&lt;/li&gt;
                              &lt;li&gt;OR&lt;/li&gt;
                              &lt;li&gt;The secured PDU (&lt;code&gt;&lt;a href=&quot;path:./SecOCTxSecuredLayerPduRef&quot;&gt;SecOCTxSecuredLayerPduRef&lt;/a&gt;&lt;/code&gt;) length must be equal with the combined length of
                              the authentic PDU (&lt;code&gt;&lt;a href=&quot;path:../SecOCTxAuthenticPduLayer/SecOCTxAuthenticLayerPduRef&quot;&gt;SecOCTxAuthenticLayerPduRef&lt;/a&gt;&lt;/code&gt;),
                              the truncated freshness value (&lt;code&gt;&lt;a href=&quot;path:../SecOCFreshnessValueTxLength&quot;&gt;SecOCFreshnessValueTxLength&lt;/a&gt;&lt;/code&gt;) and
                              the truncated authenticator (&lt;code&gt;&lt;a href=&quot;path:../SecOCAuthInfoTxLength&quot;&gt;SecOCAuthInfoTxLength&lt;/a&gt;&lt;/code&gt;), applicable when secured PDU header is not being used&lt;/li&gt;
                            &lt;/ul&gt;
                            &lt;p&gt;Note: Take into account that when calculating the combined length some configuration parameters hold the length in bytes and some in bits.&lt;/p&gt;
                          &lt;/html&gt;</a:v>
                      </a:a>
                      <a:a name="LABEL" value="Secured PDU"/>
                      <a:da name="INVALID" type="XPath">
                        <a:tst 
                               expr="node:exists(./SecOCAuthPduHeaderLength) and                                      ((node:value(./SecOCAuthPduHeaderLength) + node:value(node:ref(../SecOCTxAuthenticPduLayer/SecOCTxAuthenticLayerPduRef)/PduLength) + ceiling((node:value(../SecOCFreshnessValueTxLength) + node:value(../SecOCAuthInfoTxLength)) div 8)) !=                                       node:value(node:ref(./SecOCTxSecuredLayerPduRef)/PduLength))" 
                               true="Tx secured PDU length must be equal with the combined length of the secure PDU header, authentic PDU, truncated freshness value and MAC"/>
                        <a:tst 
                               expr="not(node:exists(./SecOCAuthPduHeaderLength)) and                                      ((node:value(node:ref(../SecOCTxAuthenticPduLayer/SecOCTxAuthenticLayerPduRef)/PduLength) + ceiling((node:value(../SecOCFreshnessValueTxLength) + node:value(../SecOCAuthInfoTxLength)) div 8)) !=                                      node:value(node:ref(./SecOCTxSecuredLayerPduRef)/PduLength))" 
                               true="Tx secured PDU length must be equal with the combined length of the authentic PDU, truncated freshness value and MAC"/>
                      </a:da>
                      <v:var name="SecOCAuthPduHeaderLength" type="INTEGER">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                                &lt;p&gt;
                                  This parameter indicates the length (in bytes) of the secured PDU
                                  header in the secured PDU. The length of zero means there&apos;s no header
                                  in the PDU.
                                &lt;/p&gt;
                              &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="PreCompile">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="LABEL" value="Secured PDU Header Length"/>
                        <a:a name="OPTIONAL" value="true"/>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:da name="DEFAULT" value="0"/>
                        <a:da name="ENABLE" value="false"/>
                        <a:da name="INVALID" type="Multi">
                          <mt:range>
                            <mt:tst expr="&lt;=4"/>
                            <mt:tst expr="&gt;=0"/>
                          </mt:range>
                          <mt:xpath 
                                    false="The length of secured PDU Header must be smaller than Tx authentic PDU." 
                                    expr="node:value(node:ref(../../SecOCTxAuthenticPduLayer/SecOCTxAuthenticLayerPduRef)/PduLength) &gt; node:value(.)"/>
                        </a:da>
                      </v:var>
                      <v:var name="SecOCTxSecuredLayerPduId" type="INTEGER">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                                &lt;p&gt;
                                  PDU identifier assigned by SecureOnboardCommunication module.
                                  Used by PduR for confirmation (SecOC_PduRTxConfirmation) and for
                                  TriggerTransmit.
                                &lt;/p&gt;

                                &lt;p&gt;Note: The Handle-Id Wizard can be used to set this value automatically.&lt;/p&gt;
                              &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="LABEL" value="Secured PDU ID"/>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="true"/>
                        <a:da name="INVALID" type="Range">
                          <a:tst expr="&gt;=0"/>
                          <a:tst expr="&lt;=65535"/>
                        </a:da>
                      </v:var>
                      <v:ref name="SecOCTxSecuredLayerPduRef" type="REFERENCE">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                                &lt;p&gt;
                                  Reference to the global PDU, which holds the secured PDU.
                                &lt;/p&gt;
                              &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="LABEL" value="Secured PDU Reference"/>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                        <a:da name="INVALID" type="XPath" 
                              expr="(node:refvalid(.))" 
                              false="The expression argument reference is not valid."/>
                        <a:da name="REF" 
                              value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/EcuC/EcucPduCollection/Pdu"/>
                      </v:ref>
                    </v:ctr>
                    <v:ctr name="SecOCTxSecuredPduCollection" 
                           type="IDENTIFIABLE">
                      <a:a name="DESC">
                        <a:v>&lt;html&gt;
                              &lt;p&gt;
                                This container specifies the PDU that is transmitted by the SecOC module
                                to the PduR after the Mac was generated. Two separate PDUs are
                                transmitted to the PduR:
                                &lt;a href=&quot;path:./SecOCTxAuthenticPdu&quot;&gt;authentic PDU&lt;/a&gt; and
                                &lt;a href=&quot;path:./SecOCTxCryptographicPdu&quot;&gt;cryptographic PDU&lt;/a&gt;.
                              &lt;/p&gt;
                            &lt;/html&gt;</a:v>
                      </a:a>
                      <a:a name="LABEL" value="Secured PDU Collection"/>
                      <v:ctr name="SecOCTxAuthenticPdu" type="IDENTIFIABLE">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                                &lt;p&gt;
                                  This container specifies the PDU that is send by the SecOC
                                  module to the lower layer, which contains the authentic data
                                  that is forming with the corresponding cryptographic PDU
                                  the secured PDU.
                                &lt;/p&gt;
                                &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                                &lt;ul&gt;
                                  &lt;li&gt;The length of the authentic PDU (&lt;code&gt;&lt;a href=&quot;path:./SecOCTxAuthenticPduRef&quot;&gt;SecOCTxAuthenticPduRef&lt;/a&gt;&lt;/code&gt;)
                                  to be transmitted to the lower layer must have the same length with the authentic PDU (&lt;code&gt;&lt;a href=&quot;path:../../SecOCRxAuthenticPduLayer/SecOCRxAuthenticLayerPduRef&quot;&gt;SecOCRxAuthenticLayerPduRef&lt;/a&gt;&lt;/code&gt;)
                                  received from the upper layer plus
                                  the secured header length (&lt;code&gt;&lt;a href=&quot;path:./SecOCAuthPduHeaderLength&quot;&gt;SecOCAuthPduHeaderLength&lt;/a&gt;&lt;/code&gt;),
                                  applicable when secured PDU header is being used&lt;/li&gt;
                                  &lt;li&gt;OR&lt;/li&gt;
                                  &lt;li&gt;The length of the authentic PDU (&lt;code&gt;&lt;a href=&quot;path:./SecOCTxAuthenticPduRef&quot;&gt;SecOCTxAuthenticPduRef&lt;/a&gt;&lt;/code&gt;)
                                  to be transmitted to the lower layer must have the same length with the authentic PDU (&lt;code&gt;&lt;a href=&quot;path:../../SecOCRxAuthenticPduLayer/SecOCRxAuthenticLayerPduRef&quot;&gt;SecOCRxAuthenticLayerPduRef&lt;/a&gt;&lt;/code&gt;)
                                  received from the upper layer, applicable when secured PDU header is not being used&lt;/li&gt;
                                &lt;/ul&gt;
                              &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="LABEL" value="Authentic PDU"/>
                        <a:da name="INVALID" type="XPath">
                          <a:tst 
                                 expr="node:exists(./SecOCAuthPduHeaderLength) and                                        (node:value(./SecOCAuthPduHeaderLength) +                                         node:value(node:ref(../../SecOCTxAuthenticPduLayer/SecOCTxAuthenticLayerPduRef)/PduLength) !=                                         node:value(node:ref(./SecOCTxAuthenticPduRef)/PduLength))" 
                                 true="The length of the authentic PDU to be transmitted to the lower layer must have the same length with the authentic PDU received from the upper layer plus the secured header length"/>
                          <a:tst 
                                 expr="not(node:exists(./SecOCAuthPduHeaderLength)) and                                        (node:value(node:ref(../../SecOCTxAuthenticPduLayer/SecOCTxAuthenticLayerPduRef)/PduLength) !=                                         node:value(node:ref(./SecOCTxAuthenticPduRef)/PduLength))" 
                                 true="The length of the authentic PDU to be transmitted to the lower layer must have the same length with the authentic PDU received from the upper layer"/>
                        </a:da>
                        <v:var name="SecOCAuthPduHeaderLength" type="INTEGER">
                          <a:a name="DESC">
                            <a:v>&lt;html&gt;
                                  &lt;p&gt;
                                    This parameter indicates the length (in bytes) of the secured PDU
                                    header in the authentic PDU. The length of zero means there&apos;s no header
                                    in the PDU.
                                  &lt;/p&gt;
                                &lt;/html&gt;</a:v>
                          </a:a>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v class="PreCompile">VariantPostBuild</icc:v>
                          </a:a>
                          <a:a name="LABEL" value="Secured PDU Header Length"/>
                          <a:a name="OPTIONAL" value="true"/>
                          <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                          <a:da name="DEFAULT" value="0"/>
                          <a:da name="ENABLE" value="false"/>
                          <a:da name="INVALID" type="Multi">
                            <mt:range>
                              <mt:tst expr="&lt;=4"/>
                              <mt:tst expr="&gt;=0"/>
                            </mt:range>
                            <mt:xpath 
                                      false="The length of secured PDU header must be smaller than Tx authentic PDU." 
                                      expr="node:value(node:ref(../SecOCTxAuthenticPduRef)/PduLength) &gt; node:value(.)"/>
                          </a:da>
                        </v:var>
                        <v:var name="SecOCTxAuthenticPduId" type="INTEGER">
                          <a:a name="DESC">
                            <a:v>&lt;html&gt;
                                  &lt;p&gt;
                                    This parameter defines the PDU identifier of the authentic
                                    PDU assigned by SecOC module.
                                    Used by PduR for confirmation (&lt;code&gt;SecOC_PduRTxConfirmation&lt;/code&gt;)
                                    and for TriggerTransmit.
                                  &lt;/p&gt;
                                  &lt;p&gt;Note: The Handle-Id Wizard can be used to set this value automatically.&lt;/p&gt;
                                &lt;/html&gt;</a:v>
                          </a:a>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                          </a:a>
                          <a:a name="LABEL" value="Authentic PDU ID"/>
                          <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                          <a:da name="EDITABLE" value="true"/>
                          <a:da name="INVALID" type="Range">
                            <a:tst expr="&gt;=0"/>
                            <a:tst expr="&lt;=65535"/>
                          </a:da>
                        </v:var>
                        <v:ref name="SecOCTxAuthenticPduRef" type="REFERENCE">
                          <a:a name="DESC">
                            <a:v>&lt;html&gt;
                                  &lt;p&gt;
                                    Reference to the global PDU.
                                  &lt;/p&gt;
                                &lt;/html&gt;</a:v>
                          </a:a>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                          </a:a>
                          <a:a name="LABEL" value="Authentic PDU Reference"/>
                          <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                          <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                          <a:da name="EDITABLE" value="true"/>
                          <a:da name="REF" 
                                value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/EcuC/EcucPduCollection/Pdu"/>
                        </v:ref>
                      </v:ctr>
                      <v:ctr name="SecOCTxCryptographicPdu" type="IDENTIFIABLE">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                                &lt;p&gt;
                                  This container specifies the cryptographic PDU that is transmitted by the
                                  SecOC module to the PduR after the Mac was generated.
                                &lt;/p&gt;
                                &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                                &lt;ul&gt;
                                  &lt;li&gt;The length of the cryptographic PDU (&lt;code&gt;&lt;a href=&quot;path:./SecOCTxCryptographicPduRef&quot;&gt;SecOCTxCryptographicPduRef&lt;/a&gt;&lt;/code&gt;)
                                  must be equal with the combined length of the truncated freshness value
                                  (&lt;code&gt;&lt;a href=&quot;path:../../SecOCFreshnessValueTxLength&quot;&gt;SecOCFreshnessValueTxLength&lt;/a&gt;&lt;/code&gt;),
                                  the truncated authenticator (&lt;code&gt;&lt;a href=&quot;path:../../SecOCAuthInfoTxLength&quot;&gt;SecOCAuthInfoTxLength&lt;/a&gt;&lt;/code&gt;),
                                  and the message link (&lt;code&gt;&lt;a href=&quot;path:../SecOCUseMessageLink/SecOCMessageLinkLen&quot;&gt;SecOCMessageLinkLen&lt;/a&gt;&lt;/code&gt;),
                                  applicable when message link is being used&lt;/li&gt;
                                  &lt;li&gt;OR&lt;/li&gt;
                                  &lt;li&gt;The length of the cryptographic PDU (&lt;code&gt;&lt;a href=&quot;path:./SecOCTxCryptographicPduRef&quot;&gt;SecOCTxCryptographicPduRef&lt;/a&gt;&lt;/code&gt;)
                                  must be equal with the combined length of the truncated freshness value
                                  (&lt;code&gt;&lt;a href=&quot;path:../../SecOCFreshnessValueTxLength&quot;&gt;SecOCFreshnessValueTxLength&lt;/a&gt;&lt;/code&gt;)
                                  and the truncated authenticator (&lt;code&gt;&lt;a href=&quot;path:../../SecOCAuthInfoTxLength&quot;&gt;SecOCAuthInfoTxLength&lt;/a&gt;&lt;/code&gt;),
                                  applicable when message link is not being used&lt;/li&gt;
                                &lt;/ul&gt;
                                &lt;p&gt;Note: Take into account that when calculating the combined length some configuration parameters hold the length in bytes and some in bits.&lt;/p&gt;
                              &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="LABEL" value="Cryptographic PDU"/>
                        <a:da name="INVALID" type="XPath">
                          <a:tst 
                                 expr="node:exists(../SecOCUseMessageLink) and                                       (as:ref(./SecOCTxCryptographicPduRef)/PduLength != (ceiling((../../SecOCAuthInfoTxLength + ../../SecOCFreshnessValueTxLength + ../SecOCUseMessageLink/SecOCMessageLinkLen) div 8)))" 
                                 true="The length of the cryptographic PDU must be equal                                        with the combined length of the truncated                                        freshness value, the truncated authenticator and                                        the message link."/>
                          <a:tst 
                                 expr="not(node:exists(../SecOCUseMessageLink)) and                                       (as:ref(./SecOCTxCryptographicPduRef)/PduLength != (ceiling((../../SecOCAuthInfoTxLength + ../../SecOCFreshnessValueTxLength) div 8)))" 
                                 true="The length of the cryptographic PDU must be equal                                        with the combined length of the truncated                                        freshness value and the truncated authenticator."/>
                        </a:da>
                        <v:var name="SecOCTxCryptographicPduId" type="INTEGER">
                          <a:a name="DESC">
                            <a:v>&lt;html&gt;
                                  &lt;p&gt;
                                    This parameter defines the PDU identifier of the cryptographic
                                    PDU assigned by SecOC module.
                                    Used by PduR for confirmation (SecOC_PduRTxConfirmation) and
                                    for TriggerTransmit.
                                  &lt;/p&gt;
                                  &lt;p&gt;Note: The Handle-Id Wizard can be used to set this value automatically.&lt;/p&gt;
                                &lt;/html&gt;</a:v>
                          </a:a>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                          </a:a>
                          <a:a name="LABEL" value="Cryptographic PDU ID"/>
                          <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                          <a:da name="EDITABLE" value="true"/>
                          <a:da name="INVALID" type="Range">
                            <a:tst expr="&gt;=0"/>
                            <a:tst expr="&lt;=65535"/>
                          </a:da>
                        </v:var>
                        <v:ref name="SecOCTxCryptographicPduRef" 
                               type="REFERENCE">
                          <a:a name="DESC">
                            <a:v>&lt;html&gt;
                                  &lt;p&gt;
                                    Reference to the global Pdu.
                                  &lt;/p&gt;
                                &lt;/html&gt;</a:v>
                          </a:a>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                          </a:a>
                          <a:a name="LABEL" value="Cryptographic PDU Reference"/>
                          <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                          <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                          <a:da name="EDITABLE" value="true"/>
                          <a:da name="REF" 
                                value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/EcuC/EcucPduCollection/Pdu"/>
                        </v:ref>
                      </v:ctr>
                      <v:ctr name="SecOCUseMessageLink" type="IDENTIFIABLE">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                                &lt;p&gt;
                                  SecOC links an authentic PDU and cryptographic PDU together by
                                  repeating a specific part (message link) of the authentic PDU
                                  in the cryptographic PDU.
                                &lt;/p&gt;
                              &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="LABEL" value="Message Link"/>
                        <a:a name="OPTIONAL" value="true"/>
                        <a:da name="ENABLE" value="false"/>
                        <a:da name="INVALID" type="XPath" 
                              expr="(num:i(ceiling((node:value(./SecOCMessageLinkLen) + node:value(./SecOCMessageLinkPos)) div 8)) &gt; node:value(node:ref(../../SecOCTxAuthenticPduLayer/SecOCTxAuthenticLayerPduRef)/PduLength))" 
                              true="The sum of configured value of message link length and message link offset (converted in bytes) must be smaller than or equal to the authentic PDU length"/>
                        <v:var name="SecOCMessageLinkLen" type="INTEGER">
                          <a:a name="DESC">
                            <a:v>&lt;html&gt;
                                  &lt;p&gt;
                                    This parameter defines the length of the message link inside
                                    the authentic PDU in bits.
                                  &lt;/p&gt;
                                &lt;/html&gt;</a:v>
                          </a:a>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                          </a:a>
                          <a:a name="LABEL" value="Message Link length"/>
                          <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                          <a:da name="EDITABLE" value="true"/>
                          <a:da name="INVALID" type="Range">
                            <a:tst expr="&gt;=1"/>
                            <a:tst expr="&lt;=65535"/>
                          </a:da>
                        </v:var>
                        <v:var name="SecOCMessageLinkPos" type="INTEGER">
                          <a:a name="DESC">
                            <a:v>&lt;html&gt;
                                  &lt;p&gt;
                                    This parameter defines the position of the message link
                                    inside the authentic PDU in bits.
                                  &lt;/p&gt;
                                &lt;/html&gt;</a:v>
                          </a:a>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                          </a:a>
                          <a:a name="LABEL" value="Message Link Position"/>
                          <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                          <a:da name="EDITABLE" value="true"/>
                          <a:da name="INVALID" type="Range">
                            <a:tst expr="&gt;=0"/>
                            <a:tst expr="&lt;=65535"/>
                          </a:da>
                        </v:var>
                      </v:ctr>
                    </v:ctr>
                  </v:chc>
                  <v:ctr name="SecOCTxPduSecuredArea" type="IDENTIFIABLE">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                              &lt;p&gt;
                                This container specifies an area in the authentic PDU that will
                                be the input to the authenticator generation algorithm. If this
                                container does not exist in the configuration the complete
                                authentic PDU will be the input to the authenticator
                                generation algorithm.
                              &lt;/p&gt;
                              &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                              &lt;ul&gt;
                                &lt;li&gt;Enable Secured Area Usage (&lt;code&gt;&lt;a href=&quot;path:/AUTOSAR/TOP-LEVEL-PACKAGES/SecOC/ELEMENTS/SecOC/SecOCEbGeneral/SecOCUseSecuredArea&quot;&gt;SecOCUseSecuredArea&lt;/a&gt;&lt;/code&gt;): must be enabled in order to be able to use this feature&lt;/li&gt;
                                &lt;li&gt;Secured Area Length (&lt;code&gt;&lt;a href=&quot;path:./SecOCSecuredTxPduLength&quot;&gt;SecOCSecuredTxPduLength&lt;/a&gt;&lt;/code&gt;) and Secured Area Offset (&lt;code&gt;&lt;a href=&quot;path:./SecOCSecuredTxPduOffset&quot;&gt;SecOCSecuredTxPduOffset&lt;/a&gt;&lt;/code&gt;): the sum of the secured area length and the secured area offset must be smaller than or equal to the authentic PDU length&lt;/li&gt;
                              &lt;/ul&gt;
                            &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="LABEL" value="Secured Area"/>
                    <a:a name="OPTIONAL" value="true"/>
                    <a:a name="POSTBUILDVARIANTMULTIPLICITY" value="true"/>
                    <a:da name="ENABLE" value="false"/>
                    <a:da name="INVALID" type="XPath">
                      <a:tst 
                             expr="(node:value(./SecOCSecuredTxPduLength) + node:value(./SecOCSecuredTxPduOffset) &gt; node:value(node:ref(../SecOCTxAuthenticPduLayer/SecOCTxAuthenticLayerPduRef)/PduLength))" 
                             true="The sum of configured value of secured area length and secured area offset must be smaller than or equal to the authentic PDU length"/>
                      <a:tst 
                             expr="node:value(../../../SecOCEbGeneral/SecOCUseSecuredArea) = &apos;false&apos;" 
                             true="In the General tab the SecOCUseSecuredArea configuration parameter must be enabled"/>
                    </a:da>
                    <v:var name="SecOCSecuredTxPduLength" type="INTEGER">
                      <a:a name="COLUMN_TITLE" value="Secured Area Length"/>
                      <a:a name="COLUMN_TOOLTIP" value="SecOCSecuredTxPduLength"/>
                      <a:a name="DESC">
                        <a:v>&lt;html&gt;
                                &lt;p&gt;
                                  This parameter defines the length (in bytes) of the area within
                                  the PDU which shall be secured.
                                &lt;/p&gt;
                              &lt;/html&gt;</a:v>
                      </a:a>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                           type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                      </a:a>
                      <a:a name="LABEL" value="Secured Area Length"/>
                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                      <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                      <a:da name="INVALID" type="Range">
                        <a:tst expr="&gt;=0"/>
                        <a:tst expr="&lt;=65535"/>
                      </a:da>
                    </v:var>
                    <v:var name="SecOCSecuredTxPduOffset" type="INTEGER">
                      <a:a name="COLUMN_TITLE" value="Secured Area Offset"/>
                      <a:a name="COLUMN_TOOLTIP" value="SecOCSecuredTxPduOffset"/>
                      <a:a name="DESC">
                        <a:v>&lt;html&gt;
                                &lt;p&gt;
                                  This parameter defines the start position (offset in bytes) of the
                                  area within the PDU which shall be secured.
                                &lt;/p&gt;
                              &lt;/html&gt;</a:v>
                      </a:a>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                           type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                      </a:a>
                      <a:a name="LABEL" value="Secured Area Offset"/>
                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                      <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                      <a:da name="INVALID" type="Range">
                        <a:tst expr="&gt;=0"/>
                        <a:tst expr="&lt;=65535"/>
                      </a:da>
                    </v:var>
                  </v:ctr>
                  <v:ctr name="SecOCTxAuthenticPduLayer" type="IDENTIFIABLE">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              This container specifies the authetic PDU that is received by the SecOC
                              module from the PduR based on this the secured PDU is generated.
                            &lt;/p&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="LABEL" value="Authentic PDU Layer"/>
                    <v:var name="SecOCPduType" type="ENUMERATION">
                      <a:a name="COLUMN_TITLE" value="PduR API Type"/>
                      <a:a name="COLUMN_TOOLTIP" value="SecOCPduType"/>
                      <a:a name="DESC">
                        <a:v>&lt;html&gt;
                              &lt;p&gt;
                                This parameter defines API Type to use for communication with PduR.
                              &lt;/p&gt;

                              &lt;p&gt;Range:&lt;/p&gt;
                              &lt;ul&gt;
                                &lt;li&gt;&lt;code&gt;SECOC_IFPDU&lt;/code&gt;: Interface communication API&lt;/li&gt;
                                &lt;li&gt;&lt;code&gt;SECOC_TPPDU&lt;/code&gt;: Transport Protocol communication API&lt;/li&gt;
                              &lt;/ul&gt;
                            &lt;/html&gt;</a:v>
                      </a:a>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                           type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                      </a:a>
                      <a:a name="LABEL" value="PduR API Type"/>
                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                      <a:da name="DEFAULT" value="SECOC_IFPDU"/>
                      <a:da name="RANGE">
                        <a:v>SECOC_IFPDU</a:v>
                        <a:v>SECOC_TPPDU</a:v>
                      </a:da>
                    </v:var>
                    <v:var name="SecOCTxAuthenticLayerPduId" type="INTEGER">
                      <a:a name="COLUMN_TITLE" value="Authentic PDU ID"/>
                      <a:a name="COLUMN_TOOLTIP" 
                           value="SecOCTxAuthenticLayerPduId"/>
                      <a:a name="DESC">
                        <a:v>&lt;html&gt;
                              &lt;p&gt;
                                PDU identifier assigned by SecureOnboardCommunication module.
                                Used by PduR for SecOC_PduRTransmit.
                              &lt;/p&gt;

                              &lt;p&gt;Note: The Handle-Id Wizard can be used to set this value automatically.&lt;/p&gt;
                            &lt;/html&gt;</a:v>
                      </a:a>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                           type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                      </a:a>
                      <a:a name="LABEL" value="Authentic PDU ID"/>
                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                      <a:a name="SYMBOLICNAMEVALUE" value="true"/>
                      <a:da name="INVALID" type="Multi">
                        <mt:range>
                          <mt:tst expr="&gt;=0"/>
                          <mt:tst expr="&lt;=65535"/>
                        </mt:range>
                        <mt:xpath>
                          <mt:tst 
                                  false="The authentic Tx PDU ID has to be unique." 
                                  expr="text:uniq(../../../*/SecOCTxAuthenticPduLayer/SecOCTxAuthenticLayerPduId,.)"/>
                          <mt:tst 
                                  true="The authentic Tx PDU ID has to be zero-based and consecutive." 
                                  expr=". &gt;= count(../../../*)"/>
                        </mt:xpath>
                      </a:da>
                    </v:var>
                    <v:ref name="SecOCTxAuthenticLayerPduRef" type="REFERENCE">
                      <a:a name="COLUMN_TITLE" value="Authentic PDU Reference"/>
                      <a:a name="COLUMN_TOOLTIP" 
                           value="SecOCTxAuthenticLayerPduRef"/>
                      <a:a name="DESC">
                        <a:v>&lt;html&gt;
                              &lt;p&gt;
                                Reference to the global PDU holding the authentic PDU, for which the
                                SecOC module shall generate an authenticator.
                              &lt;/p&gt;
                            &lt;/html&gt;</a:v>
                      </a:a>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                           type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                      </a:a>
                      <a:a name="LABEL" value="Authentic PDU Reference"/>
                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                      <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                      <a:da name="INVALID" type="XPath">
                        <a:tst expr="(node:refvalid(.))" 
                               false="The expression argument reference is not valid."/>
                        <a:tst 
                               expr="node:exists((as:modconf(&apos;PduR&apos;)[1]/PduRRoutingTables/*/PduRRoutingTable/*/PduRRoutingPath/*/PduRDestPdu/*[PduRDestPduRef = node:current()])[1]/PduRDestPduHandleId)" 
                               false="No PDU ID found in upper layer PduR that is assigned to this PDU (PduRDestPduHandleId)."/>
                      </a:da>
                      <a:da name="REF" 
                            value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/EcuC/EcucPduCollection/Pdu"/>
                    </v:ref>
                  </v:ctr>
                </v:ctr>
              </v:lst>
              <v:lst name="SecOCMainFunctionRx" type="MAP">
                <a:a name="LABEL" value="Rx Mainfunctions"/>
                <a:da name="MIN" value="0"/>
                <v:ctr name="SecOCMainFunctionRx" type="IDENTIFIABLE">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          &lt;p&gt;
                            Each element of this container defines one instance of SecOC_MainFunctionRx.
                          &lt;/p&gt;
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v mclass="PreCompile">VariantPreCompile</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="SecOCMainFunctionRx"/>
                  <a:a name="OPTIONAL" value="false"/>
                  <a:da name="ENABLE" value="true"/>
                  <a:da name="INVALID" type="XPath" 
                        expr="num:i(count(../../SecOCRxPduProcessing/*[node:name(as:ref(./SecOCRxPduMainFunctionRef)) = node:name(node:current())])) = 0" 
                        true="At least one PDU must be assigned to each main function."/>
                  <v:ref name="SecOCMainFunctionRxPartitionRef" 
                         type="REFERENCE">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                              Reference to EcucPartition, where the according SecOC_MainFunction instance is assigned to.
                            &lt;/p&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
