<!-- handle id calculator for J1939TpTxNSduId -->
  <extension
    point="dreisoft.tresos.guidedconfig.api.plugin.pushservice">
    <pushoperation id="J1939TpTxNSduIdJ1939Tp_TS_TxDxM1I2R0"
    name="J1939Tp/J1939TpTxNSduId"
    desc="">
        <operationclass class="dreisoft.tresos.guidedconfig.api.pushservice.operations.handleid.SimpleHandleIdPushOperation">
            <parameter name="moduleId" value="J1939Tp_TS_TxDxM1I2R0"/>
            <parameter name="listPath" value="/AUTOSAR/TOP-LEVEL-PACKAGES/TS_TxDxM1I2R0/ELEMENTS/J1939Tp/J1939TpConfiguration/J1939TpConfiguration/J1939TpTxChannel/J1939TpTxChannel/J1939TpTxPg/J1939TpTxPg/J1939TpTxNSdu/J1939TpTxNSdu"/>
            <parameter name="idPath" value="J1939TpTxNSduId"/>
            <parameter name="zbcGroups" value="false"/>
        </operationclass>
        <event>
            <and>
                <with variable="class">
                    <equals value="dreisoft.tresos.guidedconfig.api.pushservice.operations.handleid.HandleIdEvent"/>
                </with>
                <with variable="moduleId.J1939Tp_TS_TxDxM1I2R0">
                    <equals value="true"/>
                </with>
                <with variable="relVersion">
                    <equals value="4.2"/>
                </with>
            </and>
        </event>
    </pushoperation>
  </extension>
  
