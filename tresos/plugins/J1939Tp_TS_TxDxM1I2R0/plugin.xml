    <extension point="dreisoft.tresos.launcher2.plugin.generator"
              id="J1939Tp_TS_TxDxM1I2R0_Gen_Template">
    <generator moduleId="J1939Tp_TS_TxDxM1I2R0"
               id="J1939Tp_TS_TxDxM1I2R0_Gen_TemplateId"
               modes="generate,Template_withPostRename,verify"
               class="dreisoft.tresos.launcher2.generator.TemplateBasedCodeGenerator">
      <parameter name="mode_type" mode="Template_withPostRename" value="generate"/>

      <!-- common template path parameters -->
