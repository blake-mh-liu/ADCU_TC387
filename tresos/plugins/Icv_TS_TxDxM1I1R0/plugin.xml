      <data>
        <manager class="dreisoft.tresos.autosar2.resourcehandling.AutosarConfigManager"/>
        <schemaNode path="ASPath:/TS_TxDxM1I1R0/Icv"/>
      </data>

      <editor id="Icv_TS_TxDxM1I1R0_EditorId"
