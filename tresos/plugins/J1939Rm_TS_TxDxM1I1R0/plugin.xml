<!-- handle id calculator for J1939RmRxPduId  -->
  <extension
    point="dreisoft.tresos.guidedconfig.api.plugin.pushservice">
    <pushoperation id="J1939RmRxPduIdJ1939Rm_TS_TxDxM1I1R0"
    name="J1939Rm/J1939RmRxPduId"
    desc="">
    <!-- handle id calculator for J1939RmAckmRxPduId  -->
        <operationclass class="dreisoft.tresos.guidedconfig.api.pushservice.operations.handleid.SimpleHandleIdPushOperation">
            <parameter name="moduleId" value="J1939Rm_TS_TxDxM1I1R0"/>
            <parameter name="listPath" value="/AUTOSAR/TOP-LEVEL-PACKAGES/TS_TxDxM1I1R0/ELEMENTS/J1939Rm/J1939RmConfigSet/J1939RmConfigSet/J1939RmChannel/J1939RmChannel/J1939RmAckmRxPdu"/>
            <parameter name="idPath" value="J1939RmAckmRxPduId"/>
            <parameter name="zbcGroups" value="false"/>
    <!-- handle id calculator for J1939RmRqstRxPduId  -->
            <parameter name="listPath_2" value="/AUTOSAR/TOP-LEVEL-PACKAGES/TS_TxDxM1I1R0/ELEMENTS/J1939Rm/J1939RmConfigSet/J1939RmConfigSet/J1939RmChannel/J1939RmChannel/J1939RmRqstRxPdu"/>
            <parameter name="idPath_2" value="J1939RmRqstRxPduId"/>
            <parameter name="zbcGroups_2" value="false"/>
        </operationclass>
        <event>
            <and>
                <with variable="class">
                    <equals value="dreisoft.tresos.guidedconfig.api.pushservice.operations.handleid.HandleIdEvent"/>
                </with>
                <with variable="moduleId.J1939Rm_TS_TxDxM1I1R0">
                    <equals value="true"/>
                </with>
                <with variable="relVersion">
                    <equals value="4.1"/>
                </with>
            </and>
        </event>
    </pushoperation>
  </extension>

